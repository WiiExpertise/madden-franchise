            <attribute name="SignCoach" idx="43" type="TeamManager_SignCoach" guid="5307-38175" final="true" default="// Assign the coach contract data.&#xD;&#xA;Coach coach = contract.StaffPerson;&#xD;&#xA;&#xD;&#xA;coach.ContractYearsRemaining = contract.Length;&#xD;&#xA;coach.ExperiencePoints = coach.ExperiencePoints + contract.ExperiencePoints;&#xD;&#xA;coach.ContractLength = contract.Length;&#xD;&#xA;coach.ContractSalary = contract.Salary;&#xD;&#xA;&#xD;&#xA;bool isResign = false;&#xD;&#xA;if(contract.ContractPosition == CoachPosition.HeadCoach)&#xD;&#xA;{&#xD;&#xA;    isResign = (team.HeadCoach == coach);&#xD;&#xA;&#xD;&#xA;    // Add coach to the team.&#xD;&#xA;    team.HeadCoach = coach;&#xD;&#xA;}&#xD;&#xA;else if(contract.ContractPosition == CoachPosition.OffensiveCoordinator)&#xD;&#xA;{&#xD;&#xA;    isResign = (team.OffensiveCoordinator == coach);&#xD;&#xA;&#xD;&#xA;    // Add coach to the team.&#xD;&#xA;    team.OffensiveCoordinator = coach;&#xD;&#xA;}&#xD;&#xA;else if(contract.ContractPosition == CoachPosition.DefensiveCoordinator)&#xD;&#xA;{&#xD;&#xA;    isResign = (team.DefensiveCoordinator == coach);&#xD;&#xA;&#xD;&#xA;    // Add coach to the team.&#xD;&#xA;    team.DefensiveCoordinator = coach;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;Team oldTeam = null;&#xD;&#xA;CoachPosition oldPosition = coach.Position;&#xD;&#xA;&#xD;&#xA;// Find the coach in the free agent pool.&#xD;&#xA;int coachIndex = Franchise.FreeAgentCoaches.Find(coach);&#xD;&#xA;&#xD;&#xA;// If the coach is in the free agent pool, remove him.&#xD;&#xA;if ((0 &lt;= coachIndex) &amp;&amp; (coachIndex &lt; Franchise.FreeAgentCoaches.Population()))&#xD;&#xA;{&#xD;&#xA;    Franchise.FreeAgentCoaches.Erase(coachIndex);&#xD;&#xA;}&#xD;&#xA;else if (team.TeamIndex != coach.TeamIndex)&#xD;&#xA;{&#xD;&#xA;    oldTeam = FindTeamByTeamIndex(coach.TeamIndex);&#xD;&#xA;    if (oldTeam != null)&#xD;&#xA;    {&#xD;&#xA;        if(oldPosition == CoachPosition.HeadCoach)&#xD;&#xA;        {&#xD;&#xA;            // Remove coach from the old team.&#xD;&#xA;            oldTeam.HeadCoach = null;&#xD;&#xA;        }&#xD;&#xA;        else if(oldPosition == CoachPosition.OffensiveCoordinator)&#xD;&#xA;        {&#xD;&#xA;            // Remove OC from the old team.&#xD;&#xA;            oldTeam.OffensiveCoordinator = null;&#xD;&#xA;        }&#xD;&#xA;        else if(oldPosition == CoachPosition.DefensiveCoordinator)&#xD;&#xA;        {&#xD;&#xA;            // Remove DC from the old team.&#xD;&#xA;            oldTeam.DefensiveCoordinator = null;&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;    // Otherwise, he is newly created. Set his previous status to &quot;signed&quot;.&#xD;&#xA;    // TODO: Why wouldn't the previous status be &quot;Free Agent&quot; for created coaches?&#xD;&#xA;    else&#xD;&#xA;    {&#xD;&#xA;        coach.ContractStatus = StaffPersonContractStatus.Signed;&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;else if (oldPosition != contract.ContractPosition)&#xD;&#xA;{&#xD;&#xA;    if(oldPosition == CoachPosition.HeadCoach)&#xD;&#xA;    {&#xD;&#xA;        // Remove coach from the old position.&#xD;&#xA;        oldTeam.HeadCoach = null;&#xD;&#xA;    }&#xD;&#xA;    else if(oldPosition == CoachPosition.OffensiveCoordinator)&#xD;&#xA;    {&#xD;&#xA;        // Remove OC from the old position.&#xD;&#xA;        oldTeam.OffensiveCoordinator = null;&#xD;&#xA;    }&#xD;&#xA;    else if(oldPosition == CoachPosition.DefensiveCoordinator)&#xD;&#xA;    {&#xD;&#xA;        // Remove DC from the old position.&#xD;&#xA;        oldTeam.DefensiveCoordinator = null;&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if (oldPosition != contract.ContractPosition)&#xD;&#xA;{&#xD;&#xA;    coach.Position = contract.ContractPosition;&#xD;&#xA;}&#xD;&#xA;// Set the contract to &quot;Accepted&quot;.&#xD;&#xA;contract.Status = ContractOfferStatus.Accepted;&#xD;&#xA;&#xD;&#xA;// Update the coach contract status.&#xD;&#xA;CoachManager.UpdateCoachContractStatus(coach, oldTeam, oldPosition, team, StaffPersonContractStatus.Signed);&#xD;&#xA;&#xD;&#xA;// Issue a notification.&#xD;&#xA;// TODO: Notifications do not belong at this level. A listener for the coach contract status update&#xD;&#xA;// should issue the notification. FranchiseRules modules should not know about them.&#xD;&#xA;StaffEventType eventType = StaffEventType.Hired;&#xD;&#xA;if (isResign)&#xD;&#xA;{&#xD;&#xA;    eventType = StaffEventType.ReSigned;&#xD;&#xA;}&#xD;&#xA;else if (IsValidationHire)&#xD;&#xA;{&#xD;&#xA;    eventType = StaffEventType.ValidationHired;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;IssueStaffEventNotificationRequest(contract, eventType, team.Owner);&#xD;&#xA;" />
            <attribute name="SignPlayer" idx="44" type="TeamManager_SignPlayer" guid="5307-28085" final="true" default="// Assign the player contract data.&#xD;&#xA;player.ContractLength = contract.Length;&#xD;&#xA;player.ContractYear = contract.CurrentYear;&#xD;&#xA;player.ContractExtraYearOption = contract.ExtraYearOption;&#xD;&#xA;&#xD;&#xA;// Assign the player salary.&#xD;&#xA;player.ContractSalary0 = contract.SalaryTable[0];&#xD;&#xA;player.ContractSalary1 = contract.SalaryTable[1];&#xD;&#xA;player.ContractSalary2 = contract.SalaryTable[2];&#xD;&#xA;player.ContractSalary3 = contract.SalaryTable[3];&#xD;&#xA;player.ContractSalary4 = contract.SalaryTable[4];&#xD;&#xA;player.ContractSalary5 = contract.SalaryTable[5];&#xD;&#xA;player.ContractSalary6 = contract.SalaryTable[6];&#xD;&#xA;&#xD;&#xA;// Assign the player bonus.&#xD;&#xA;player.ContractBonus0 = contract.BonusTable[0];&#xD;&#xA;player.ContractBonus1 = contract.BonusTable[1];&#xD;&#xA;player.ContractBonus2 = contract.BonusTable[2];&#xD;&#xA;player.ContractBonus3 = contract.BonusTable[3];&#xD;&#xA;player.ContractBonus4 = contract.BonusTable[4];&#xD;&#xA;player.ContractBonus5 = contract.BonusTable[5];&#xD;&#xA;player.ContractBonus6 = contract.BonusTable[6];&#xD;&#xA;&#xD;&#xA;// Assign the player a jersey number.&#xD;&#xA;player.JerseyNum = FindAvailableJerseyNumber(team, player);&#xD;&#xA;&#xD;&#xA;Team oldTeam = null;&#xD;&#xA;PlayerContractStatus oldStatus = player.ContractStatus;&#xD;&#xA;PlayerContractStatus newStatus = PlayerContractStatus.Signed;&#xD;&#xA;&#xD;&#xA;// If the player was a free agent, remove him from the free agent pool.&#xD;&#xA;if (oldStatus == PlayerContractStatus.FreeAgent)&#xD;&#xA;{&#xD;&#xA;    Franchise.FreeAgents.FindAndErase(player);&#xD;&#xA;}&#xD;&#xA;// If the player was on a practice squad, remove him from the free agent pool and&#xD;&#xA;// from his old team.&#xD;&#xA;else if (oldStatus == PlayerContractStatus.PracticeSquad)&#xD;&#xA;{&#xD;&#xA;    Franchise.FreeAgents.FindAndErase(player);&#xD;&#xA;    &#xD;&#xA;    oldTeam = FindTeamByTeamIndex(player.TeamIndex);&#xD;&#xA;    if (oldTeam != null)&#xD;&#xA;    {&#xD;&#xA;        RemovePlayer(oldTeam, player, false);&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;// If the player was drafted or previously signed, he is moving to the same team.&#xD;&#xA;else if ((oldStatus == PlayerContractStatus.Drafted) ||&#xD;&#xA;         (oldStatus == PlayerContractStatus.Expiring) ||&#xD;&#xA;         (oldStatus == PlayerContractStatus.Signed))&#xD;&#xA;{&#xD;&#xA;    oldTeam = team;&#xD;&#xA;}&#xD;&#xA;// If the player was just created, add him to the free agent pool.&#xD;&#xA;else if (oldStatus == PlayerContractStatus.Created)&#xD;&#xA;{&#xD;&#xA;    oldStatus = PlayerContractStatus.FreeAgent;&#xD;&#xA;    player.ContractStatus = PlayerContractStatus.FreeAgent;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Add the player to the team.&#xD;&#xA;AddPlayer(team, player, newStatus);&#xD;&#xA;&#xD;&#xA;// Update the player's contract status.&#xD;&#xA;PlayerManager.UpdatePlayerContractStatus(player, oldTeam, team, oldStatus, newStatus, false, false);&#xD;&#xA;" />
            <attribute name="SignPlayerToPracticeSquad" idx="45" type="TeamManager_SignPlayerToPracticeSquad" guid="5307-1342521" final="true" default="PlayerContractStatus oldStatus = player.ContractStatus;&#xD;&#xA;Team oldTeam = null;&#xD;&#xA;&#xD;&#xA;if (oldStatus == PlayerContractStatus.Signed)&#xD;&#xA;{&#xD;&#xA;    // Player is being moved from roster to practice squad&#xD;&#xA;    RemovePlayer(team, player, true);&#xD;&#xA;    Franchise.FreeAgents.Append(player);&#xD;&#xA;    oldTeam = team;&#xD;&#xA;    &#xD;&#xA;    // Clear the player's contract data now that any penalties have been applied.&#xD;&#xA;    player.ContractLength = 0;&#xD;&#xA;    player.ContractYear = 0;&#xD;&#xA;    player.ContractSalary0 = 0;&#xD;&#xA;    player.ContractSalary1 = 0;&#xD;&#xA;    player.ContractSalary2 = 0;&#xD;&#xA;    player.ContractSalary3 = 0;&#xD;&#xA;    player.ContractSalary4 = 0;&#xD;&#xA;    player.ContractSalary5 = 0;&#xD;&#xA;    player.ContractSalary6 = 0;&#xD;&#xA;    player.ContractSalary7 = 0;&#xD;&#xA;    player.ContractBonus0 = 0;&#xD;&#xA;    player.ContractBonus1 = 0;&#xD;&#xA;    player.ContractBonus2 = 0;&#xD;&#xA;    player.ContractBonus3 = 0;&#xD;&#xA;    player.ContractBonus4 = 0;&#xD;&#xA;    player.ContractBonus5 = 0;&#xD;&#xA;    player.ContractBonus6 = 0;&#xD;&#xA;    player.ContractBonus7 = 0;&#xD;&#xA;    player.ContractExtraYearOption = false;&#xD;&#xA;}&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;    // Player is a free agent being signed to a practice squad&#xD;&#xA;    player.PLYR_CONSECYEARSWITHTEAM = 0;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;team.PracticeSquad.Append(player);&#xD;&#xA;player.ContractStatus = PlayerContractStatus.PracticeSquad;&#xD;&#xA;player.IsInjuredReserve = false;&#xD;&#xA;player.WasOnPracticeSquadThisYear = true;&#xD;&#xA;&#xD;&#xA;PlayerManager.UpdatePlayerContractStatus(player, oldTeam, team, oldStatus, player.ContractStatus, false, false);" />
            <attribute name="SignScout" idx="46" type="TeamManager_SignScout" guid="5307-122061" final="true" default="// Assign the scout contract data.&#xD;&#xA;Scout scout= contract.StaffPerson;&#xD;&#xA;&#xD;&#xA;scout.ContractYearsRemaining = contract.Length;&#xD;&#xA;scout.ContractLength = contract.Length;&#xD;&#xA;scout.ContractSalary = contract.Salary;&#xD;&#xA;&#xD;&#xA;bool isResign = team.HeadScout == scout;&#xD;&#xA;&#xD;&#xA;// Add scout to the team.&#xD;&#xA;team.HeadScout = scout;&#xD;&#xA;&#xD;&#xA;int scoutIndex = Franchise.FreeAgentScouts.Find(scout);&#xD;&#xA;&#xD;&#xA;//Remove scout from freeAgentScouts unless they are newly created&#xD;&#xA;if ((0 &lt;= scoutIndex) &amp;&amp; (scoutIndex &lt; Franchise.FreeAgentScouts.Population()))&#xD;&#xA;{&#xD;&#xA;    Franchise.FreeAgentScouts.FindAndErase(scout);&#xD;&#xA;}&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;    // this was a newly created scout so make him signed&#xD;&#xA;    scout.ContractStatus = StaffPersonContractStatus.Signed;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;contract.Status = ContractOfferStatus.Accepted;&#xD;&#xA;&#xD;&#xA;Team nullTeam = null;&#xD;&#xA;CoachManager.UpdateScoutContractStatus(scout, nullTeam, team, StaffPersonContractStatus.Signed);&#xD;&#xA;&#xD;&#xA;StaffEventType eventType = StaffEventType.Hired;&#xD;&#xA;if (isResign)&#xD;&#xA;{&#xD;&#xA;    eventType = StaffEventType.ReSigned;&#xD;&#xA;}&#xD;&#xA;else if (IsValidationHire)&#xD;&#xA;{&#xD;&#xA;    eventType = StaffEventType.ValidationHired;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;IssueStaffEventNotificationRequest(contract, eventType, team.Owner);" />
            <attribute name="SignStaffPerson" idx="47" type="TeamManager_SignStaffPerson" guid="5307-114553" final="true" default="CoachPosition contractPosition = contract.ContractPosition;&#xD;&#xA;&#xD;&#xA;if(contractPosition == CoachPosition.HeadCoach || contractPosition == CoachPosition.DefensiveCoordinator || contractPosition == CoachPosition.OffensiveCoordinator)&#xD;&#xA;{&#xD;&#xA;SignCoach(team, contract, IsValidationHire);&#xD;&#xA;}&#xD;&#xA;else if(contractPosition == CoachPosition.Scout)&#xD;&#xA;{&#xD;&#xA;SignScout(team, contract, IsValidationHire);&#xD;&#xA;}&#xD;&#xA;else if(contractPosition == CoachPosition.Trainer)&#xD;&#xA;{&#xD;&#xA;SignTrainer(team, contract, IsValidationHire);&#xD;&#xA;}&#xD;&#xA;" />
            <attribute name="SignTrainer" idx="48" type="TeamManager_SignTrainer" guid="5307-122066" final="true" default="// Assign the scout contract data.&#xD;&#xA;Trainer trainer = contract.StaffPerson;&#xD;&#xA;&#xD;&#xA;trainer.ContractYearsRemaining = contract.Length;&#xD;&#xA;trainer.ContractLength = contract.Length;&#xD;&#xA;trainer.ContractSalary = contract.Salary;&#xD;&#xA;&#xD;&#xA;bool isResign = team.HeadTrainer == trainer;&#xD;&#xA;&#xD;&#xA;// Add trainer to the team.&#xD;&#xA;team.HeadTrainer = trainer;&#xD;&#xA;&#xD;&#xA;int trainerIndex = Franchise.FreeAgentTrainers.Find(trainer);&#xD;&#xA;&#xD;&#xA;//Remove trainer from freeAgentTrainers unless they are newly created&#xD;&#xA;if ((0 &lt;= trainerIndex) &amp;&amp; (trainerIndex &lt; Franchise.FreeAgentTrainers.Population()))&#xD;&#xA;{&#xD;&#xA;    Franchise.FreeAgentTrainers.FindAndErase(trainer);&#xD;&#xA;}&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;    // this was a newly created scout so make him signed&#xD;&#xA;    trainer.ContractStatus = StaffPersonContractStatus.Signed;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;contract.Status = ContractOfferStatus.Accepted;&#xD;&#xA;&#xD;&#xA;Team nullTeam = null;&#xD;&#xA;CoachManager.UpdateTrainerContractStatus(trainer, nullTeam, team, StaffPersonContractStatus.Signed);&#xD;&#xA;&#xD;&#xA;StaffEventType eventType = StaffEventType.Hired;&#xD;&#xA;if (isResign)&#xD;&#xA;{&#xD;&#xA;    eventType = StaffEventType.ReSigned;&#xD;&#xA;}&#xD;&#xA;else if (IsValidationHire)&#xD;&#xA;{&#xD;&#xA;    eventType = StaffEventType.ValidationHired;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;IssueStaffEventNotificationRequest(contract, eventType, team.Owner);" />
