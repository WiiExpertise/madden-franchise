        <schema name="ReSignNegotiationManager" numMembers="43" base="Module" assetId="11750" isRecordPersistent="true">
            <attribute name="AcceptOffer" idx="0" type="ReSignNegotiationManager_AcceptOffer" guid="11750-61471" final="true" />
            <attribute name="AdjustPlayerReSignStatus" idx="1" type="ReSignNegotiationManager_AdjustPlayerReSignStatus" guid="11750-1352370" final="true" default="//Tuning values&#xD;&#xA;int OneYearTotalPerYearPremium = 150;&#xD;&#xA;int TwoYearTotalPerYearPremium = 130;&#xD;&#xA;int ThreeYearTotalPerYearPremium = 120;&#xD;&#xA;int BonusSlopeModifier = 3;&#xD;&#xA;int TotalPerYearModDenominator = -4;&#xD;&#xA;&#xD;&#xA;//Reading data from reSignNegotiation. Salary and Bonus numbers are TOTAL numbers and NOT &quot;per Year&quot; numbers.&#xD;&#xA;int salaryOffer = reSignNegotiation.ContractSalary;&#xD;&#xA;int bonusOffer = reSignNegotiation.ContractBonus;&#xD;&#xA;int lengthOffer = reSignNegotiation.ContractLength;&#xD;&#xA;int salaryDesired = reSignNegotiation.ActualDesiredContractSalary;&#xD;&#xA;int bonusDesired = reSignNegotiation.ActualDesiredContractBonus;&#xD;&#xA;int lengthDesired = reSignNegotiation.ActualDesiredContractLength;&#xD;&#xA;&#xD;&#xA;//Calculate some annualized contract terms&#xD;&#xA;int totalPerYearOffer = 0;&#xD;&#xA;int totalPerYearDesired = 0;&#xD;&#xA;&#xD;&#xA;if(lengthOffer != 0 &amp;&amp; lengthDesired != 0)&#xD;&#xA;{&#xD;&#xA;  totalPerYearOffer = (salaryOffer + bonusOffer) / lengthOffer;&#xD;&#xA;  totalPerYearDesired = (salaryDesired + bonusDesired) / lengthDesired;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;//Is this an acceptable Total/Yr Difference? We will accept less total money on longer contracts but require more for shorter contracts&#xD;&#xA;int AcceptableTotalPerYearMin = totalPerYearDesired + ((lengthOffer - lengthDesired)/TotalPerYearModDenominator);&#xD;&#xA;bool isTotalPerYearDiffAcceptable = totalPerYearOffer &gt;= AcceptableTotalPerYearMin;&#xD;&#xA;&#xD;&#xA;//Is Total Bonus &gt; Desired Bonus + Acceptable Bonus Difference? We need to make sure we get an acceptable amount of guaranteed money with some wiggle room depending on contract length.&#xD;&#xA;int AcceptableBonusDiff = BonusSlopeModifier * (lengthOffer - lengthDesired);&#xD;&#xA;&#xD;&#xA;bool isTotalBonusOfferedAcceptable = bonusOffer &gt;= bonusDesired + AcceptableBonusDiff;&#xD;&#xA;&#xD;&#xA;//else ifs for the combinations. All Good combination is ignored because it should be accepted in DelegatePlayerResponse()&#xD;&#xA;if(isTotalPerYearDiffAcceptable &amp;&amp; isTotalBonusOfferedAcceptable &amp;&amp; lengthOffer &lt; lengthDesired)&#xD;&#xA;{&#xD;&#xA;  reSignNegotiation.ReSignStatus = ReSignStatus.GoodSalaryBonusResponse;&#xD;&#xA;}&#xD;&#xA;else if (isTotalPerYearDiffAcceptable &amp;&amp; isTotalBonusOfferedAcceptable == false &amp;&amp; lengthOffer &lt; lengthDesired)&#xD;&#xA;{&#xD;&#xA;  reSignNegotiation.ReSignStatus = ReSignStatus.GoodSalaryResponse;&#xD;&#xA;}&#xD;&#xA;else if (isTotalPerYearDiffAcceptable &amp;&amp; isTotalBonusOfferedAcceptable == false &amp;&amp; lengthOffer &gt;= lengthDesired)&#xD;&#xA;{&#xD;&#xA;  reSignNegotiation.ReSignStatus = ReSignStatus.GoodSalaryLengthResponse;&#xD;&#xA;}&#xD;&#xA;else if (isTotalPerYearDiffAcceptable == false &amp;&amp; isTotalBonusOfferedAcceptable &amp;&amp; lengthOffer &gt;= lengthDesired)&#xD;&#xA;{&#xD;&#xA;  reSignNegotiation.ReSignStatus = ReSignStatus.GoodBonusLengthResponse;&#xD;&#xA;}&#xD;&#xA;else if(isTotalPerYearDiffAcceptable == false &amp;&amp; isTotalBonusOfferedAcceptable == false &amp;&amp; lengthOffer &gt;= lengthDesired)&#xD;&#xA;{&#xD;&#xA;  reSignNegotiation.ReSignStatus = ReSignStatus.GoodLengthResponse;&#xD;&#xA;}&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;  reSignNegotiation.ReSignStatus = ReSignStatus.AllBadResponse;&#xD;&#xA;}" />
            <attribute name="BeginReSignTransaction" idx="2" type="ReSignNegotiationManager_BeginReSignTransaction" guid="11750-61456" final="true" />
            <attribute name="BreakdownEvent" idx="3" type="ReSignNegotiationBreakdownEvent" />
            <attribute name="ChoosePlayerResponseOffSeason" idx="4" type="ReSignNegotiationManager_ChoosePlayerResponseOffSeason" guid="11750-1352365" final="true" default="&#xD;&#xA;if(reSignNegotiation.PlayerReSignInterest &gt;= 80 || reSignNegotiation.Player.NarrativeLock)&#xD;&#xA;{&#xD;&#xA;    AcceptOffer(reSignNegotiation);&#xD;&#xA;}&#xD;&#xA;else&#xD;&#xA;    {&#xD;&#xA;        DeclineNegotiations(reSignNegotiation);&#xD;&#xA;    }&#xD;&#xA;" />
            <attribute name="DebugModuleRef" idx="5" type="FranchiseDebugModule" />
            <attribute name="DeclineNegotiations" idx="6" type="ReSignNegotiationManager_DeclineNegotiations" guid="11750-61322" final="true" default="reSignNegotiation.PreviousReSignStatus = reSignNegotiation.ReSignStatus;&#xD;&#xA;reSignNegotiation.ReSignStatus = ReSignStatus.NotInterested;&#xD;&#xA;&#xD;&#xA;PlayerSigningEval.LogPlayerDecision(reSignNegotiation.Player, reSignNegotiation.Team,  PlayerDecision.Decline);&#xD;&#xA;EndReSignTransaction(reSignNegotiation);&#xD;&#xA;&#xD;&#xA;//Send negotiation breakdown event&#xD;&#xA;BreakdownEvent.contractNegotiation = reSignNegotiation;&#xD;&#xA;BreakdownEvent.Send();&#xD;&#xA;BreakdownEvent.contractNegotiation = null;" />
            <attribute name="DelegatePlayerResponse" idx="7" type="ReSignNegotiationManager_DelegatePlayerResponse" guid="11750-1342389" final="true" default="// Determine if player should accept, decline, or wait on the offer.&#xD;&#xA;bool isWaitingOnOffer = false;&#xD;&#xA;&#xD;&#xA;reSignNegotiation.NegotiationCount = reSignNegotiation.NegotiationCount + 1;&#xD;&#xA;int randomPct = Random.GetRandomIntRange( 0, 100 );&#xD;&#xA;&#xD;&#xA;//Tuning values&#xD;&#xA;int OneYearTotalPerYearPremium = 150;&#xD;&#xA;int TwoYearTotalPerYearPremium = 130;&#xD;&#xA;int ThreeYearTotalPerYearPremium = 120;&#xD;&#xA;int BonusSlopeModifier = 3;&#xD;&#xA;int TotalPerYearModDenominator = -4;&#xD;&#xA;&#xD;&#xA;//Reading data from reSignNegotiation. Salary and Bonus numbers are TOTAL numbers and NOT &quot;per Year&quot; numbers.&#xD;&#xA;int salaryOffer = reSignNegotiation.ContractSalary;&#xD;&#xA;int bonusOffer = reSignNegotiation.ContractBonus;&#xD;&#xA;int lengthOffer = reSignNegotiation.ContractLength;&#xD;&#xA;int salaryDesired = reSignNegotiation.ActualDesiredContractSalary;&#xD;&#xA;int bonusDesired = reSignNegotiation.ActualDesiredContractBonus;&#xD;&#xA;int lengthDesired = reSignNegotiation.ActualDesiredContractLength;&#xD;&#xA;&#xD;&#xA;//Calculate some annualized contract terms&#xD;&#xA;int totalPerYearOffer = 0;&#xD;&#xA;int totalPerYearDesired = 0;&#xD;&#xA;&#xD;&#xA;if(lengthOffer != 0 &amp;&amp; lengthDesired != 0)&#xD;&#xA;{&#xD;&#xA;  totalPerYearOffer = (salaryOffer + bonusOffer) / lengthOffer;&#xD;&#xA;  totalPerYearDesired = (salaryDesired + bonusDesired) / lengthDesired;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;//Is this an acceptable Total/Yr Difference? We will accept less total money on longer contracts but require more for shorter contracts&#xD;&#xA;int AcceptableTotalPerYearMin = totalPerYearDesired + ((lengthOffer - lengthDesired)/TotalPerYearModDenominator);&#xD;&#xA;bool isTotalPerYearDiffAcceptable = totalPerYearOffer &gt;= AcceptableTotalPerYearMin;&#xD;&#xA;&#xD;&#xA;//Is Total Bonus &gt; Desired Bonus + Acceptable Bonus Difference? We need to make sure we get an acceptable amount of guaranteed money with some wiggle room depending on contract length.&#xD;&#xA;int AcceptableBonusDiff = BonusSlopeModifier * (lengthOffer - lengthDesired);&#xD;&#xA;&#xD;&#xA;bool isTotalBonusOfferedAcceptable = bonusOffer &gt;= bonusDesired + AcceptableBonusDiff;&#xD;&#xA;&#xD;&#xA;//Does the offer beat our premium price&#xD;&#xA;int PremiumTotalPerYearDesired = 0;&#xD;&#xA;if (lengthOffer == 1)&#xD;&#xA;{&#xD;&#xA;  PremiumTotalPerYearDesired = OneYearTotalPerYearPremium * totalPerYearDesired / 100;&#xD;&#xA;}&#xD;&#xA;else if (lengthOffer == 2)&#xD;&#xA;{&#xD;&#xA;  PremiumTotalPerYearDesired = TwoYearTotalPerYearPremium * totalPerYearDesired / 100;&#xD;&#xA;}&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;  PremiumTotalPerYearDesired = ThreeYearTotalPerYearPremium * totalPerYearDesired / 100;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;bool isPremiumOffer = totalPerYearOffer &gt;= PremiumTotalPerYearDesired;&#xD;&#xA;&#xD;&#xA;//if the offer is Premium, the player should accept the offer&#xD;&#xA;if (isPremiumOffer || reSignNegotiation.Player.NarrativeLock)&#xD;&#xA;{&#xD;&#xA;  AcceptOffer(reSignNegotiation);&#xD;&#xA;}&#xD;&#xA;//Otherwise we only accept offers in re-sign where our bonus per year requirements and total per year requirements are met&#xD;&#xA;else if(isTotalPerYearDiffAcceptable &amp;&amp; isTotalBonusOfferedAcceptable)&#xD;&#xA;{&#xD;&#xA;  AcceptOffer(reSignNegotiation);&#xD;&#xA;}&#xD;&#xA;//If this is the off-season, make a decision now.&#xD;&#xA;else if(SeasonInfo.CurrentStage == SeasonStage.OffSeason)&#xD;&#xA;  {&#xD;&#xA;    //instant rejection&#xD;&#xA;    DeclineNegotiations(reSignNegotiation);&#xD;&#xA;  }&#xD;&#xA;//Never reject on first offer 1st offer&#xD;&#xA;else if(reSignNegotiation.NegotiationCount == 1)&#xD;&#xA;  {&#xD;&#xA;        //we need something to wait for next week&#xD;&#xA;        isWaitingOnOffer = true;&#xD;&#xA;  }&#xD;&#xA;//2nd offer rejection chance&#xD;&#xA;else if (reSignNegotiation.NegotiationCount == 2)&#xD;&#xA;  {&#xD;&#xA;    //increase rejection chance&#xD;&#xA;    if (randomPct &lt; 80)&#xD;&#xA;      {&#xD;&#xA;        //we need something to wait for next week&#xD;&#xA;        isWaitingOnOffer = true;&#xD;&#xA;      }&#xD;&#xA;    else &#xD;&#xA;      {&#xD;&#xA;        //instant rejection&#xD;&#xA;        DeclineNegotiations(reSignNegotiation);&#xD;&#xA;      }&#xD;&#xA;  }&#xD;&#xA;//3rd offer rejection chance&#xD;&#xA;else if(reSignNegotiation.NegotiationCount == 3)&#xD;&#xA;  {&#xD;&#xA;    //increase rejection chance&#xD;&#xA;    if (randomPct &lt; 70)&#xD;&#xA;      {&#xD;&#xA;        //we need something to wait for next week&#xD;&#xA;        isWaitingOnOffer = true;&#xD;&#xA;      }&#xD;&#xA;    else &#xD;&#xA;      {&#xD;&#xA;        //instant rejection&#xD;&#xA;        DeclineNegotiations(reSignNegotiation);&#xD;&#xA;      }&#xD;&#xA;  }&#xD;&#xA;//4th offer rejection chance&#xD;&#xA;else if (reSignNegotiation.NegotiationCount &gt;= 4)&#xD;&#xA;  {&#xD;&#xA;    //50 percent chance of rejection&#xD;&#xA;    if (randomPct &lt; 50)&#xD;&#xA;      {&#xD;&#xA;        //we need something to wait for next week&#xD;&#xA;        isWaitingOnOffer = true;&#xD;&#xA;      }&#xD;&#xA;    else &#xD;&#xA;      {&#xD;&#xA;        //instant rejection&#xD;&#xA;        DeclineNegotiations(reSignNegotiation);&#xD;&#xA;      }&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;// Determine if player signed or declined the contract.&#xD;&#xA;bool contractAccepted = (reSignNegotiation.ReSignStatus == ReSignStatus.ReSigned);&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;//If we're waiting on an offer, interest status should change based on how good/poor the new offer is&#xD;&#xA;if(isWaitingOnOffer)&#xD;&#xA;{&#xD;&#xA;    AdjustPlayerReSignStatus(reSignNegotiation);&#xD;&#xA;    &#xD;&#xA;    // Wait on offer after the status/values have been updated&#xD;&#xA;    WaitOnOffer(reSignNegotiation);&#xD;&#xA;}&#xD;&#xA;// Send out notifications for the negotiation's conclusion&#xD;&#xA;else if(contractAccepted)&#xD;&#xA;{        &#xD;&#xA;    // Erase negotiation for signed players.&#xD;&#xA;    PlayerSigningEval.PlayerReSignList.FindAndErase(reSignNegotiation);&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;reSignNegotiation.PreviousOfferedContractSalary = reSignNegotiation.ContractSalary;&#xD;&#xA;reSignNegotiation.PreviousOfferedContractBonus = reSignNegotiation.ContractBonus;&#xD;&#xA;reSignNegotiation.PreviousOfferedContractLength = reSignNegotiation.ContractLength;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;isWaitingOnOffer;&#xD;&#xA;" />
