            <attribute name="ProcessPlayerResponse" idx="19" type="ReSignNegotiationManager_ProcessPlayerResponse" guid="11750-33736" final="true" default="// Handle for ReSign_PlayerRequest record.&#xD;&#xA;bool result = false;&#xD;&#xA;&#xD;&#xA;reSignNegotiation.ActiveRequestID = -1;&#xD;&#xA;Player player = reSignNegotiation.Player;&#xD;&#xA;&#xD;&#xA;if( (playerRequest.Response == PlayerSigningEval.PlayerAcceptResponseForm) || (player.NarrativeLock &amp;&amp; !player.IsUserControlled))&#xD;&#xA;{&#xD;&#xA;    // update the contract&#xD;&#xA;    // This CreatePlayerContract call is also being called inside AcceptOffer&#xD;&#xA;    PlayerContract contract = PlayerContractManager.CreatePlayerContract(reSignNegotiation.ContractSalary, reSignNegotiation.ContractBonus, reSignNegotiation.ContractLength);&#xD;&#xA;    AcceptOffer(reSignNegotiation);&#xD;&#xA;    result = false;&#xD;&#xA;}&#xD;&#xA;else if( playerRequest.Response == PlayerSigningEval.PlayerDeclineResponseForm)&#xD;&#xA;{&#xD;&#xA;    DeclineNegotiations(reSignNegotiation);&#xD;&#xA;    result = false;&#xD;&#xA;}&#xD;&#xA;else if( playerRequest.Response == PlayerSigningEval.PlayerWaitResponseForm)&#xD;&#xA;{&#xD;&#xA;    //Send Player Waiting On Negotiation event&#xD;&#xA;    WaitOnOffer(reSignNegotiation);&#xD;&#xA;    result = true;&#xD;&#xA;}&#xD;&#xA;else if( playerRequest.Response == PlayerSigningEval.PlayerDelegateResponseForm)&#xD;&#xA;{&#xD;&#xA;    //Delegate the player's response&#xD;&#xA;    result = DelegatePlayerResponse(reSignNegotiation);&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;result;" />
            <attribute name="ProcessTeamInitialResponse" idx="20" type="ReSignNegotiationManager_ProcessTeamInitialResponse" guid="11750-33728" final="true" default="bool result = false;&#xD;&#xA;&#xD;&#xA;// If the team submitted an offer, update the negotiation and continue the negotiation.&#xD;&#xA;if ((teamRequest.Response == teamRequest.MakeOfferResponse) ||&#xD;&#xA;    (teamRequest.Response == teamRequest.SubmitResponse))&#xD;&#xA;{&#xD;&#xA;    UIContractNegotiationForm contractForm = teamRequest.RequestForm;&#xD;&#xA;    reSignNegotiation.ContractLength = contractForm.Length;&#xD;&#xA;    reSignNegotiation.ContractSalary = contractForm.Salary;&#xD;&#xA;    reSignNegotiation.ContractBonus = contractForm.Bonus;&#xD;&#xA;    result = true;&#xD;&#xA;}&#xD;&#xA;// If the team chose to withdraw, end the negotiation.&#xD;&#xA;else if (teamRequest.Response == teamRequest.WithdrawResponse)&#xD;&#xA;{&#xD;&#xA;    result = false;&#xD;&#xA;}&#xD;&#xA;// If the team chose to delegate, roll the dice to see if negotiations continue.&#xD;&#xA;else if ((teamRequest.Response == null) ||&#xD;&#xA;         (teamRequest.Response == teamRequest.DefaultResponse))&#xD;&#xA;{&#xD;&#xA;    int continueChance = reSignNegotiation.TeamReSignInterest;&#xD;&#xA;    int continueValue = Random.GetRandomIntRange(0, 1);&#xD;&#xA;&#xD;&#xA;    result = (continueValue &lt; continueChance);&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Begin the re-sign transaction. Note that we don't check the result here because the negotiation&#xD;&#xA;// will always be begun and ended regardless of what the team decides here.&#xD;&#xA;BeginReSignTransaction(reSignNegotiation);&#xD;&#xA;&#xD;&#xA;result;&#xD;&#xA;" />
            <attribute name="ProcessTeamResponse" idx="21" type="ReSignNegotiationManager_ProcessTeamResponse" guid="11750-33741" final="true" default="bool result = false;&#xD;&#xA;&#xD;&#xA;// If an offer response was chosen, update the offer and continue the negotiation.&#xD;&#xA;if ((teamRequest.Response == teamRequest.MakeOfferResponse) || (teamRequest.Response == teamRequest.SubmitResponse))&#xD;&#xA;{&#xD;&#xA;   UIContractNegotiationForm contractForm = teamRequest.RequestForm;&#xD;&#xA;   result = UpdateOffer(reSignNegotiation, contractForm.Salary, contractForm.Bonus, contractForm.Length);&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// If the offer was withdrawn, log the decision and end the negotiation.&#xD;&#xA;else if (teamRequest.Response == teamRequest.WithdrawResponse)&#xD;&#xA;{&#xD;&#xA;   PlayerSigningEval.LogTeamDecision(reSignNegotiation.Team, reSignNegotiation.Player, TeamDecision.Withdraw);&#xD;&#xA;   result = false;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;else if ((teamRequest.Response == null) || (teamRequest.Response == teamRequest.DefaultResponse))&#xD;&#xA;{&#xD;&#xA;   int interest = reSignNegotiation.TeamReSignInterest;&#xD;&#xA;   int randomPct = Random.GetRandomIntRange( 0, 100 );&#xD;&#xA;   int playerPercentage = reSignNegotiation.Player.PlayerPercentage;&#xD;&#xA;   PositionE position = reSignNegotiation.Player.Position;&#xD;&#xA;&#xD;&#xA;   if(position == PositionE.QB)&#xD;&#xA;   {&#xD;&#xA;      if(reSignNegotiation.Player.SuperBowlWins &gt;= 2)&#xD;&#xA;      {&#xD;&#xA;         //increase&#xD;&#xA;         IncreaseOffer(reSignNegotiation, 8);&#xD;&#xA;         result = true;&#xD;&#xA;      }&#xD;&#xA;      else if(playerPercentage &lt;= 25)&#xD;&#xA;      {&#xD;&#xA;         //increase&#xD;&#xA;         IncreaseOffer(reSignNegotiation, 8);&#xD;&#xA;         result = true;&#xD;&#xA;      }&#xD;&#xA;      else if(playerPercentage &gt; 20)&#xD;&#xA;      {&#xD;&#xA;         // 30%keep, 70% increase&#xD;&#xA;         if(randomPct &lt; 30)&#xD;&#xA;         {&#xD;&#xA;            //keep&#xD;&#xA;            PlayerSigningEval.LogTeamDecision(reSignNegotiation.Team, reSignNegotiation.Player,  TeamDecision.Wait);&#xD;&#xA;            result = true;&#xD;&#xA;         }&#xD;&#xA;         else&#xD;&#xA;         {&#xD;&#xA;            //increase&#xD;&#xA;            IncreaseOffer(reSignNegotiation, 5);&#xD;&#xA;            result = true;&#xD;&#xA;         }&#xD;&#xA;      }&#xD;&#xA;   }&#xD;&#xA;   if(position == PositionE.FB || position == PositionE.K || position == PositionE.P)&#xD;&#xA;   {&#xD;&#xA;     if(playerPercentage &lt;= 25)&#xD;&#xA;          {&#xD;&#xA;                //increase&#xD;&#xA;           IncreaseOffer(reSignNegotiation, 7);&#xD;&#xA;             result = true;&#xD;&#xA;          }&#xD;&#xA;      else if (playerPercentage &gt;25)&#xD;&#xA;        {&#xD;&#xA;            WaitOnOffer(reSignNegotiation);&#xD;&#xA;            result = true;&#xD;&#xA;       }&#xD;&#xA;   }&#xD;&#xA;   else if(reSignNegotiation.Player.Age &gt;= 32)&#xD;&#xA;   {  &#xD;&#xA;      //keep&#xD;&#xA;      PlayerSigningEval.LogTeamDecision(reSignNegotiation.Team, reSignNegotiation.Player,  TeamDecision.Wait);&#xD;&#xA;      result = true;&#xD;&#xA;   }&#xD;&#xA;   else if(playerPercentage &lt;= 5)&#xD;&#xA;   {  &#xD;&#xA;      // 15%keep, 90% increase&#xD;&#xA;      if(randomPct &lt; 15)&#xD;&#xA;      {&#xD;&#xA;         //keep&#xD;&#xA;         PlayerSigningEval.LogTeamDecision(reSignNegotiation.Team, reSignNegotiation.Player,  TeamDecision.Wait);&#xD;&#xA;         result = true;&#xD;&#xA;      }&#xD;&#xA;      else&#xD;&#xA;      {&#xD;&#xA;         //increase&#xD;&#xA;         IncreaseOffer(reSignNegotiation, 6);&#xD;&#xA;         result = true;&#xD;&#xA;      }&#xD;&#xA;   }&#xD;&#xA;   &#xD;&#xA;   else if(playerPercentage &lt;= 20)&#xD;&#xA;   {&#xD;&#xA;      // 20%keep, 80% increase&#xD;&#xA;      if(randomPct &lt; 20)&#xD;&#xA;      {&#xD;&#xA;         //keep&#xD;&#xA;         PlayerSigningEval.LogTeamDecision(reSignNegotiation.Team, reSignNegotiation.Player,  TeamDecision.Wait);&#xD;&#xA;         result = true;&#xD;&#xA;      }&#xD;&#xA;      else&#xD;&#xA;      {&#xD;&#xA;         //increase&#xD;&#xA;         IncreaseOffer(reSignNegotiation, 5);&#xD;&#xA;         result = true;&#xD;&#xA;      }&#xD;&#xA;   }&#xD;&#xA;   else&#xD;&#xA;   if(playerPercentage &gt; 35)&#xD;&#xA;   {&#xD;&#xA;      //keep&#xD;&#xA;      PlayerSigningEval.LogTeamDecision(reSignNegotiation.Team, reSignNegotiation.Player,  TeamDecision.Wait);&#xD;&#xA;      result = true;&#xD;&#xA;   }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;result;" />
