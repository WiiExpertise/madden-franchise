        <schema name="AwardsTuning" numMembers="11" defaultStoreCapacity="1" assetId="4404442" defaultStoreGroupId="2" isRecordPersistent="true" defaultStoreGroupName="Tuning" defaultStoreOptimizationId="1" defaultStoreOptimizationName="Default_">
            <attribute name="CalculateBestPositionOfYearRating" idx="0" type="AwardsTuning_CalculateBestPositionOfYearRating" guid="4404442-1774151" final="true" default="int playerRating = 0;&#xD;&#xA;&#xD;&#xA;if(statManager != null &amp;&amp; player != null)&#xD;&#xA;{&#xD;&#xA;    SeasonStats playerSeasonStats = statManager.GetPlayerSeasonStatsForYear(player, year);&#xD;&#xA;    if(playerSeasonStats != null)&#xD;&#xA;    {&#xD;&#xA;        PositionE position = player.Position;&#xD;&#xA;        if(position == PositionE.QB)&#xD;&#xA;        {&#xD;&#xA;            int QBRating = statManager.GetPlayerPasserRating( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassAttempts),&#xD;&#xA;                                                              statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassCompleted),&#xD;&#xA;                                                              statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassYards),&#xD;&#xA;                                                              statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassTds),&#xD;&#xA;                                                              statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassInts),&#xD;&#xA;                                                              statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.GamesPlayed) );&#xD;&#xA;            QBRating = QBRating / 10; // Divide by ten and truncate.&#xD;&#xA;&#xD;&#xA;            playerRating = ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassYards) / 100 ) +&#xD;&#xA;                             ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassTds) * 5) +&#xD;&#xA;                             ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushYards) / 40 ) +&#xD;&#xA;                             ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushTds) * 5 ) +&#xD;&#xA;                             ( QBRating ) -&#xD;&#xA;                             ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassInts) * 6 ));&#xD;&#xA;        }&#xD;&#xA;        else if(position == PositionE.HB || position == PositionE.FB)&#xD;&#xA;        {&#xD;&#xA;            playerRating = &#xD;&#xA;                ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushTds) * 5 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveTDs) * 5 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushYards) / 12 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveYards) / 15 ) -&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushFumbles) * 2 ) );&#xD;&#xA;        }&#xD;&#xA;        else if(position == PositionE.WR || position == PositionE.TE)&#xD;&#xA;        {&#xD;&#xA;            playerRating = &#xD;&#xA;                ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushTds) * 3 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveTDs) * 7 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushYards) / 16 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveYards) / 12 ) -&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushFumbles) * 2 ) );&#xD;&#xA;        }&#xD;&#xA;        else if(position &gt;= PositionE.FirstOffenseLine_ &amp;&amp; position &lt;= PositionE.LastOffenseLine_)&#xD;&#xA;        {&#xD;&#xA;            playerRating = &#xD;&#xA;                ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.Pancakes) - statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.SacksAllowed) ) +&#xD;&#xA;                  ( player.GetBestOverallGrade() * 4 ) +&#xD;&#xA;                  ( teamWins * 6 ) );&#xD;&#xA;        }&#xD;&#xA;        else if(position &gt;= PositionE.FirstDefenseLine_ &amp;&amp; position &lt;= PositionE.LastDefenseLine_)&#xD;&#xA;        {&#xD;&#xA;            playerRating = &#xD;&#xA;                ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveInts) * 7 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DLineForcedFumbles) * 2 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DLineFumbleRecoveries) * 2 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveSacks) * 12 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveTacklesForLoss) * 3) +&#xD;&#xA;                  ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveTackles) * 3 ) / 4 ) );&#xD;&#xA;        }&#xD;&#xA;        else if(position &gt;= PositionE.FirstDefenseLB_ &amp;&amp; position &lt;= PositionE.LastDefenseLB_)&#xD;&#xA;        {&#xD;&#xA;            playerRating = &#xD;&#xA;                ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveInts) * 9 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DLineForcedFumbles) * 2 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DLineFumbleRecoveries) * 2 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveSacks) * 10 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveTacklesForLoss) * 3) +&#xD;&#xA;                  ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveTackles) * 3 ) / 4 ) ); &#xD;&#xA;        }&#xD;&#xA;        else if(position &gt;= PositionE.FirstDefenseDB_ &amp;&amp; position &lt;= PositionE.LastDefenseDB_)&#xD;&#xA;        {&#xD;&#xA;            playerRating = &#xD;&#xA;                ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveInts) * 15 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DLineForcedFumbles) * 2 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DLineFumbleRecoveries) * 2 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveSacks) * 3 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveTacklesForLoss) * 2) +&#xD;&#xA;                  ( (statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveTackles) * 2 ) / 7 ) );&#xD;&#xA;        }&#xD;&#xA;        else if(position == PositionE.K)&#xD;&#xA;        {&#xD;&#xA;            playerRating = &#xD;&#xA;                ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.FieldGoalMade) * 3 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.FieldGoalPct) * 40 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ExtraPointPct) * 10 ) );&#xD;&#xA;&#xD;&#xA;            int numKickoffs = statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.KickNumberofKickOffs);&#xD;&#xA;            if (numKickoffs &gt; 0)&#xD;&#xA;            {&#xD;&#xA;                playerRating = playerRating + ( ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.Touchbacks) * 100 ) / numKickoffs ) * 10 );&#xD;&#xA;            }&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;playerRating;" />
            <attribute name="CalculateCoachOfTheYearRating" idx="1" type="AwardsTuning_CalculateCoachOfTheYearRating" guid="4404442-1774120" final="true" default="int coachRating = 0;&#xD;&#xA;&#xD;&#xA;if(statManager != null &amp;&amp; coach != null &amp;&amp; team != null)&#xD;&#xA;{&#xD;&#xA;    int overallRating = team.TEAM_RATINGOVR;&#xD;&#xA;    int seasonsWithTeam = coach.SeasonsWithTeam;&#xD;&#xA;    int thisSeasWins = coach.SeasWins;&#xD;&#xA;    int lastSeasWins = team.TEAM_PREVSEASWINS;&#xD;&#xA;&#xD;&#xA;    // Avoid division by zero, and also nonsensical values from negative denominators.&#xD;&#xA;    if(overallRating &gt; 0)&#xD;&#xA;    {&#xD;&#xA;        if(seasonsWithTeam &lt;= 1)&#xD;&#xA;        {    &#xD;&#xA;            coachRating = ((105 * coach.SeasWins) / overallRating + (thisSeasWins - lastSeasWins));&#xD;&#xA;        }&#xD;&#xA;        else if(seasonsWithTeam &gt; 1)&#xD;&#xA;        {    &#xD;&#xA;            coachRating = ((100 * coach.SeasWins) / overallRating + (thisSeasWins - lastSeasWins));&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;//Clamp to 0&#xD;&#xA;if(coachRating &lt; 0)&#xD;&#xA;{&#xD;&#xA;    coachRating = 0;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;coachRating;" />
            <attribute name="CalculateDefPlayerMVPRating" idx="2" type="AwardsTuning_CalculateDefPlayerMVPRating" guid="4404442-1774130" final="true" default="int playerRating = 0;&#xD;&#xA;&#xD;&#xA;if(statManager != null &amp;&amp; player != null)&#xD;&#xA;{&#xD;&#xA;    SeasonStats playerSeasonStats = statManager.GetPlayerSeasonStatsForYear(player, year);&#xD;&#xA;    if(playerSeasonStats != null)&#xD;&#xA;    {&#xD;&#xA;        playerRating = &#xD;&#xA;            ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveInts) * 15 ) +&#xD;&#xA;              ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DLineFumbleRecoveries) * 5 ) +&#xD;&#xA;              ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DLineForcedFumbles) * 5 ) +&#xD;&#xA;              ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveSacks) * 13 ) +&#xD;&#xA;        ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveTacklesForLoss) * 2 ) +&#xD;&#xA;              ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveTackles) / 5 )  );&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;playerRating;" />
            <attribute name="CalculateDefPlayerOfWeekRating" idx="3" type="AwardsTuning_CalculateDefPlayerOfWeekRating" guid="4404442-1774115" final="true" default="// Caution! This expression is called on every signed player every week of the season&#xD;&#xA;// Be aware of what you are doing as changes here can significantly impact sim time&#xD;&#xA;&#xD;&#xA;int playerRating = 0;&#xD;&#xA;&#xD;&#xA;if(statManager!= null &amp;&amp; player != null)&#xD;&#xA;{&#xD;&#xA;    playerRating = &#xD;&#xA;        ( statManager.GetPlayerGameStat(player, StatType.DefensiveInts, week) * 12 ) +&#xD;&#xA;        ( statManager.GetPlayerGameStat(player, StatType.DLineForcedFumbles, week) * 5 ) +&#xD;&#xA;        ( statManager.GetPlayerGameStat(player, StatType.DLineFumbleRecoveries, week) * 5 ) +&#xD;&#xA;        ( statManager.GetPlayerGameStat(player, StatType.DefensiveSacks, week) * 7 ) +&#xD;&#xA;        ( statManager.GetPlayerGameStat(player, StatType.DefensiveTackles, week) * 2 ) +&#xD;&#xA;        ( statManager.GetPlayerGameStat(player, StatType.DefensiveIntsTDS, week) * 10 ) +&#xD;&#xA;        ( statManager.GetPlayerGameStat(player, StatType.DLineFumbleTDS, week) * 10 );&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;playerRating;" />
            <attribute name="CalculateDefPlayerOfYearRating" idx="4" type="AwardsTuning_CalculateDefPlayerOfYearRating" guid="4404442-1774146" final="true" default="int playerRating = 0;&#xD;&#xA;&#xD;&#xA;if(statManager != null &amp;&amp; player != null)&#xD;&#xA;{&#xD;&#xA;    SeasonStats playerSeasonStats = statManager.GetPlayerSeasonStatsForYear(player, year);&#xD;&#xA;    if(playerSeasonStats != null)&#xD;&#xA;    {&#xD;&#xA;        playerRating =&#xD;&#xA;            ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveInts) * 20 ) +&#xD;&#xA;              ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DLineForcedFumbles) * 3 ) +&#xD;&#xA;              ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DLineFumbleRecoveries) * 2 ) +&#xD;&#xA;              ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveSacks) * 10 ) +&#xD;&#xA;          &#xD;&#xA;              ( (statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveTackles) * 2) / 2) +  &#xD;&#xA;                ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveTacklesForLoss) * 3) );&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;playerRating;" />
            <attribute name="CalculateKickerPunterMVPRating" idx="5" type="AwardsTuning_CalculateKickerPunterMVPRating" guid="4404442-1774135" final="true" default="int playerRating = 0;&#xD;&#xA;&#xD;&#xA;if(statManager != null &amp;&amp; player != null)&#xD;&#xA;{&#xD;&#xA;    SeasonStats playerSeasonStats = statManager.GetPlayerSeasonStatsForYear(player, year);&#xD;&#xA;    if(playerSeasonStats != null)&#xD;&#xA;    {&#xD;&#xA;        playerRating = statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.FieldGoalMade) + teamWins;&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;playerRating;" />
            <attribute name="CalculateKPReturnerProBowlVotes" idx="6" type="AwardsTuning_CalculateKPReturnerProBowlVotes" guid="4404442-1774163" final="true" default="// Caution! This expression is called every week after midseasons for &#xD;&#xA;// every signed HB/WR/CB/FS/SS player&#xD;&#xA;// Be aware of what you are doing as changes here can significantly impact sim time&#xD;&#xA;&#xD;&#xA;int votes = 0;&#xD;&#xA;&#xD;&#xA;if(statManager != null &amp;&amp; seasonStats != null)&#xD;&#xA;{&#xD;&#xA;    int retTDS   = 0;&#xD;&#xA;    int retAvg   = 0;  &#xD;&#xA;    &#xD;&#xA;    if(position == PositionE.KR)&#xD;&#xA;    {&#xD;&#xA;        retAvg = statManager.GetPlayerSeasonStat(seasonStats, StatType.KickReturnAvg);&#xD;&#xA;    }&#xD;&#xA;    else if(position == PositionE.PR)&#xD;&#xA;    {&#xD;&#xA;        retAvg = statManager.GetPlayerSeasonStat(seasonStats, StatType.PuntReturnAvg);&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Offensive KR&#xD;&#xA;    if(isOffensiveReturner &amp;&amp; position == PositionE.KR)&#xD;&#xA;    {&#xD;&#xA;        retTDS = statManager.GetPlayerSeasonStat(seasonStats, StatType.KickReturnTDS);&#xD;&#xA;    }&#xD;&#xA;    // Offensive PR, Defensive PR, &amp; Defensive KR all use punt return TDS&#xD;&#xA;    else&#xD;&#xA;    {&#xD;&#xA;        retTDS = statManager.GetPlayerSeasonStat(seasonStats, StatType.PuntReturnTDS);&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    votes = &#xD;&#xA;&#xD;&#xA;        ( ( retAvg ) +&#xD;&#xA;          ( retTDS ) * 3 );&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;votes;" />
            <attribute name="CalculateOffPlayerMVPRating" idx="7" type="AwardsTuning_CalculateOffPlayerMVPRating" guid="4404442-1774125" final="true" default="int playerRating = 0;&#xD;&#xA;&#xD;&#xA;if(statManager != null &amp;&amp; player != null)&#xD;&#xA;{&#xD;&#xA;    SeasonStats playerSeasonStats = statManager.GetPlayerSeasonStatsForYear(player, year);&#xD;&#xA;    if(playerSeasonStats != null)&#xD;&#xA;    {&#xD;&#xA;        playerRating =&#xD;&#xA;          ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassTds) * 6 ) +&#xD;&#xA;                 ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushTds) * 9 ) +&#xD;&#xA;                 ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveTDs) * 7 ) +&#xD;&#xA;                 ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassYards) / 21 ) +&#xD;&#xA;                 ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushYards) / 9 ) +&#xD;&#xA;                 ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveYards) / 12 ) -&#xD;&#xA;&#xD;&#xA;                 ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassInts) * 7 ) + statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushFumbles) * 5 ) );&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;playerRating;" />
            <attribute name="CalculateOffPlayerOfWeekRating" idx="8" type="AwardsTuning_CalculateOffPlayerOfWeekRating" guid="4404442-1774110" final="true" default="// Caution! This expression is called on every signed player every week of the season&#xD;&#xA;// Be aware of what you are doing as changes here can significantly impact sim time&#xD;&#xA;&#xD;&#xA;int playerRating = 0;&#xD;&#xA;&#xD;&#xA;if(statManager != null &amp;&amp; player != null)&#xD;&#xA;{&#xD;&#xA;    playerRating =&#xD;&#xA;        ( ( statManager.GetPlayerGameStat(player, StatType.PassTds, week) * 5 ) +&#xD;&#xA;          ( statManager.GetPlayerGameStat(player, StatType.RushTds, week) * 4 ) +&#xD;&#xA;          ( statManager.GetPlayerGameStat(player, StatType.ReceiveTDs, week) * 5 ) ) +&#xD;&#xA;&#xD;&#xA;        ( ( statManager.GetPlayerGameStat(player, StatType.PassYards, week) / 25 ) +&#xD;&#xA;          ( statManager.GetPlayerGameStat(player, StatType.RushYards, week) / 11 ) +&#xD;&#xA;          ( statManager.GetPlayerGameStat(player, StatType.ReceiveYards, week) / 14 ) ) +&#xD;&#xA;&#xD;&#xA;        ( ( statManager.GetPlayerGameStat(player, StatType.PassYards, week) / 150 ) +&#xD;&#xA;          ( statManager.GetPlayerGameStat(player, StatType.RushYards, week) / 50 ) +&#xD;&#xA;          ( statManager.GetPlayerGameStat(player, StatType.ReceiveYards, week) / 40 ) ) -&#xD;&#xA;&#xD;&#xA;        ( ( statManager.GetPlayerGameStat(player, StatType.PassInts, week) * 6 ) +&#xD;&#xA;          ( statManager.GetPlayerGameStat(player, StatType.RushFumbles, week) * 3 ) );&#xD;&#xA;&#xD;&#xA;    // Prevent a negative rating from interceptions and fumbles&#xD;&#xA;    if (playerRating &lt; 0)&#xD;&#xA;    {&#xD;&#xA;        playerRating = 0;&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;playerRating;" />
            <attribute name="CalculateOffPlayerOfYearRating" idx="9" type="AwardsTuning_CalculateOffPlayerOfYearRating" guid="4404442-1774141" final="true" default="int playerRating = 0;&#xD;&#xA;&#xD;&#xA;if(statManager != null &amp;&amp; player != null)&#xD;&#xA;{&#xD;&#xA;    SeasonStats playerSeasonStats = statManager.GetPlayerSeasonStatsForYear(player, year);&#xD;&#xA;    if(playerSeasonStats != null)&#xD;&#xA;    {&#xD;&#xA;        playerRating =&#xD;&#xA;            ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassTds) * 5 ) +&#xD;&#xA;              ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushTds) * 6 ) +&#xD;&#xA;              ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveTDs) * 6 ) +&#xD;&#xA;              ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassYards) / 22 ) +&#xD;&#xA;              ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushYards) / 8 ) +&#xD;&#xA;              ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveYards) / 10 ) ) -&#xD;&#xA;&#xD;&#xA;            ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassInts) * 6 ) +&#xD;&#xA;              ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushFumbles) * 4 ) );&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;playerRating;" />
            <attribute name="CalculateProBowlVotesForPlayer" idx="10" type="AwardsTuning_CalculateProBowlVotesForPlayer" guid="4404442-1774157" final="true" default="// Caution! This expression is called every week after midseason for &#xD;&#xA;// almost every signed player&#xD;&#xA;// Be aware of what you are doing as changes here can significantly impact sim time&#xD;&#xA;&#xD;&#xA;int votes = 0;&#xD;&#xA;&#xD;&#xA;if(statManager != null &amp;&amp; player != null)&#xD;&#xA;{&#xD;&#xA;    SeasonStats playerSeasonStats = statManager.GetPlayerSeasonStatsForYear(player, year);&#xD;&#xA;    if(playerSeasonStats != null)&#xD;&#xA;    {&#xD;&#xA;        PositionE position = player.Position;&#xD;&#xA;        if(position == PositionE.QB)&#xD;&#xA;        {&#xD;&#xA;            int minYearlyQBPassAttempts = 200;&#xD;&#xA;            int passYards = statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassYards);&#xD;&#xA;            int passAttempts = statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassAttempts);&#xD;&#xA;            int QBRating = 0;&#xD;&#xA;            if (passAttempts &gt;= minYearlyQBPassAttempts)&#xD;&#xA;            {&#xD;&#xA;                QBRating = statManager.GetPlayerPasserRating( passAttempts,&#xD;&#xA;                                                              statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassCompleted),&#xD;&#xA;                                                              passYards,&#xD;&#xA;                                                              statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassTds),&#xD;&#xA;                                                              statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PassInts),&#xD;&#xA;                                                              statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.GamesPlayed) );&#xD;&#xA;&#xD;&#xA;                QBRating = QBRating / 10; // Divide by ten and truncate.&#xD;&#xA;            }&#xD;&#xA;&#xD;&#xA;            // PASSYDS/100 + QBRATING/2 + 3*TeamWins + RushYds/100 + RushTDs&#xD;&#xA;            votes = &#xD;&#xA;                ( ( passYards / 100 ) + &#xD;&#xA;                  ( QBRating / 2 ) + &#xD;&#xA;                  ( teamWins * 3 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushYards) / 100) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushTds)) );&#xD;&#xA;        }&#xD;&#xA;        else if(position == PositionE.HB)&#xD;&#xA;        {&#xD;&#xA;            // RUSHYDS/25 + RECYDS/35 + RUSHTDS - FUMB&#xD;&#xA;            votes = &#xD;&#xA;                ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushYards) / 25 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveYards) / 35 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushTds) ) -&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushFumbles) ) );&#xD;&#xA;        }&#xD;&#xA;        else if(position == PositionE.FB)&#xD;&#xA;        {&#xD;&#xA;            // votes = teamWins + gamesStarted + rushTDS*6 + bestOverallGrade * (3/4) - rushFumbles;&#xD;&#xA;            votes = &#xD;&#xA;                ( ( teamWins ) + &#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.GamesStarted) ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushTds) * 6 ) +&#xD;&#xA;                  ( (player.GetBestOverallGrade() * 3) / 4 ) -&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushFumbles) ) );&#xD;&#xA;        }&#xD;&#xA;        else if(position == PositionE.WR)&#xD;&#xA;        {&#xD;&#xA;            // votes = rushYards / 20 + recieveYards / 25 + recieveCatches / 2 + recieveTDS * 2;&#xD;&#xA;                votes = &#xD;&#xA;                ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushYards) / 20 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveYards) / 25 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveCatches) / 2 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveTDs) * 2 ) );&#xD;&#xA;        }&#xD;&#xA;        else if(position == PositionE.TE)&#xD;&#xA;        {&#xD;&#xA;            // RUSHYDS/20 + RECYDS/35 + RECCATCHES/3 + RECTDS*1.5 + TeamWins*1.5&#xD;&#xA;            votes = &#xD;&#xA;                ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.RushYards) / 20 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveYards) / 35 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveCatches) / 2 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ReceiveTDs) * 2 ) +&#xD;&#xA;                  ( teamWins * 2 ) );&#xD;&#xA;        }&#xD;&#xA;        else if(position &gt;= PositionE.FirstOffenseLine_ &amp;&amp; position &lt;= PositionE.LastOffenseLine_)&#xD;&#xA;        {&#xD;&#xA;            // votes = teamWins + bestOverallGrade + gamesStarted * 3&#xD;&#xA;            votes = &#xD;&#xA;                ( ( teamWins ) +&#xD;&#xA;                  ( player.GetBestOverallGrade() ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.GamesStarted) * 3 ) );&#xD;&#xA;        }&#xD;&#xA;        else if( (position &gt;= PositionE.FirstDefenseLine_ &amp;&amp; position &lt;= PositionE.LastDefenseLine_) ||&#xD;&#xA;                 (position &gt;= PositionE.FirstDefenseLB_ &amp;&amp; position &lt;= PositionE.LastDefenseLB_ ) )&#xD;&#xA;        {&#xD;&#xA;            //votes = defensiveInts * 3 + dLineForcedFumbles * 2 + dLineFumbleRecoveries * 2 + defensiveSacks * 8 + defensiveTackles * (5 / 4) + TackleForLoss * 3 teamWins&#xD;&#xA;            votes = &#xD;&#xA;                ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveInts) * 3 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DLineForcedFumbles) * 2 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DLineFumbleRecoveries) * 2 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveSacks) * 8 ) +&#xD;&#xA;                  ( (statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveTackles) * 5) / 4 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveTacklesForLoss) * 3 ) +&#xD;&#xA;                  ( teamWins ) );&#xD;&#xA;        }&#xD;&#xA;        else if(position &gt;= PositionE.FirstDefenseDB_ &amp;&amp; position &lt;= PositionE.LastDefenseDB_)&#xD;&#xA;        {&#xD;&#xA;            // votes = defensiveInts * 8 + dLineForcedFumbles * 2 + dLineFumbleRecoveries * 2 + defensiveSacks * 3 + defensiveTackles * (5 / 4) + teamWins&#xD;&#xA;            votes = &#xD;&#xA;                ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveInts) * 8 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DLineForcedFumbles) * 2 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DLineFumbleRecoveries) * 2 ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveSacks) * 5 ) +&#xD;&#xA;                  ( (statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.DefensiveTackles) * 3) / 4 ) +&#xD;&#xA;                  ( teamWins ) );&#xD;&#xA;        }&#xD;&#xA;        else if(position == PositionE.K)&#xD;&#xA;        {&#xD;&#xA;            // K = FGMADE + FG%*30 + XP%*10 + TB%*20 + teamWins&#xD;&#xA;            // FGMade + ( ((FG% * 3) + (XP% * 2)) / 10 )&#xD;&#xA;            votes = &#xD;&#xA;                ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.FieldGoalMade) ) +&#xD;&#xA;                  ( ( (statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.FieldGoalPct) * 3) +&#xD;&#xA;                      (statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.ExtraPointPct) * 2) ) / 10 ) );&#xD;&#xA;      &#xD;&#xA;            int numKickoffs = statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.KickNumberofKickOffs);&#xD;&#xA;            if(numKickoffs &gt; 0)&#xD;&#xA;            {&#xD;&#xA;                votes = votes + ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.Touchbacks) / numKickoffs ) * 30 ) + teamWins;&#xD;&#xA;            }&#xD;&#xA;        }&#xD;&#xA;        else if(position == PositionE.P)&#xD;&#xA;        {&#xD;&#xA;            // Punt ave + PUNTIN20 + SeasonWins&#xD;&#xA;            votes = &#xD;&#xA;                ( ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PuntAvg) ) +&#xD;&#xA;                  ( statManager.GetPlayerSeasonStat(playerSeasonStats, StatType.PuntIn20) ) +&#xD;&#xA;                  teamWins );&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;votes;" />
        </schema>
