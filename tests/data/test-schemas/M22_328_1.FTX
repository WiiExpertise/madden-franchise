            <attribute name="TradePlayer" idx="55" type="TeamManager_TradePlayer" guid="5307-28097" final="true" default="// Note: All players involved in a trade must be removed from their teams prior&#xD;&#xA;// to calling this method in case both teams have a full roster.&#xD;&#xA;&#xD;&#xA;PlayerContractStatus oldStatus = player.ContractStatus;&#xD;&#xA;PlayerContractStatus newStatus = PlayerContractStatus.Signed;&#xD;&#xA;&#xD;&#xA;// Assign the player a jersey number prior to adding him to the roster.&#xD;&#xA;player.JerseyNum = FindAvailableJerseyNumber(newTeam, player);&#xD;&#xA;&#xD;&#xA;// Add the player to the new team.&#xD;&#xA;AddPlayer(newTeam, player, newStatus);&#xD;&#xA;&#xD;&#xA;// Update the player's contract status.&#xD;&#xA;PlayerManager.UpdatePlayerContractStatus(player, oldTeam, newTeam, oldStatus, newStatus, false, false);&#xD;&#xA;&#xD;&#xA;// Clear the player's signing bonus. The new team does not have to pay it. Note that we do this&#xD;&#xA;// after updating the player's contract status so that the old team can remove it from their&#xD;&#xA;// salary reserves properly.&#xD;&#xA;player.ContractBonus0 = 0;&#xD;&#xA;player.ContractBonus1 = 0;&#xD;&#xA;player.ContractBonus2 = 0;&#xD;&#xA;player.ContractBonus3 = 0;&#xD;&#xA;player.ContractBonus4 = 0;&#xD;&#xA;player.ContractBonus5 = 0;&#xD;&#xA;player.ContractBonus6 = 0;&#xD;&#xA;player.ContractBonus7 = 0;&#xD;&#xA;" />
            <attribute name="UpdateTeamEndOfYearContentionPhase" idx="56" type="TeamManager_UpdateTeamEndOfYearContentionPhase" guid="5307-1342018" final="true" default="// Null-check the team parameter.&#xD;&#xA;Team nullTeam = null;&#xD;&#xA;&#xD;&#xA;if (team != nullTeam)&#xD;&#xA;{&#xD;&#xA;    // Updates the team's contention phase depending on the results of the most recent season.&#xD;&#xA;    PlayoffRoundType lastYearPlayoffResult = team.PlayoffRoundReached;&#xD;&#xA;    TeamContentionPhase phase = TeamContentionPhase.Losing;&#xD;&#xA;&#xD;&#xA;    // Team made the playoffs last year. &#xD;&#xA;    if (lastYearPlayoffResult != PlayoffRoundType.None)&#xD;&#xA;    {&#xD;&#xA;        // Team made the Conference game last year.  They're in a championship (win now) phase.&#xD;&#xA;        if (lastYearPlayoffResult &gt;= PlayoffRoundType.ConferenceChampionship)&#xD;&#xA;        {&#xD;&#xA;            phase = TeamContentionPhase.Championship;&#xD;&#xA;        }&#xD;&#xA;        // Team made a playoff run but didn't reach the Championship game.  They're in an playoff (balanced) phase.&#xD;&#xA;        else&#xD;&#xA;        {&#xD;&#xA;            phase = TeamContentionPhase.Playoff;&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;    // Team missed the playoffs last year.&#xD;&#xA;    else&#xD;&#xA;    {&#xD;&#xA;        // Team had a winning season last year.  They're in an playoff (balanced) phase.&#xD;&#xA;        if (team.SeasonWinPct &gt;= 500)&#xD;&#xA;        {&#xD;&#xA;            phase = TeamContentionPhase.Playoff;&#xD;&#xA;        }&#xD;&#xA;        // Team had a losing season last year.  They're in a losing (rebuilding) phase.&#xD;&#xA;        else&#xD;&#xA;        {&#xD;&#xA;            phase = TeamContentionPhase.Losing;&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Update the Team record with the new contention phase for the upcoming year or remainder of season.&#xD;&#xA;    team.ContentionPhase = phase;&#xD;&#xA;}" />
