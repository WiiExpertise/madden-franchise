        <schema name="UpdateReSignNegotiationRequests" numMembers="10" base="ModifierView" assetId="11752" isRecordPersistent="true">
            <attribute name="DelegateTeamRequest" idx="0" type="bool" default="False" />
            <attribute name="Formula" idx="1" type="UpdateReSignNegotiationRequests_Formula" guid="11752-33656" default="PlayerReSignNegotiation negotiation = Subject;&#xD;&#xA;Team team = negotiation.Team;&#xD;&#xA;Player player = negotiation.Player;&#xD;&#xA;&#xD;&#xA;// Find the depth chart depth for this player.&#xD;&#xA;int depth = team.DepthChart.GetPlayerDepth(player, player.Position);&#xD;&#xA;&#xD;&#xA;// If the player does not yet have a contract, generate it.&#xD;&#xA;if (negotiation.ContractLength == 0)&#xD;&#xA;{&#xD;&#xA;    // Generate the market value contract.&#xD;&#xA;    PlayerBaseContract marketContract = PlayerSigningEval.GenerateMarketValueContract(player);&#xD;&#xA;    &#xD;&#xA;    if (marketContract != null)&#xD;&#xA;    {&#xD;&#xA;        negotiation.ContractSalary = marketContract.Salary;&#xD;&#xA;        negotiation.ContractBonus = marketContract.Bonus;&#xD;&#xA;        negotiation.ContractLength = marketContract.Length;&#xD;&#xA;        &#xD;&#xA;        negotiation.FairMarketContractSalary = marketContract.Salary;&#xD;&#xA;        negotiation.FairMarketContractBonus = marketContract.Bonus;&#xD;&#xA;        negotiation.FairMarketContractLength = marketContract.Length;&#xD;&#xA;        &#xD;&#xA;        int contractVarianceRoll = ReSignNegotiationManager.Random.GetRandomIntRange(0, 100);&#xD;&#xA;        int contractVariancePercent = ReSignNegotiationManager.DesiredContractVarianceSpline.CalculateY(contractVarianceRoll);&#xD;&#xA;        &#xD;&#xA;        negotiation.ActualDesiredContractSalary = marketContract.Salary + ((marketContract.Salary * contractVariancePercent ) / 100);&#xD;&#xA;        negotiation.ActualDesiredContractBonus = marketContract.Bonus + ((marketContract.Bonus * contractVariancePercent ) / 100);&#xD;&#xA;        negotiation.ActualDesiredContractLength = marketContract.Length;&#xD;&#xA;        negotiation.NegotiationCount = 0;&#xD;&#xA;    &#xD;&#xA;        negotiation.InitialContract = true;&#xD;&#xA;    }&#xD;&#xA;    else&#xD;&#xA;    {&#xD;&#xA;        negotiation.ContractSalary = 0;&#xD;&#xA;        negotiation.ContractBonus = 0;&#xD;&#xA;        negotiation.ContractLength = 0;&#xD;&#xA;        &#xD;&#xA;        negotiation.FairMarketContractSalary = 0;&#xD;&#xA;        negotiation.FairMarketContractBonus = 0;&#xD;&#xA;        negotiation.FairMarketContractLength = 0;&#xD;&#xA;        &#xD;&#xA;        negotiation.ActualDesiredContractSalary = 0;&#xD;&#xA;        negotiation.ActualDesiredContractBonus = 0;&#xD;&#xA;        negotiation.ActualDesiredContractLength = 0;&#xD;&#xA;        negotiation.NegotiationCount = 0;&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// If the player has already been re-signed, end the negotiation and remove it.&#xD;&#xA;if (negotiation.ReSignStatus == ReSignStatus.ReSigned)&#xD;&#xA;{&#xD;&#xA;    negotiation.NegotiationsEnded = true;&#xD;&#xA;&#xD;&#xA;    // Create the contract response.&#xD;&#xA;    ContractResponse contractResponse = new ContractResponse;&#xD;&#xA;    contractResponse.Player = player;&#xD;&#xA;    contractResponse.Accepted = true;&#xD;&#xA;    contractResponse.ContractLength = negotiation.ContractLength;&#xD;&#xA;    contractResponse.ContractSalary = negotiation.ContractSalary;&#xD;&#xA;    contractResponse.ContractBonus = negotiation.ContractBonus;&#xD;&#xA;&#xD;&#xA;    // Create and issue a accept notification.&#xD;&#xA;    ReSign_PlayerAcceptedRequest request = PlayerSigningEval.CreatePlayerOfferAcceptedRequest();&#xD;&#xA;    if (request != null)&#xD;&#xA;    {&#xD;&#xA;        request.RequestData = contractResponse;&#xD;&#xA;        TeamRequestManager.Issue(team, request );&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Erase the negotiation from the list.&#xD;&#xA;    PlayerSigningEval.PlayerReSignList.FindAndErase( negotiation );&#xD;&#xA;}&#xD;&#xA;// If the player is unavailable, end the negotiation and remove it.&#xD;&#xA;else if (negotiation.ReSignStatus == ReSignStatus.NotInterested)&#xD;&#xA;{&#xD;&#xA;    negotiation.NegotiationsEnded = true;&#xD;&#xA;&#xD;&#xA;    // Create the contract response.&#xD;&#xA;    ContractResponse contractResponse = new ContractResponse;&#xD;&#xA;    contractResponse.Player = player;&#xD;&#xA;    contractResponse.Accepted = false;&#xD;&#xA;    contractResponse.ContractLength = negotiation.ContractLength;&#xD;&#xA;    contractResponse.ContractSalary = negotiation.ContractSalary;&#xD;&#xA;    contractResponse.ContractBonus = negotiation.ContractBonus;&#xD;&#xA;&#xD;&#xA;    // Create and issue the decline notification.&#xD;&#xA;    ReSign_PlayerDeclinedRequest request = PlayerSigningEval.CreatePlayerOfferDeclinedRequest();&#xD;&#xA;    if (request != null)&#xD;&#xA;    {&#xD;&#xA;        request.RequestData = contractResponse;&#xD;&#xA;        TeamRequestManager.Issue(team, request);&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;// If the player is still signed, but has not re-signed, send the team request.&#xD;&#xA;else if ( (player.ContractStatus == PlayerContractStatus.Signed) || (player.ContractStatus == PlayerContractStatus.Expiring) )&#xD;&#xA;{&#xD;&#xA;    if (negotiation.ReSignStatus &lt; ReSignStatus.NegotiatingBegin || negotiation.ReSignStatus &gt; ReSignStatus.NegotiatingEnd)&#xD;&#xA;    {&#xD;&#xA;        negotiation.ReSignStatus =  ReSignStatus.StartNegotiations;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // If the team requests flag is set, proceed. Otherwise, this view is only being called to&#xD;&#xA;    // process player responses.&#xD;&#xA;    if (SendTeamRequests)&#xD;&#xA;    {&#xD;&#xA;   // If the player is locked by narrative we force the team to be interested.&#xD;&#xA;   if(player.NarrativeLock)&#xD;&#xA;   {&#xD;&#xA;       negotiation.TeamReSignInterest = 100;&#xD;&#xA;   }&#xD;&#xA;   // Otherwise, determine the interest.&#xD;&#xA;   else&#xD;&#xA;   {&#xD;&#xA;       // If the team can offer the player, determine their interest. Note that we don't check the&#xD;&#xA;            // spending money if it is not the initial offer, since we assume the previous offer was&#xD;&#xA;            // already validated.&#xD;&#xA;            if (!negotiation.InitialContract || PlayerSigningEval.CanTeamOfferReSignPlayer(team, player, negotiation.ContractLength, negotiation.ContractSalary, negotiation.ContractBonus))&#xD;&#xA;            {&#xD;&#xA;                negotiation.TeamReSignInterest = PlayerSigningEval.TeamReSignInterest(team, player);&#xD;&#xA;            }&#xD;&#xA;            // Otherwise, the team is not interested.&#xD;&#xA;            else&#xD;&#xA;            {&#xD;&#xA;                negotiation.TeamReSignInterest = 0;&#xD;&#xA;            }&#xD;&#xA;   }&#xD;&#xA;&#xD;&#xA;        // Create the re-sign request, but only if it is delegated.  It should not be visible to the user.&#xD;&#xA;        if (DelegateTeamRequest)&#xD;&#xA;        {&#xD;&#xA;            ReSign_TeamRequest teamRequest = PlayerSigningEval.CreateTeamRequest(negotiation.ContractLength, negotiation.ContractSalary, negotiation.ContractBonus);&#xD;&#xA;            if (teamRequest != null)&#xD;&#xA;            {&#xD;&#xA;                teamRequest.Player = player;&#xD;&#xA;                teamRequest.Team = team;&#xD;&#xA;                teamRequest.PlayerDesiredSalary = negotiation.FairMarketContractSalary;&#xD;&#xA;                teamRequest.PlayerDesiredLength = negotiation.FairMarketContractLength;&#xD;&#xA;                teamRequest.PlayerDesiredBonus = negotiation.FairMarketContractBonus;&#xD;&#xA;                teamRequest.TeamReSignInterest = negotiation.TeamReSignInterest;&#xD;&#xA;                teamRequest.PlayerReSignInterest = negotiation.PlayerReSignInterest;&#xD;&#xA;                teamRequest.RequestData = team;&#xD;&#xA;                teamRequest.PlayerReSignNegotiation = negotiation;&#xD;&#xA;                teamRequest.ReSignNegotiationManager = ReSignNegotiationManager;&#xD;&#xA;                teamRequest.IsInitialOffer = negotiation.InitialContract;&#xD;&#xA;                &#xD;&#xA;                teamRequest.Response = teamRequest.DefaultResponse;&#xD;&#xA;                teamRequest.HandleResponse();&#xD;&#xA;            }&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;// Otherwise, the negotiation must end.&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;    negotiation.NegotiationsEnded = true;&#xD;&#xA;}&#xD;&#xA;" />
            <attribute name="PlayerGradeEval" idx="2" type="PlayerGradeEval" />
            <attribute name="PlayerSigningEval" idx="3" type="PlayerSigningEval" />
            <attribute name="ReSignNegotiationManager" idx="4" type="ReSignNegotiationManager" />
            <attribute name="SendTeamRequests" idx="5" type="bool" default="True" />
