            <attribute name="HandleRegisterUser" idx="15" type="DraftTransactionV2_HandleRegisterUser" guid="5295443-1824024" final="true" default="FranchiseUser newUser = event.User;&#xD;&#xA;Team newUserTeam = newUser.Team;&#xD;&#xA;Team currentTeam = DraftInfo.CurrentTeam;&#xD;&#xA;&#xD;&#xA;if (DraftInfo.IsLive &amp;&amp; !DraftInfo.IsFantasyDraft &amp;&amp; currentTeam == newUserTeam)&#xD;&#xA;{&#xD;&#xA;    Request.Dismiss();&#xD;&#xA;    // Create the draft pick request for the current team.&#xD;&#xA;    Request = DraftEval.InitDraftPickRequest(DraftInfo.CurrentTeam, DraftInfo.CurrentPick, Request, DraftInfo.CurrentPickIndex);&#xD;&#xA;    &#xD;&#xA;   // Issue the draft pick request.&#xD;&#xA;   TeamRequestManager.Issue(DraftInfo.CurrentTeam, Request);&#xD;&#xA;&#xD;&#xA;   // If the pick was not already traded, begin a trade negotiation for this pick.&#xD;&#xA;   if (!IsDraftPickTraded)&#xD;&#xA;   {&#xD;&#xA;       // Clear the outstanding trade negotiation.&#xD;&#xA;       if (TradeNegotiation != null)&#xD;&#xA;       {&#xD;&#xA;          TradeNegotiation.Response = TradeResponse.Decline;&#xD;&#xA;          TradeNegotiationManager.EndTradeNegotiation(TradeNegotiation);&#xD;&#xA;          TradeNegotiation = null;&#xD;&#xA;       }&#xD;&#xA;       TradeNegotiation = TradeNegotiationManager.BeginDraftPickTradeNegotiation(DraftInfo.CurrentTeam, DraftInfo.CurrentPick);&#xD;&#xA;   }&#xD;&#xA;}&#xD;&#xA;" />
            <attribute name="HandleTrade" idx="16" type="DraftTransactionV2_HandleTrade" guid="5295443-1824020" final="true" default="// If the normal draft is active, check if the current pick was traded.&#xD;&#xA;if (!DraftInfo.IsDisableTrading &amp;&amp; !DraftInfo.IsFantasyDraft &amp;&amp; DraftInfo.IsStarted &amp;&amp; !DraftInfo.IsComplete)&#xD;&#xA;{&#xD;&#xA;    DraftPick currentPick = DraftInfo.CurrentPick;&#xD;&#xA;&#xD;&#xA;    // If the current pick is valid, check if it is included in the trade. If so, we need to&#xD;&#xA;    // transition the draft flow to the new team.&#xD;&#xA;    if (currentPick != null)&#xD;&#xA;    {&#xD;&#xA;        TeamTradePackage packageA = event.PackageA;&#xD;&#xA;        TeamTradePackage packageB = event.PackageB;&#xD;&#xA;    &#xD;&#xA;        // If either package contains the current pick, store the external trade.&#xD;&#xA;        if ((packageA.Entity1 == currentPick) ||&#xD;&#xA;            (packageA.Entity2 == currentPick) ||&#xD;&#xA;            (packageA.Entity3 == currentPick) ||&#xD;&#xA;            (packageB.Entity1 == currentPick) ||&#xD;&#xA;            (packageB.Entity2 == currentPick) ||&#xD;&#xA;            (packageB.Entity3 == currentPick))&#xD;&#xA;        {&#xD;&#xA;            TradePackageA = packageA;&#xD;&#xA;            TradePackageB = packageB;&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;" />
