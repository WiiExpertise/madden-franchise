            <attribute name="SetPlayerContractData" idx="41" type="TeamManager_SetPlayerContractData" guid="5307-1636008" final="true" default="//Force player's contract with new salary, bonus and contract length values&#xD;&#xA;player.ContractLength = contract.Length;&#xD;&#xA;player.ContractYear = contract.CurrentYear;&#xD;&#xA;&#xD;&#xA;// Assign the player salary.&#xD;&#xA;player.ContractSalary0 = contract.SalaryTable[0];&#xD;&#xA;player.ContractSalary1 = contract.SalaryTable[1];&#xD;&#xA;player.ContractSalary2 = contract.SalaryTable[2];&#xD;&#xA;player.ContractSalary3 = contract.SalaryTable[3];&#xD;&#xA;player.ContractSalary4 = contract.SalaryTable[4];&#xD;&#xA;player.ContractSalary5 = contract.SalaryTable[5];&#xD;&#xA;player.ContractSalary6 = contract.SalaryTable[6];&#xD;&#xA;&#xD;&#xA;// Assign the player bonus.&#xD;&#xA;player.ContractBonus0 = contract.BonusTable[0];&#xD;&#xA;player.ContractBonus1 = contract.BonusTable[1];&#xD;&#xA;player.ContractBonus2 = contract.BonusTable[2];&#xD;&#xA;player.ContractBonus3 = contract.BonusTable[3];&#xD;&#xA;player.ContractBonus4 = contract.BonusTable[4];&#xD;&#xA;player.ContractBonus5 = contract.BonusTable[5];&#xD;&#xA;player.ContractBonus6 = contract.BonusTable[6];" />
