            <attribute name="GetPlayerPerUnreachedMaxPosition" idx="17" type="DraftEval_GetPlayerPerUnreachedMaxPosition" guid="5157-1378079" final="true" />
            <attribute name="GetTopOfTheDraftBoardPlayer" idx="18" type="DraftEval_GetTopOfTheDraftBoardPlayer" guid="5157-1378091" final="true" />
            <attribute name="GetUndraftedPlayer" idx="19" type="DraftEval_GetUndraftedPlayer" guid="5157-1378417" final="true" />
            <attribute name="GetUndraftedPlayerPositionOnTheBoard" idx="20" type="DraftEval_GetUndraftedPlayerPositionOnTheBoard" guid="5157-1378103" final="true" />
            <attribute name="HasDraftValue" idx="21" type="DraftEval_HasDraftValue" guid="5157-1698000" final="true" default="// IMPORTANT!! This is called per potential draft pick per team during the draft,&#xD;&#xA;// so keep performance in mind when authoring logic!&#xD;&#xA;// The default value of hasValue is true. Every draft pick starts as having a draft value.&#xD;&#xA;bool hasValue = true;&#xD;&#xA;// DESIGN SHOULD NOT CHANGE&#xD;&#xA;int DEPTH_ZERO = 0;&#xD;&#xA;int STARTING_POSITION = 0;&#xD;&#xA;int BACKUP_POSITION = 1;&#xD;&#xA;int FIRST_ROUND = 0;&#xD;&#xA;int SECOND_ROUND = 1;&#xD;&#xA;int THIRD_ROUND = 2;&#xD;&#xA;int PREVIOUS_DRAFT_YEAR = (SeasonInfo.CurrentYear - 1);&#xD;&#xA;int PREVIOUS_DRAFT_YEAR_2 = (SeasonInfo.CurrentYear - 2);&#xD;&#xA;&#xD;&#xA;// DESIGN CAN CHANGE&#xD;&#xA;int MINIMUM_YEARS_PRO = 2;&#xD;&#xA;int MIN_QB_AGE = 30;&#xD;&#xA;int MIN_SAFE_QB_AGE = 27;&#xD;&#xA;int MIN_BACKUP_QB_AGE = 28;&#xD;&#xA;int MIN_OLD_QB_AGE = 35;&#xD;&#xA;int MIN_OLD_QB_OVR = 87;&#xD;&#xA;int MIN_NORMAL_QB_OVR = 71;&#xD;&#xA;int MIN_SAFE_QB_OVR = 73;&#xD;&#xA;int MIN_GREAT_QB_OVR = 85;&#xD;&#xA;&#xD;&#xA;//Draft Thresholds Per Position Depth&#xD;&#xA;int MIN_QB2_PICKNUMBER = 16;&#xD;&#xA;int MIN_HB1_PICKNUMBER = 4;&#xD;&#xA;int MIN_HB2_PICKNUMBER = 24;&#xD;&#xA;int MIN_WR1_PICKNUMBER = 4;&#xD;&#xA;int MIN_WR2_PICKNUMBER = 7;&#xD;&#xA;int MIN_TE1_PICKNUMBER = 6;&#xD;&#xA;int MIN_TE2_PICKNUMBER = 22;&#xD;&#xA;int MIN_RT1_PICKNUMBER = 8;&#xD;&#xA;int MIN_LRT2_PICKNUMBER = 22;&#xD;&#xA;int MIN_LRG1_PICKNUMBER = 8;&#xD;&#xA;int MIN_C1_PICKNUMBER = 20;&#xD;&#xA;int MIN_IOL2_PICKNUMBER = 29;&#xD;&#xA;int MIN_DE2_PICKNUMBER = 9;&#xD;&#xA;int MIN_DT1_PICKNUMBER = 3;&#xD;&#xA;int MIN_DT2_PICKNUMBER = 7;&#xD;&#xA;int MIN_OLB1_PICKNUMBER = 4;&#xD;&#xA;int MIN_OLB2_PICKNUMBER = 20;&#xD;&#xA;int MIN_MLB1_PICKNUMBER = 7;&#xD;&#xA;int MIN_MLB2_PICKNUMBER = 15;&#xD;&#xA;int MIN_CB1_PICKNUMBER = 4;&#xD;&#xA;int MIN_CB2_PICKNUMBER = 9;&#xD;&#xA;int MIN_S1_PICKNUMBER = 6;&#xD;&#xA;int MIN_S2_PICKNUMBER = 28;&#xD;&#xA;&#xD;&#xA;//Position Specific Pick Thresholds (CPU WILL DEVALUE SELECTING A PLAYER AT THIS POSITION DEPTH UNTIL A MINIMUM PICK TO IMPROVE CPU PICK QUALITY)&#xD;&#xA;//Quarterbacks including M21 CD2 logic&#xD;&#xA;if(pick != null &amp;&amp; draftPlayer != null)&#xD;&#xA;{&#xD;&#xA;    PositionE position = draftPlayer.Position;&#xD;&#xA;    if (position ==  PositionE.QB)&#xD;&#xA;    {&#xD;&#xA;        // If this is the round of the draft there is special logic we want to check before drafting a QB&#xD;&#xA;        if(pick.Round == FIRST_ROUND)&#xD;&#xA;        {&#xD;&#xA;            if(depthPlayer != null)&#xD;&#xA;            {&#xD;&#xA;                TraitDevelopment depthPlayerTrait = depthPlayer.TraitDevelopment;&#xD;&#xA;                int depthPlayerAge = depthPlayer.Age;&#xD;&#xA;                int depthPlayerDraftRound = depthPlayer.PLYR_DRAFTROUND;&#xD;&#xA;                int depthPlayerOvr = depthPlayer.GetBestOverallGrade();&#xD;&#xA;                int depthYearsPro = depthPlayer.YearsPro;&#xD;&#xA;                // We are looking at our current starting QB and he was drafted in last two seasons&#xD;&#xA;                if(depth == STARTING_POSITION)&#xD;&#xA;                {&#xD;&#xA;                    if(depthPlayer.YearDrafted == PREVIOUS_DRAFT_YEAR || depthPlayer.YearDrafted == PREVIOUS_DRAFT_YEAR_2)&#xD;&#xA;                    {&#xD;&#xA;                        // Do not value first round QB if team selected a first round QB in either of the previous two seasons&#xD;&#xA;                        if(depthPlayerDraftRound == FIRST_ROUND)&#xD;&#xA;                        {&#xD;&#xA;                            hasValue = false;&#xD;&#xA;                        }&#xD;&#xA;                        else if(depthPlayerDraftRound == SECOND_ROUND || depthPlayerDraftRound == THIRD_ROUND)&#xD;&#xA;                        {&#xD;&#xA;                            if(depthPlayerTrait == TraitDevelopment.Normal)&#xD;&#xA;                            {&#xD;&#xA;                                // Do not value first round QB if starting QB was drafted in last two years in 2nd/3rd round, is Normal Dev AND over 71+ OVR, unless picking in Top 5&#xD;&#xA;                                if(depthPlayerOvr &gt;= MIN_NORMAL_QB_OVR)&#xD;&#xA;                                {&#xD;&#xA;                                    hasValue = (pick.PickNumber &lt;= 5);&#xD;&#xA;                                }&#xD;&#xA;                            }&#xD;&#xA;                            // Do not value first round QB if starting QB was drafted in last two years in 2nd/3rd round and has a Star, Superstar, or X-Factor Dev Trait, unless picking in Top 5&#xD;&#xA;                            else if(depthPlayerTrait != TraitDevelopment.Normal)&#xD;&#xA;                            {&#xD;&#xA;                                hasValue = (pick.PickNumber &lt;= 5);&#xD;&#xA;                            }&#xD;&#xA;                        }&#xD;&#xA;                    }&#xD;&#xA;          &#xD;&#xA;                    // Don't value a first round QB if current starting QB is 85+ OVR and under 35&#xD;&#xA;                    else if(depthPlayerOvr &gt;= MIN_GREAT_QB_OVR &amp;&amp; depthPlayerAge &lt; MIN_SAFE_QB_AGE)&#xD;&#xA;                    {&#xD;&#xA;                        hasValue = false;&#xD;&#xA;                    }&#xD;&#xA;                    // Don't value a first round QB if current starting QB is 27 or younger AND 73+ OVR with good Dev&#xD;&#xA;                    else if(depthPlayerAge &lt;= MIN_SAFE_QB_AGE &amp;&amp; depthPlayerOvr &gt;= MIN_SAFE_QB_OVR &amp;&amp; depthPlayerTrait != TraitDevelopment.Normal)&#xD;&#xA;                    {&#xD;&#xA;                        hasValue = false;&#xD;&#xA;                    }&#xD;&#xA;&#xD;&#xA;                    // Consider a first round QB if current starting QB is 27 or older AND 73 OVR with Normal Dev if in Top 10&#xD;&#xA;                    else if(depthPlayerAge &lt;= MIN_SAFE_QB_AGE &amp;&amp; depthPlayerOvr &gt;= MIN_SAFE_QB_OVR &amp;&amp; depthPlayerTrait == TraitDevelopment.Normal)&#xD;&#xA;                    {&#xD;&#xA;                        hasValue = (pick.PickNumber &lt;= 10);&#xD;&#xA;                    }&#xD;&#xA;&#xD;&#xA;                    // Do value a first round QB if current starting QB is 35+ years old&#xD;&#xA;                    else if(depth == STARTING_POSITION &amp;&amp; depthPlayerAge &gt;= MIN_OLD_QB_AGE)&#xD;&#xA;                    {&#xD;&#xA;                        hasValue = true;&#xD;&#xA;                    }&#xD;&#xA;&#xD;&#xA;                    else // all other depths&#xD;&#xA;                    {&#xD;&#xA;                    // Before we draft a backup QB we want to make sure we are past the exclude pick&#xD;&#xA;                        hasValue = (pick.PickNumber &gt;= MIN_QB2_PICKNUMBER);&#xD;&#xA;                    }&#xD;&#xA;                }&#xD;&#xA;&#xD;&#xA;                // Don't value a first round QB if a backup QB is 28 or younger and has a Star, Superstar, or X-Factor Dev Trait&#xD;&#xA;                else if(depth == BACKUP_POSITION &amp;&amp; depthPlayerAge &lt;= MIN_BACKUP_QB_AGE &amp;&amp; depthPlayerTrait != TraitDevelopment.Normal)&#xD;&#xA;                {&#xD;&#xA;                    hasValue = false;&#xD;&#xA;                }&#xD;&#xA;            }&#xD;&#xA;            else if(depth == STARTING_POSITION &amp;&amp; depthPlayer == null)&#xD;&#xA;            {&#xD;&#xA;               hasValue = true;&#xD;&#xA;            }&#xD;&#xA;            else // all other depths&#xD;&#xA;            {&#xD;&#xA;                // Before we draft a backup QB we want to make sure we are past the exclude pick&#xD;&#xA;                hasValue = (pick.PickNumber &gt;= MIN_QB2_PICKNUMBER);&#xD;&#xA;            }&#xD;&#xA;        }&#xD;&#xA;        else // all other rounds&#xD;&#xA;        {&#xD;&#xA;            // Before we draft a backup QB we want to make sure we are past the exclude pick&#xD;&#xA;            hasValue = (pick.PickNumber &gt;= MIN_QB2_PICKNUMBER);&#xD;&#xA;        }&#xD;&#xA;    } // end QB&#xD;&#xA;    else if (position == PositionE.HB)&#xD;&#xA;    {&#xD;&#xA;          if(depth == STARTING_POSITION)&#xD;&#xA;          {&#xD;&#xA;            hasValue = (pick.PickNumber &gt;= MIN_HB1_PICKNUMBER);&#xD;&#xA;          }&#xD;&#xA;          else if(depth == BACKUP_POSITION)&#xD;&#xA;          {&#xD;&#xA;            hasValue = (pick.PickNumber &gt;= MIN_HB2_PICKNUMBER);&#xD;&#xA;          }&#xD;&#xA;          else&#xD;&#xA;          {&#xD;&#xA;            hasValue = (pick.PickNumber &gt;= MIN_HB1_PICKNUMBER);&#xD;&#xA;          }&#xD;&#xA;    } // end HB&#xD;&#xA;    else if (position == PositionE.WR)&#xD;&#xA;    {&#xD;&#xA;      if(depth == STARTING_POSITION)&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_WR1_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;      else&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_WR2_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;    } // end WR&#xD;&#xA;    else if (position == PositionE.TE)&#xD;&#xA;    {&#xD;&#xA;      if(depth == STARTING_POSITION)&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_TE1_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;      else if(depth == BACKUP_POSITION)&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_TE2_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;      else&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_TE1_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;    } // end TE&#xD;&#xA;    else if (position == PositionE.LT)&#xD;&#xA;    {&#xD;&#xA;      if(depth == BACKUP_POSITION)&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_LRT2_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;      // else hasValue = true;&#xD;&#xA;    } // end LT&#xD;&#xA;    else if (position == PositionE.RT)&#xD;&#xA;    {&#xD;&#xA;      if(depth == STARTING_POSITION)&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_RT1_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;      else if(depth == BACKUP_POSITION)&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_LRT2_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;      else&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_RT1_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;    } // end RT&#xD;&#xA;    else if (position == PositionE.LG || position == PositionE.RG)&#xD;&#xA;    {&#xD;&#xA;      if(depth == STARTING_POSITION)&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_LRG1_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;      else if(depth == BACKUP_POSITION)&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_IOL2_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;      else&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_LRG1_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;    } // end LG/RG&#xD;&#xA;    else if (position == PositionE.C)&#xD;&#xA;    {&#xD;&#xA;      if(depth == STARTING_POSITION)&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_C1_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;      else if(depth == BACKUP_POSITION)&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_IOL2_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;      else&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_C1_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;    } // end C&#xD;&#xA;    else if (position == PositionE.LE || position == PositionE.RE)&#xD;&#xA;    {&#xD;&#xA;      if(depth == BACKUP_POSITION)&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_DE2_PICKNUMBER);&#xD;&#xA;      }&#xD;&#xA;      // else hasValue = true;&#xD;&#xA;    } // end LE/RE&#xD;&#xA;    else if (position == PositionE.DT)&#xD;&#xA;    {&#xD;&#xA;      if(depth == STARTING_POSITION)&#xD;&#xA;      {&#xD;&#xA;        hasValue = (pick.PickNumber &gt;= MIN_DT1_PICKNUMBER);&#xD;&#xA;      }