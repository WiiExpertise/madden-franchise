            <attribute name="ReleaseCoach" idx="24" type="TeamManager_ReleaseCoach" guid="5307-46450" final="true" default="// Zero out the coach's contract&#xD;&#xA;&#xD;&#xA;Coach nullCoach = null;&#xD;&#xA;Coach coach = nullCoach;&#xD;&#xA;&#xD;&#xA;if(coachPosition == CoachPosition.HeadCoach)&#xD;&#xA;{&#xD;&#xA;  coach = team.HeadCoach;&#xD;&#xA;}&#xD;&#xA;else if(coachPosition == CoachPosition.OffensiveCoordinator)&#xD;&#xA;{&#xD;&#xA;  coach = team.OffensiveCoordinator;&#xD;&#xA;}&#xD;&#xA;else if(coachPosition == CoachPosition.DefensiveCoordinator)&#xD;&#xA;{&#xD;&#xA;  coach = team.DefensiveCoordinator;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if (coach != nullCoach)&#xD;&#xA;{&#xD;&#xA;    coach.ContractYearsRemaining = 0;&#xD;&#xA;&#xD;&#xA;    // Remove the coach from the team.&#xD;&#xA;    if(coach.Position == CoachPosition.HeadCoach)&#xD;&#xA;    {&#xD;&#xA;        // Remove coach from the old team.&#xD;&#xA;        team.HeadCoach = null;&#xD;&#xA;    }&#xD;&#xA;    else if(coach.Position == CoachPosition.OffensiveCoordinator)&#xD;&#xA;    {&#xD;&#xA;        // Remove OC from the old team.&#xD;&#xA;        team.OffensiveCoordinator = null;&#xD;&#xA;    }&#xD;&#xA;    else if(coach.Position == CoachPosition.DefensiveCoordinator)&#xD;&#xA;    {&#xD;&#xA;        // Remove DC from the old team.&#xD;&#xA;        team.DefensiveCoordinator = null;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Add the coach to the free agent pool&#xD;&#xA;    Franchise.FreeAgentCoaches.Append(coach);&#xD;&#xA;&#xD;&#xA;    // Update the coach's contract status.&#xD;&#xA;    Team newTeam = null;&#xD;&#xA;    CoachManager.UpdateCoachContractStatus( coach, team, coach.Position, newTeam, StaffPersonContractStatus.FreeAgent);&#xD;&#xA;&#xD;&#xA;    // Add the coach to the team's coach blacklist&#xD;&#xA;    team.StaffPersonBlacklist.Append(coach);&#xD;&#xA;&#xD;&#xA;    // send the notification&#xD;&#xA;    StaffPersonContractOffer contract = new StaffPersonContractOffer;&#xD;&#xA;    contract.StaffPerson = coach;&#xD;&#xA;    contract.Length = 0;&#xD;&#xA;    contract.Salary = 0;&#xD;&#xA;    contract.Team = null;&#xD;&#xA;&#xD;&#xA;    StaffEventType event = StaffEventType.Fired;&#xD;&#xA;    IssueStaffEventNotificationRequest(contract, event, coach);&#xD;&#xA;}&#xD;&#xA;" />
            <attribute name="ReleasePlayer" idx="25" type="TeamManager_ReleasePlayer" guid="5307-28103" final="true" default="// Only apply cap penalties if the player is not demanding a release. If he demands a release,&#xD;&#xA;// he does not get his remaining signing bonus.&#xD;&#xA;bool applyPenalty = !player.IsDemandRelease;&#xD;&#xA;&#xD;&#xA;PlayerContractStatus oldStatus = player.ContractStatus;&#xD;&#xA;&#xD;&#xA;// Remove the player from the team.&#xD;&#xA;RemovePlayer(team, player, applyPenalty);&#xD;&#xA;&#xD;&#xA;// Update the player's contract status.&#xD;&#xA;Team newTeam = null;&#xD;&#xA;PlayerManager.UpdatePlayerContractStatus(player, team, newTeam, player.ContractStatus, PlayerContractStatus.FreeAgent, false, isDeadWeightCut);&#xD;&#xA;&#xD;&#xA;// Clear the player's contract data.&#xD;&#xA;player.ContractLength = 0;&#xD;&#xA;player.ContractYear = 0;&#xD;&#xA;player.ContractSalary0 = 0;&#xD;&#xA;player.ContractSalary1 = 0;&#xD;&#xA;player.ContractSalary2 = 0;&#xD;&#xA;player.ContractSalary3 = 0;&#xD;&#xA;player.ContractSalary4 = 0;&#xD;&#xA;player.ContractSalary5 = 0;&#xD;&#xA;player.ContractSalary6 = 0;&#xD;&#xA;player.ContractSalary7 = 0;&#xD;&#xA;player.ContractBonus0 = 0;&#xD;&#xA;player.ContractBonus1 = 0;&#xD;&#xA;player.ContractBonus2 = 0;&#xD;&#xA;player.ContractBonus3 = 0;&#xD;&#xA;player.ContractBonus4 = 0;&#xD;&#xA;player.ContractBonus5 = 0;&#xD;&#xA;player.ContractBonus6 = 0;&#xD;&#xA;player.ContractBonus7 = 0;&#xD;&#xA;player.ContractExtraYearOption = false;&#xD;&#xA;&#xD;&#xA;// Add the player to the free agent pool.&#xD;&#xA;if (oldStatus != PlayerContractStatus.FreeAgent &amp;&amp; oldStatus != PlayerContractStatus.PracticeSquad)&#xD;&#xA;{&#xD;&#xA;    Franchise.FreeAgents.Append(player);&#xD;&#xA;}&#xD;&#xA;" />
            <attribute name="ReleaseScout" idx="26" type="TeamManager_ReleaseScout" guid="5307-114433" final="true" default="// Zero out the scout's contract&#xD;&#xA;Scout scout = team.HeadScout;&#xD;&#xA;Scout nullScout = null;&#xD;&#xA;&#xD;&#xA;if (scout!= nullScout )&#xD;&#xA;{&#xD;&#xA;    scout.ContractYearsRemaining = 0;&#xD;&#xA;&#xD;&#xA;    // Update the coach's contract status.&#xD;&#xA;    Team newTeam = null;&#xD;&#xA;    CoachManager.UpdateScoutContractStatus( scout, team, newTeam, StaffPersonContractStatus.FreeAgent);&#xD;&#xA;&#xD;&#xA;    // Remove the coach from the team.&#xD;&#xA;    team.HeadScout= null;&#xD;&#xA;&#xD;&#xA;    // Add the scout to the free agent pool&#xD;&#xA;    Franchise.FreeAgentScouts.Append(scout);&#xD;&#xA;&#xD;&#xA;    // Add the scout to the team's staff blacklist&#xD;&#xA;    team.StaffPersonBlacklist.Append(scout);&#xD;&#xA;}" />
            <attribute name="ReleaseTrainer" idx="27" type="TeamManager_ReleaseTrainer" guid="5307-114437" final="true" default="// Zero out the trainer's contract&#xD;&#xA;Trainer trainer = team.HeadTrainer;&#xD;&#xA;Trainer nullTrainer = null;&#xD;&#xA;&#xD;&#xA;if (trainer != nullTrainer )&#xD;&#xA;{&#xD;&#xA;    trainer.ContractYearsRemaining = 0;&#xD;&#xA;&#xD;&#xA;    // Update the coach's contract status.&#xD;&#xA;    Team newTeam = null;&#xD;&#xA;    CoachManager.UpdateTrainerContractStatus( trainer, team, newTeam, StaffPersonContractStatus.FreeAgent);&#xD;&#xA;&#xD;&#xA;    // Remove the coach from the team.&#xD;&#xA;    team.HeadTrainer = null;&#xD;&#xA;&#xD;&#xA;    // Add the coach to the free agent pool&#xD;&#xA;    Franchise.FreeAgentTrainers.Append(trainer);&#xD;&#xA;&#xD;&#xA;    // Add the trainer to the team's staff blacklist&#xD;&#xA;    team.StaffPersonBlacklist.Append(trainer);&#xD;&#xA;}" />
