            <attribute name="AdvancePickTransitionV2" idx="0" type="bool()" guid="5295443-1824171" final="true" default="// If this is the normal draft, use the normal advance functionality.&#xD;&#xA;if (!DraftInfo.IsFantasyDraft)&#xD;&#xA;{&#xD;&#xA;    // If we were simming to the next pick, go back to live.&#xD;&#xA;    if (SimState == DraftSimState.SimToNextPick)&#xD;&#xA;    {&#xD;&#xA;        SetSimState(DraftSimState.Live);&#xD;&#xA;    }&#xD;&#xA;    // If the draft is still active and we're simming to the next round,&#xD;&#xA;    // check if we have reached the next round.&#xD;&#xA;    else if (SimState == DraftSimState.SimToNextRound)&#xD;&#xA;    {&#xD;&#xA;        if (DraftInfo.CurrentRoundPickIndex == DraftManager.GetPicksPerRound() - 1)&#xD;&#xA;        {&#xD;&#xA;            SetSimState(DraftSimState.Live);&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;    // If we were simming to the next user pick, go back to live.&#xD;&#xA;    else if (SimState == DraftSimState.SimToNextUserPick)&#xD;&#xA;    {&#xD;&#xA;        DraftPick nextPick = DraftManager.GetNextPick();&#xD;&#xA;        if (nextPick != null)&#xD;&#xA;        {&#xD;&#xA;            Team team = nextPick.CurrentTeam;&#xD;&#xA;            if (team.UserCharacter != null)&#xD;&#xA;            {&#xD;&#xA;                User user = DraftEval.FindUserByEntity(team.UserCharacter);&#xD;&#xA;              if (user != null)&#xD;&#xA;          {&#xD;&#xA;        if (!user.IsActionTimeoutOn)&#xD;&#xA;              {&#xD;&#xA;                  SetSimState(DraftSimState.Live);&#xD;&#xA;              }&#xD;&#xA;          }&#xD;&#xA;            }&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;// Otherwise, use the fantasy draft functionality.&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;    Team[] teams = Franchise.Teams;&#xD;&#xA;    int teamCount = teams.Population();&#xD;&#xA;    int roundIndex = DraftInfo.CurrentRoundIndex;&#xD;&#xA;    int roundPickIndex = DraftInfo.CurrentRoundPickIndex;&#xD;&#xA;&#xD;&#xA;    // If we were simming to the next pick, go back to live.&#xD;&#xA;    if (SimState == DraftSimState.SimToNextPick)&#xD;&#xA;    {&#xD;&#xA;        SetSimState(DraftSimState.Live);&#xD;&#xA;    }&#xD;&#xA;    // If the draft is still active and we're simming to the next round,&#xD;&#xA;    // check if we have reached the next round.&#xD;&#xA;    else if (SimState == DraftSimState.SimToNextRound)&#xD;&#xA;    {&#xD;&#xA;        if (roundPickIndex + 1 &gt;= teamCount)&#xD;&#xA;        {&#xD;&#xA;            SetSimState(DraftSimState.Live);&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;    // If we were simming to the next user pick, go back to live.&#xD;&#xA;    else if (SimState == DraftSimState.SimToNextUserPick)&#xD;&#xA;    {&#xD;&#xA;        Team team = FantasyDraft.GetNextTeam(roundIndex, roundPickIndex, teams);&#xD;&#xA;        if (team.UserCharacter != null)&#xD;&#xA;        {&#xD;&#xA;            User user = DraftEval.FindUserByEntity(team.UserCharacter);&#xD;&#xA;            if (user != null)&#xD;&#xA;            {&#xD;&#xA;                if (!user.IsActionTimeoutOn)&#xD;&#xA;                {&#xD;&#xA;                    SetSimState(DraftSimState.Live);&#xD;&#xA;                }&#xD;&#xA;            }&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Advance the pick.&#xD;&#xA;bool isActive = DraftManager.AdvancePick();&#xD;&#xA;&#xD;&#xA;// Transition if the draft is still active.&#xD;&#xA;isActive;&#xD;&#xA;" />
