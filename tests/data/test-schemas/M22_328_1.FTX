            <attribute name="GetContractOfferFromNegotiation" idx="10" type="ReSignNegotiationManager_GetContractOfferFromNegotiation" guid="11750-1342390" final="true" default="// Return a ContractOffer record from a PlayerReSignNegotiation record.&#xD;&#xA;PlayerContract contract = PlayerContractManager.CreatePlayerContract(reSignNegotiation.ContractSalary, reSignNegotiation.ContractBonus, reSignNegotiation.ContractLength);&#xD;&#xA;&#xD;&#xA;// we need to turn the player contract into a contract offer&#xD;&#xA;ContractOffer contractOffer = new ContractOffer;&#xD;&#xA;contractOffer.Player = reSignNegotiation.Player;&#xD;&#xA;contractOffer.Team = reSignNegotiation.Team;&#xD;&#xA;contractOffer.Contract = contract;&#xD;&#xA;&#xD;&#xA;contractOffer;" />
            <attribute name="HandleTeamResponse" idx="11" type="ReSignNegotiationManager_HandleTeamResponse" guid="11750-52290" final="true" default="bool continueNegotiations = false;&#xD;&#xA;playerReSignNegotiation.ActiveRequestID = -1;&#xD;&#xA;&#xD;&#xA;// If the League does not have owner, do not process any negotiation.&#xD;&#xA;// No-owner leagues are created from binary exporter simulations, where the Weeks are generated processing all the teams like CPU teams.&#xD;&#xA;// Thus, we wanted to avoid any auto negotiation for all the teams.&#xD;&#xA;if (PlayerSigningEval.FranchiseRef.LeagueOwner != null)&#xD;&#xA;{&#xD;&#xA;    if( !playerReSignNegotiation.Team.IsUserManaged() &#xD;&#xA;      || ( playerReSignNegotiation.Team.IsUserManaged() &amp;&amp; &#xD;&#xA;            (  (teamRequest.Response == teamRequest.WithdrawResponse)&#xD;&#xA;            || (teamRequest.Response == teamRequest.MakeOfferResponse)&#xD;&#xA;            || (teamRequest.Response == teamRequest.SubmitResponse)&#xD;&#xA;            || ( ( (teamRequest.Response == teamRequest.DefaultResponse) || (teamRequest.Response == null) ) &amp;&amp; !playerReSignNegotiation.Team.TeamSettingRef.IsCPUReSignPlayersEnabled ) ) ) )&#xD;&#xA;    {&#xD;&#xA;        // is this the initial generated contract&#xD;&#xA;        if(playerReSignNegotiation.InitialContract)&#xD;&#xA;        {&#xD;&#xA;            // inital resign response&#xD;&#xA;            continueNegotiations = ProcessTeamInitialResponse(playerReSignNegotiation, teamRequest);&#xD;&#xA;            playerReSignNegotiation.InitialContract = false;&#xD;&#xA;        }&#xD;&#xA;        else&#xD;&#xA;        {&#xD;&#xA;            // player wait&#xD;&#xA;            continueNegotiations = ProcessTeamResponse(playerReSignNegotiation, teamRequest);&#xD;&#xA;        }&#xD;&#xA;&#xD;&#xA;        // remove the player if the team decided not to ReSign him&#xD;&#xA;        if(continueNegotiations)&#xD;&#xA;        {&#xD;&#xA;            ReSign_PlayerRequest playerRequest = PlayerSigningEval.CreatePlayerRequest( playerReSignNegotiation );&#xD;&#xA;            playerRequest.PlayerReSignNegotiation = playerReSignNegotiation;&#xD;&#xA;            playerRequest.ReSignNegotiationManager = this;&#xD;&#xA;&#xD;&#xA;            playerRequest.Issue(playerReSignNegotiation.Player);&#xD;&#xA;&#xD;&#xA;            playerReSignNegotiation.ActiveRequestID = playerRequest.RequestId;&#xD;&#xA;        }&#xD;&#xA;        else&#xD;&#xA;        {&#xD;&#xA;            playerReSignNegotiation.NegotiationsEnded = true;&#xD;&#xA;            playerReSignNegotiation.ReSignStatus = ReSignStatus.NotInterested;&#xD;&#xA;            playerReSignNegotiation.ActiveRequestID = -1;&#xD;&#xA;            EndReSignTransaction(playerReSignNegotiation);&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;" />
            <attribute name="IncreaseOffer" idx="12" type="ReSignNegotiationManager_IncreaseOffer" guid="11750-61475" final="true" />
            <attribute name="IssuePlayerRequests" idx="13" type="ReSignNegotiationManager_IssuePlayerRequests" guid="11750-52341" final="true" default="if(activeReSignNegotiations.Population() != 0)&#xD;&#xA;{&#xD;&#xA;  UpdateReSignNegotiationRequests updateReSignRequestView = new UpdateReSignNegotiationRequests;&#xD;&#xA;  updateReSignRequestView.PlayerSigningEval = PlayerSigningEval;&#xD;&#xA;  updateReSignRequestView.TeamRequestManager = TeamRequestManager;&#xD;&#xA;   updateReSignRequestView.PlayerGradeEval = PlayerGradeEval;&#xD;&#xA;   updateReSignRequestView.ReSignNegotiationManager = this;&#xD;&#xA;  updateReSignRequestView.Subjects = activeReSignNegotiations;&#xD;&#xA;   updateReSignRequestView.DelegateTeamRequest = true;&#xD;&#xA;  updateReSignRequestView.SendTeamRequests = true;&#xD;&#xA;   updateReSignRequestView.Update();&#xD;&#xA;}" />
            <attribute name="IssueRequests" idx="14" type="ReSignNegotiationManager_IssueRequests" guid="11750-33640" final="true" default="if(activeReSignNegotiations.Population() != 0)&#xD;&#xA;{&#xD;&#xA;  UpdateReSignNegotiationRequests updateReSignRequestView = new UpdateReSignNegotiationRequests;&#xD;&#xA;       updateReSignRequestView.PlayerSigningEval = PlayerSigningEval;&#xD;&#xA;       updateReSignRequestView.TeamRequestManager = TeamRequestManager;&#xD;&#xA;  updateReSignRequestView.PlayerGradeEval = PlayerGradeEval;&#xD;&#xA;  updateReSignRequestView.ReSignNegotiationManager = this;&#xD;&#xA;       updateReSignRequestView.Subjects = activeReSignNegotiations;&#xD;&#xA;  updateReSignRequestView.DelegateTeamRequest = false;&#xD;&#xA;  updateReSignRequestView.SendTeamRequests = true;&#xD;&#xA;       updateReSignRequestView.Update();&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;" />
