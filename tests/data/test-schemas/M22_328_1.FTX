)&#xD;&#xA;{&#xD;&#xA;    popularity = (popularity * 90) / 100;&#xD;&#xA;}&#xD;&#xA;else if (position == PositionE.SS)&#xD;&#xA;{&#xD;&#xA;    popularity = (popularity * 92) / 100;&#xD;&#xA;}&#xD;&#xA;else if (position == PositionE.C || position == PositionE.LT || position == PositionE.RG || position == PositionE.LG || position == PositionE.RT)&#xD;&#xA;{&#xD;&#xA;    popularity = (popularity * 65) / 100;&#xD;&#xA;}&#xD;&#xA;else if(position == PositionE.K || position == PositionE.P)&#xD;&#xA;{&#xD;&#xA; popularity = (popularity * 10) / 100;&#xD;&#xA;}&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA; popularity = (popularity * 10) / 100;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if (popularity &gt; 99)&#xD;&#xA;{&#xD;&#xA;    popularity = 99;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;popularity;" />
