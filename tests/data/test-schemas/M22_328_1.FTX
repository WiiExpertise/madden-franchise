            <attribute name="ProcessAdvanceRequest" idx="26" type="none()" guid="5295443-1824014" final="true" default="// If the advance request has been resolved, set the sim state accordingly.&#xD;&#xA;if (AdvanceRequest.HasResolution())&#xD;&#xA;{&#xD;&#xA;    // Only change SimState if it is not advancing.&#xD;&#xA;    if (SimState == DraftSimState.Live)&#xD;&#xA;    {&#xD;&#xA;        // Set the appropriate sim state.&#xD;&#xA;        if (AdvanceRequest.Response == AdvanceRequest.NextPickResponse)&#xD;&#xA;        {&#xD;&#xA;            SetSimState(DraftSimState.SimToNextPick);&#xD;&#xA;        }&#xD;&#xA;        else if (AdvanceRequest.Response == AdvanceRequest.NextUserPickResponse)&#xD;&#xA;        {&#xD;&#xA;            SetSimState(DraftSimState.SimToNextUserPick);&#xD;&#xA;        }&#xD;&#xA;        else if (AdvanceRequest.Response == AdvanceRequest.NextRoundResponse)&#xD;&#xA;        {&#xD;&#xA;            SetSimState(DraftSimState.SimToNextRound);&#xD;&#xA;        }&#xD;&#xA;        else if (AdvanceRequest.Response == AdvanceRequest.EndDraftResponse)&#xD;&#xA;        {&#xD;&#xA;            SetSimState(DraftSimState.SimToEnd);&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;    // Clear the response.&#xD;&#xA;    AdvanceRequest.Response = null;&#xD;&#xA;}&#xD;&#xA;" />
            <attribute name="ProcessDelegateStage" idx="27" type="Stage" />
            <attribute name="ProcessDelegateStage_EndV2" idx="28" type="none()" guid="5295443-1824147" final="true" default="// Release the trade negotiation.&#xD;&#xA;TradeNegotiation = null;" />
            <attribute name="ProcessDelegateStage_ProcessV2" idx="29" type="none()" guid="5295443-1824144" final="true" default="TeamTradePackage tradePackage = null;&#xD;&#xA;&#xD;&#xA;// If there are trade offers available, enable trade evaluation.&#xD;&#xA;if (TradeNegotiation != null)&#xD;&#xA;{&#xD;&#xA;    if (TradeNegotiation.TeamTradePackageListB != null)&#xD;&#xA;    {&#xD;&#xA;        tradePackage = TradeEval.EvaluateTradeOffers(TradeNegotiation.PackageA, TradeNegotiation.TeamTradePackageListB);&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Select the player to draft.&#xD;&#xA;SelectedPlayer = DraftEval.SelectPlayerToDraft(&#xD;&#xA;    DraftInfo.CurrentTeam,&#xD;&#xA;    DraftInfo.CurrentPick,&#xD;&#xA;    DraftInfo.IsFantasyDraft,&#xD;&#xA;    tradePackage);&#xD;&#xA;&#xD;&#xA;// If the trade negotiation is valid, set the trade response.&#xD;&#xA;if (TradeNegotiation != null)&#xD;&#xA;{&#xD;&#xA;    // If no player was selected, attempt to accept the best trade offer.&#xD;&#xA;    if (SelectedPlayer == null)&#xD;&#xA;    {&#xD;&#xA;        TradeNegotiation.PackageB = tradePackage;&#xD;&#xA;        TradeNegotiation.Response = TradeResponse.Accept;&#xD;&#xA;    }&#xD;&#xA;    // Otherwise, decline all trade offers.&#xD;&#xA;    else&#xD;&#xA;    {&#xD;&#xA;        TradeNegotiation.PackageB = null;&#xD;&#xA;        TradeNegotiation.Response = TradeResponse.Decline;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // End the trade negotiation.&#xD;&#xA;    IsDraftPickTraded = TradeNegotiationManager.EndTradeNegotiation(TradeNegotiation);&#xD;&#xA;}&#xD;&#xA;// Otherwise, the pick was not traded.&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;    IsDraftPickTraded = false;&#xD;&#xA;}&#xD;&#xA;" />
            <attribute name="ProcessDelegateStage_StartV2" idx="30" type="none()" guid="5295443-1824141" final="true" default="// can't be empty&#xD;&#xA;int noop=1;" />
            <attribute name="ProcessDraftStage_EndV2" idx="31" type="none()" guid="5295443-1824138" final="true" default="// Once the active team has made a decision, clear all pending requests. This must happen&#xD;&#xA;// in order to clear out the pending pick request, trade requests, and the advance request.&#xD;&#xA;if(!DraftInfo.IsDisablePicks)&#xD;&#xA;{&#xD;&#xA;    RequestServerRef.ClearRequests();&#xD;&#xA;}&#xD;&#xA;// Clear the request to prevent dangling references.&#xD;&#xA;DraftEval.ClearDraftPickRequest(Request);&#xD;&#xA;&#xD;&#xA;// Clear the pause/resume request.&#xD;&#xA;PauseResumeRequest = null;&#xD;&#xA;&#xD;&#xA;if (DraftInfo.CurrentRoundPickIndex == 0)&#xD;&#xA;{&#xD;&#xA;    Sleep(0.5);&#xD;&#xA;}&#xD;&#xA;" />
            <attribute name="ProcessDraftStage_ProcessV2" idx="32" type="none()" guid="5295443-1824135" final="true" default="// Process the pause/resume request.&#xD;&#xA;ProcessPauseResumeRequest();&#xD;&#xA;&#xD;&#xA;// Process the advance request.&#xD;&#xA;ProcessAdvanceRequest();&#xD;&#xA;&#xD;&#xA;// If the draft pick request is pending, see if it should be auto-dismissed.&#xD;&#xA;if (Request.IsResolutionPending())&#xD;&#xA;{&#xD;&#xA;    // If this is not the live draft, dismiss the request immediately.&#xD;&#xA;    if (SimState != DraftSimState.Live)&#xD;&#xA;    {&#xD;&#xA;        Request.Dismiss();&#xD;&#xA;    }&#xD;&#xA;    // If a trade negotiation is active and all responses are in, reduce the CPU response time.&#xD;&#xA;    else if (TradeNegotiation != null)&#xD;&#xA;    {&#xD;&#xA;        if (TradeNegotiation.PendingTeamListB.Population() == 0)&#xD;&#xA;        {&#xD;&#xA;            Request.CpuResponseDuration = 20;&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;    // If this is the mock draft, set the CPU response time to select quickly.&#xD;&#xA;    else if (DraftInfo.IsFantasyDraft)&#xD;&#xA;    {&#xD;&#xA;        Request.CpuResponseDuration = 1;&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// If the draft pick request is still pending, sleep to avoid busy waiting.&#xD;&#xA;if (Request.IsResolutionPending())&#xD;&#xA;{&#xD;&#xA;    Sleep(1.0);&#xD;&#xA;}&#xD;&#xA;// If the draft is paused, sleep to avoid busy waiting.&#xD;&#xA;else if (DraftInfo.IsPaused &amp;&amp; DraftInfo.IsLive)&#xD;&#xA;{&#xD;&#xA;    Sleep(1.0);&#xD;&#xA;}&#xD;&#xA;" />
            <attribute name="ProcessDraftStage_StartV2" idx="33" type="none()" guid="5295443-1824132" final="true" default="int teamCount = Franchise.Teams.Population();&#xD;&#xA;&#xD;&#xA;// Create the draft pick request for the current team.&#xD;&#xA;Request = DraftEval.InitDraftPickRequest(DraftInfo.CurrentTeam, DraftInfo.CurrentPick, Request, DraftInfo.CurrentPickIndex);&#xD;&#xA;&#xD;&#xA;// If the draft pick was trade, reduce CPU response time.&#xD;&#xA;if (IsDraftPickTraded)&#xD;&#xA;{&#xD;&#xA;    Request.CpuResponseMinDuration = 10;&#xD;&#xA;    Request.CpuResponseMaxDuration = 10;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Issue the draft pick request.&#xD;&#xA;TeamRequestManager.Issue(DraftInfo.CurrentTeam, Request);&#xD;&#xA;&#xD;&#xA;// If the pick was not already traded, begin a trade negotiation for this pick.&#xD;&#xA;if (!DraftInfo.IsFantasyDraft &amp;&amp; !IsDraftPickTraded &amp;&amp; !DraftInfo.IsDisableTrading)&#xD;&#xA;{&#xD;&#xA;    TradeNegotiation = TradeNegotiationManager.BeginDraftPickTradeNegotiation(DraftInfo.CurrentTeam, DraftInfo.CurrentPick);&#xD;&#xA;}&#xD;&#xA;// Otherwise, clear the pick traded flag.&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;    IsDraftPickTraded = false;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// If this is the fantasy draft, determine the sim state.&#xD;&#xA;if (DraftInfo.IsFantasyDraft)&#xD;&#xA;{&#xD;&#xA;    if ((Request.TargetUser == null) &amp;&amp; (SimState == DraftSimState.Live))&#xD;&#xA;    {&#xD;&#xA;        SetSimState(DraftSimState.SimToNextPick);&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// If the draft is live, issue all draft requests.&#xD;&#xA;if (SimState == DraftSimState.Live)&#xD;&#xA;{&#xD;&#xA;    // Issue the draft board requests to all teams that aren't on the clock.&#xD;&#xA;    IssueTeamDraftBoardRequestsView.Subjects = Franchise.Teams;&#xD;&#xA;    IssueTeamDraftBoardRequestsView.CurrentTeam = DraftInfo.CurrentTeam;&#xD;&#xA;    IssueTeamDraftBoardRequestsView.Update();&#xD;&#xA;&#xD;&#xA;    // Issue the advance request.&#xD;&#xA;    AdvanceRequest.Issue(Franchise.LeagueOwner);&#xD;&#xA;&#xD;&#xA;    // Issue the pause/resume requests.&#xD;&#xA;    if (!DraftInfo.IsPaused)&#xD;&#xA;    {&#xD;&#xA;        PauseResumeRequest = PauseRequest;&#xD;&#xA;    }&#xD;&#xA;    else&#xD;&#xA;    {&#xD;&#xA;        PauseResumeRequest = ResumeRequest;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    PauseResumeRequest.Issue(Franchise.LeagueOwner);&#xD;&#xA;    UserRequestIssuer.IssueUserRequests();&#xD;&#xA;}&#xD;&#xA;// Otherwise, dismiss all requests immediately.&#xD;&#xA;else if(!DraftInfo.IsDisablePicks)&#xD;&#xA;{&#xD;&#xA;    RequestServerRef.ClearRequests();&#xD;&#xA;}&#xD;&#xA;" />
            <attribute name="ProcessPauseResumeRequest" idx="34" type="none()" guid="5295443-1824011" final="true" default="// If the pause/resume request has been resolved, set the pause/resume state accordingly.&#xD;&#xA;if (PauseResumeRequest != null)&#xD;&#xA;{&#xD;&#xA;    if (PauseResumeRequest.HasResolution())&#xD;&#xA;    {&#xD;&#xA;        // If pause was selected, pause and send the resume request.&#xD;&#xA;        if (PauseResumeRequest == PauseRequest)&#xD;&#xA;        {&#xD;&#xA;            DraftManager.PauseDraft();&#xD;&#xA;            IsBlocking = true;&#xD;&#xA;&#xD;&#xA;            PauseResumeRequest = ResumeRequest;&#xD;&#xA;            PauseResumeRequest.Issue(Franchise.LeagueOwner);&#xD;&#xA;        }&#xD;&#xA;        // If resume was selected, resume and send the pause request.&#xD;&#xA;        else if (PauseResumeRequest == ResumeRequest)&#xD;&#xA;        {&#xD;&#xA;            DraftManager.ResumeDraft();&#xD;&#xA;&#xD;&#xA;            // If the simulation state is live, resume the live draft.&#xD;&#xA;            if (SimState == DraftSimState.Live)&#xD;&#xA;            {&#xD;&#xA;                PauseResumeRequest = PauseRequest;&#xD;&#xA;                PauseResumeRequest.Issue(Franchise.LeagueOwner);&#xD;&#xA;                IsBlocking = false;&#xD;&#xA;            }&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;" />
