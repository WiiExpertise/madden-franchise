            <attribute name="IsTeamInterestedInRebuilding" idx="38" type="TeamRelocationEval_IsTeamInterestedInRebuilding" guid="28067-114812" final="true" default="bool isRebuilding = false;&#xD;&#xA;&#xD;&#xA;if(currentStadiumRating &lt; 91)&#xD;&#xA;{&#xD;&#xA; if(RandomRef.GetRandomIntRange(0, 100) &lt; 5)&#xD;&#xA; {&#xD;&#xA;  isRebuilding = true;&#xD;&#xA; }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if(team.PresentationId == 20)&#xD;&#xA;{&#xD;&#xA; isRebuilding = false;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if(team.PresentationId == 17 &amp;&amp; SeasonInfo.CurrentSeasonYear == 2013)&#xD;&#xA;{&#xD;&#xA; isRebuilding = false;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if(currentStadiumRating &lt; 90 &amp;&amp; team.PresentationId == 14)&#xD;&#xA;{&#xD;&#xA; if(RandomRef.GetRandomIntRange(0, 100) &lt; 20)&#xD;&#xA; {&#xD;&#xA;  isRebuilding = true;&#xD;&#xA; }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if(currentStadiumRating &lt; 200 &amp;&amp; team.PresentationId == 31 &amp;&amp; SeasonInfo.CurrentSeasonYear == 2013)&#xD;&#xA;{&#xD;&#xA; if(RandomRef.GetRandomIntRange(0, 100) &lt; 99)&#xD;&#xA; {&#xD;&#xA;  isRebuilding = true;&#xD;&#xA; }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if(team.PresentationId == 23 &amp;&amp; SeasonInfo.CurrentSeasonYear &lt; 2015)&#xD;&#xA;{&#xD;&#xA; isRebuilding = false;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if(team.PresentationId == 24 &amp;&amp; SeasonInfo.CurrentSeasonYear &lt; 2015)&#xD;&#xA;{&#xD;&#xA; isRebuilding = false;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;isRebuilding;" />
            <attribute name="IsTeamInterestedInRelocating" idx="39" type="TeamRelocationEval_IsTeamInterestedInRelocating" guid="28067-114819" final="true" default="bool isRelocating = false;&#xD;&#xA;&#xD;&#xA;//Make the chargers likely to move&#xD;&#xA;if (SeasonInfo.CurrentYear == 1)&#xD;&#xA;{&#xD;&#xA;  if (team.PresentationId == 8)&#xD;&#xA;   { &#xD;&#xA;    if (RandomRef.GetRandomIntRange(0,100) &lt; 25) //25% chance to move here&#xD;&#xA;     {&#xD;&#xA;      isRelocating = true;&#xD;&#xA;     }&#xD;&#xA;   }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;//If it's after year two,  the team's stadium is poor and they're unpopular move the team rarely&#xD;&#xA;if (SeasonInfo.CurrentYear &gt; 2 &amp;&amp; currentStadiumRating &lt; 120 &amp;&amp; team.OverallPopularity &lt;= 35)&#xD;&#xA;{&#xD;&#xA; if (RandomRef.GetRandomIntRange(0,100) &lt; 1)&#xD;&#xA; {&#xD;&#xA;  isRelocating = true;&#xD;&#xA; }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Add exceptions after year one for Raiders and Chargers &#xD;&#xA;// Removed Vikings from the list since they are getting a new stadium next year&#xD;&#xA;// Add special case for Bills as well&#xD;&#xA;if (SeasonInfo.CurrentYear &gt; 2)&#xD;&#xA;{&#xD;&#xA; &#xD;&#xA; if (currentStadiumRating &lt; 120 &amp;&amp; team.OverallPopularity &lt;= 50)&#xD;&#xA; {&#xD;&#xA;  if (team.PresentationId == 23)&#xD;&#xA;   { &#xD;&#xA;    if (RandomRef.GetRandomIntRange(0,100) &lt; 1)&#xD;&#xA;     {&#xD;&#xA;      isRelocating = true;&#xD;&#xA;     }&#xD;&#xA;   }&#xD;&#xA; }&#xD;&#xA; if (currentStadiumRating &lt; 100 &amp;&amp; team.OverallPopularity &lt;= 75)&#xD;&#xA; {&#xD;&#xA;  if (team.PresentationId == 3)&#xD;&#xA;   {&#xD;&#xA;    if (RandomRef.GetRandomIntRange (0,100) &lt; 1)&#xD;&#xA;     {&#xD;&#xA;      isRelocating = true;&#xD;&#xA;     }&#xD;&#xA;   }&#xD;&#xA; }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;//Unless it's one of the following teams&#xD;&#xA;if(team.PresentationId == 31 || &#xD;&#xA;team.PresentationId == 20 || &#xD;&#xA;team.PresentationId == 12 || &#xD;&#xA;team.PresentationId == 15 || &#xD;&#xA;team.PresentationId == 11 || &#xD;&#xA;team.PresentationId == 29 || &#xD;&#xA;team.PresentationId == 22 || &#xD;&#xA;team.PresentationId == 21 || &#xD;&#xA;team.PresentationId == 14 || &#xD;&#xA;team.PresentationId == 22 || &#xD;&#xA;team.PresentationId == 1 || &#xD;&#xA;team.PresentationId == 4 || &#xD;&#xA;team.PresentationId == 10 || &#xD;&#xA;team.PresentationId == 13 || &#xD;&#xA;team.PresentationId == 16 || &#xD;&#xA;team.PresentationId == 19 || &#xD;&#xA;team.PresentationId == 25 || &#xD;&#xA;team.PresentationId == 26 || &#xD;&#xA;team.PresentationId == 24 || &#xD;&#xA;team.PresentationId == 32)&#xD;&#xA;{&#xD;&#xA;   isRelocating = false;&#xD;&#xA;}&#xD;&#xA;isRelocating;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;" />
