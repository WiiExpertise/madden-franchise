            <attribute name="RemovePlayer" idx="30" type="TeamManager_RemovePlayer" guid="5307-28120" final="true" default="// If the player has been drafted but not signed, remove him from the drafted players &#xD;&#xA;// array.&#xD;&#xA;if (player.ContractStatus == PlayerContractStatus.Drafted)&#xD;&#xA;{&#xD;&#xA;    team.DraftedPlayers.FindAndErase(player) ;&#xD;&#xA;}&#xD;&#xA;// If the player was on a practice squad, remove him from the team's practice squad array.&#xD;&#xA;else if (player.ContractStatus == PlayerContractStatus.PracticeSquad)&#xD;&#xA;{&#xD;&#xA;    team.PracticeSquad.FindAndErase(player);&#xD;&#xA;    player.IsDemandRelease = false;&#xD;&#xA;    player.IsInjuredReserve = false;&#xD;&#xA;}&#xD;&#xA;// Otherwise, remove the player from the team roster.&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;    team.Roster.FindAndErase(player);&#xD;&#xA;&#xD;&#xA;    int trainingPlayerIndex = team.FocusTrainingList.Find(player);&#xD;&#xA;    if(trainingPlayerIndex != -1)&#xD;&#xA;    {&#xD;&#xA;        team.FocusTrainingList.Erase(trainingPlayerIndex );&#xD;&#xA;    }&#xD;&#xA; &#xD;&#xA;    // If requested for a non-expiring contract, apply the appropriate cap penalty.&#xD;&#xA;    if (applyPenalty &amp;&amp; (player.ContractStatus != PlayerContractStatus.Expiring))&#xD;&#xA;    {&#xD;&#xA;        SalaryCapManagerRef.ApplyReleasePenalties(team, player);&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Subtract the player from the active roster size. An injured reserved player is not counted in the roster size.&#xD;&#xA;    if(!player.IsInjuredReserve)&#xD;&#xA;    {&#xD;&#xA;        team.ActiveRosterSize = team.ActiveRosterSize - 1;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Clear team-specific flags.&#xD;&#xA;    player.IsDemandRelease = false;&#xD;&#xA;    player.IsInjuredReserve = false;&#xD;&#xA;}" />
            <attribute name="RemovePlayerFromInjuredReserve" idx="31" type="TeamManager_RemovePlayerFromInjuredReserve" guid="5307-61270" final="true" default="if(player.IsInjuredReserve == true)&#xD;&#xA;{&#xD;&#xA; player.IsInjuredReserve = false;&#xD;&#xA; team.ActiveRosterSize = team.ActiveRosterSize + 1;&#xD;&#xA;&#xD;&#xA; if (decrementIRRemovalCount &amp;&amp; (SeasonInfoRef.CurrentStage != SeasonStage.OffSeason))&#xD;&#xA; {&#xD;&#xA;  team.IRRemovalCount = team.IRRemovalCount - 1;&#xD;&#xA; }&#xD;&#xA;}" />
            <attribute name="RemovePlayerFromPracticeSquad" idx="32" type="TeamManager_RemovePlayerFromPracticeSquad" guid="5307-1342527" final="true" default="if (player.ContractStatus == PlayerContractStatus.PracticeSquad)&#xD;&#xA;{&#xD;&#xA;    RemovePlayer(team, player, false);&#xD;&#xA;    player.ContractStatus = PlayerContractStatus.FreeAgent;&#xD;&#xA;    Team nullTeam = null;&#xD;&#xA;    &#xD;&#xA;    PlayerManager.UpdatePlayerContractStatus(player, team, nullTeam, PlayerContractStatus.PracticeSquad, PlayerContractStatus.FreeAgent, false, false);&#xD;&#xA;}" />
            <attribute name="ReSignPlayer" idx="33" type="TeamManager_ReSignPlayer" guid="5307-28091" final="true" default="// If the player's contract is expiring, create an entirely new contract.&#xD;&#xA;if (player.ContractStatus == PlayerContractStatus.Expiring)&#xD;&#xA;{&#xD;&#xA;    // Assign the player contract data.&#xD;&#xA;    player.ContractLength = contract.Length;&#xD;&#xA;    player.ContractYear = contract.CurrentYear;&#xD;&#xA;    player.ContractExtraYearOption = contract.ExtraYearOption; &#xD;&#xA;&#xD;&#xA;    // Assign the player salary.&#xD;&#xA;    player.ContractSalary0 = contract.SalaryTable[0];&#xD;&#xA;    player.ContractSalary1 = contract.SalaryTable[1];&#xD;&#xA;    player.ContractSalary2 = contract.SalaryTable[2];&#xD;&#xA;    player.ContractSalary3 = contract.SalaryTable[3];&#xD;&#xA;    player.ContractSalary4 = contract.SalaryTable[4];&#xD;&#xA;    player.ContractSalary5 = contract.SalaryTable[5];&#xD;&#xA;    player.ContractSalary6 = contract.SalaryTable[6];&#xD;&#xA;    player.ContractSalary7 = 0;&#xD;&#xA;&#xD;&#xA;    // Assign the player bonus.&#xD;&#xA;    player.ContractBonus0 = contract.BonusTable[0];&#xD;&#xA;    player.ContractBonus1 = contract.BonusTable[1];&#xD;&#xA;    player.ContractBonus2 = contract.BonusTable[2];&#xD;&#xA;    player.ContractBonus3 = contract.BonusTable[3];&#xD;&#xA;    player.ContractBonus4 = contract.BonusTable[4];&#xD;&#xA;    player.ContractBonus5 = contract.BonusTable[5];&#xD;&#xA;    player.ContractBonus6 = contract.BonusTable[6];&#xD;&#xA;    player.ContractBonus7 = 0;&#xD;&#xA;}&#xD;&#xA;// Otherwise, use the current year as the first year of his new contract.&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;    int currentYear = player.ContractYear;&#xD;&#xA;&#xD;&#xA;    // Assign the player contract data.&#xD;&#xA;    player.ContractLength = contract.Length + 1;&#xD;&#xA;    player.ContractYear = 0;&#xD;&#xA;    player.ContractExtraYearOption = contract.ExtraYearOption;&#xD;&#xA;&#xD;&#xA;    int currentYearSalary = 0;&#xD;&#xA;    int currentYearBonus = 0;    &#xD;&#xA;&#xD;&#xA;    if(currentYear == 0)&#xD;&#xA;    {&#xD;&#xA;        currentYearSalary = player.ContractSalary0;&#xD;&#xA;        currentYearBonus = player.ContractBonus0;&#xD;&#xA;    }&#xD;&#xA;    else if(currentYear == 1)&#xD;&#xA;    {&#xD;&#xA;        currentYearSalary = player.ContractSalary1;&#xD;&#xA;        currentYearBonus = player.ContractBonus1;&#xD;&#xA;    }&#xD;&#xA;    else if(currentYear == 2)&#xD;&#xA;    {&#xD;&#xA;        currentYearSalary = player.ContractSalary2;&#xD;&#xA;        currentYearBonus = player.ContractBonus2;&#xD;&#xA;    }&#xD;&#xA;    else if(currentYear == 3)&#xD;&#xA;    {&#xD;&#xA;        currentYearSalary = player.ContractSalary3;&#xD;&#xA;        currentYearBonus = player.ContractBonus3;&#xD;&#xA;    }&#xD;&#xA;    else if(currentYear == 4)&#xD;&#xA;    {&#xD;&#xA;        currentYearSalary = player.ContractSalary4;&#xD;&#xA;        currentYearBonus = player.ContractBonus4;&#xD;&#xA;    }&#xD;&#xA;    else if(currentYear == 5)&#xD;&#xA;    {&#xD;&#xA;        currentYearSalary = player.ContractSalary5;&#xD;&#xA;        currentYearBonus = player.ContractBonus5;&#xD;&#xA;    }&#xD;&#xA;    else if(currentYear == 6)&#xD;&#xA;    {&#xD;&#xA;        currentYearSalary = player.ContractSalary6;&#xD;&#xA;        currentYearBonus = player.ContractBonus6;&#xD;&#xA;    }&#xD;&#xA;    else if(currentYear == 7)&#xD;&#xA;    {&#xD;&#xA;        currentYearSalary = player.ContractSalary7;&#xD;&#xA;        currentYearBonus = player.ContractBonus7;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Assign the player salary.&#xD;&#xA;    player.ContractSalary0 = currentYearSalary;&#xD;&#xA;    player.ContractSalary1 = contract.SalaryTable[0];&#xD;&#xA;    player.ContractSalary2 = contract.SalaryTable[1];&#xD;&#xA;    player.ContractSalary3 = contract.SalaryTable[2];&#xD;&#xA;    player.ContractSalary4 = contract.SalaryTable[3];&#xD;&#xA;    player.ContractSalary5 = contract.SalaryTable[4];&#xD;&#xA;    player.ContractSalary6 = contract.SalaryTable[5];&#xD;&#xA;    player.ContractSalary7 = contract.SalaryTable[6];&#xD;&#xA;&#xD;&#xA;    // Assign the player bonus.&#xD;&#xA;    player.ContractBonus0 = currentYearBonus;&#xD;&#xA;    player.ContractBonus1 = contract.BonusTable[0];&#xD;&#xA;    player.ContractBonus2 = contract.BonusTable[1];&#xD;&#xA;    player.ContractBonus3 = contract.BonusTable[2];&#xD;&#xA;    player.ContractBonus4 = contract.BonusTable[3];&#xD;&#xA;    player.ContractBonus5 = contract.BonusTable[4];&#xD;&#xA;    player.ContractBonus6 = contract.BonusTable[5];&#xD;&#xA;    player.ContractBonus7 = contract.BonusTable[6];&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Update the player's contract status.&#xD;&#xA;PlayerManager.UpdatePlayerContractStatus(player, team, team, player.ContractStatus, PlayerContractStatus.Signed, wasFranchiseTagged, false);&#xD;&#xA;&#xD;&#xA;if (wasFranchiseTagged)&#xD;&#xA;{&#xD;&#xA;    // If the player was franchise tagged, set the team franchise tag player and send notification.&#xD;&#xA;    team.FranchiseTaggedPlayer = player;&#xD;&#xA;    IssueFranchiseTagNotifyRequest(team, player);&#xD;&#xA;}" />
            <attribute name="RetireCoach" idx="34" type="TeamManager_RetireCoach" guid="5307-32881" final="true" default="// Remove the coach from the team.&#xD;&#xA;if(coach.Position == CoachPosition.HeadCoach)&#xD;&#xA;    {&#xD;&#xA;        // Remove coach from the old team.&#xD;&#xA;        team.HeadCoach = null;&#xD;&#xA;    }&#xD;&#xA;    else if(coach.Position == CoachPosition.OffensiveCoordinator)&#xD;&#xA;    {&#xD;&#xA;        // Remove OC from the old team.&#xD;&#xA;        team.OffensiveCoordinator = null;&#xD;&#xA;    }&#xD;&#xA;    else if(coach.Position == CoachPosition.DefensiveCoordinator)&#xD;&#xA;    {&#xD;&#xA;        // Remove DC from the old team.&#xD;&#xA;        team.DefensiveCoordinator = null;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;// Update the coach's contract status.&#xD;&#xA;Team newTeam = null ;&#xD;&#xA;CoachManager.UpdateCoachContractStatus(coach, team, coach.Position, newTeam, StaffPersonContractStatus.Retired);&#xD;&#xA;" />
            <attribute name="RetireOwner" idx="35" type="TeamManager_RetireOwner" guid="5307-121108" final="true" default="if (team != null)&#xD;&#xA;{&#xD;&#xA;    // Restore the default team owner.&#xD;&#xA;    team.Owner = team.DefaultOwner;&#xD;&#xA;&#xD;&#xA;    owner.PrevTeamIndex = owner.TeamIndex;&#xD;&#xA;    owner.TeamIndex = TeamIndex.None;&#xD;&#xA;  &#xD;&#xA;    // Add the user to the team blacklist.&#xD;&#xA;    team.BlacklistUserIdLower = user.UserIdLower;&#xD;&#xA;    team.BlacklistUserIdUpper = user.UserIdUpper;&#xD;&#xA;&#xD;&#xA;    // Send the owner retirement event.&#xD;&#xA;    RetireOwnerEvent event = RetireOwnerEventRef;&#xD;&#xA;    event.Owner = owner;&#xD;&#xA;    event.Team = team; &#xD;&#xA;    event.Send();&#xD;&#xA;&#xD;&#xA;    // Clear the event data to prevent leaks.&#xD;&#xA;    event.Owner = null;&#xD;&#xA;    event.Team = null;&#xD;&#xA;}&#xD;&#xA;" />
