            <attribute name="Formula" idx="0" type="Season_EndOfPlayersSeasonView_Formula" guid="9467-36219" default="Player player = Subject;&#xD;&#xA;&#xD;&#xA;// This is called for all team roster players, team practice squad players, and free agents.&#xD;&#xA;// Since practice squad players are in the free agent array, don't call this twice for them.&#xD;&#xA;if (player.ContractStatus == PlayerContractStatus.FreeAgent || Team != null)&#xD;&#xA;{&#xD;&#xA;    // Increment the player's age.&#xD;&#xA;    PlayerManager.AgePlayer(player);&#xD;&#xA;&#xD;&#xA;    // Handle the player on practice squad.&#xD;&#xA;    if (player.WasOnPracticeSquadThisYear)&#xD;&#xA;    {&#xD;&#xA;        player.WasOnPracticeSquadThisYear = false;&#xD;&#xA;        player.PracticeSquadYears = player.PracticeSquadYears + 1;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // If TeamManager is null, it means this player is in the free agent or&#xD;&#xA;    // retirement pool for some reason. He needs to be removed from the&#xD;&#xA;    // pools via validation and repair.&#xD;&#xA;    if (TeamManager != null &amp;&amp; Team != null)&#xD;&#xA;    {&#xD;&#xA;        // Advance the player's contract year.&#xD;&#xA;        if (player.ContractStatus == PlayerContractStatus.Signed)&#xD;&#xA;        {&#xD;&#xA;            TeamManager.AdvancePlayerContractYear(Team, player);&#xD;&#xA;        }&#xD;&#xA;        else if (player.ContractStatus == PlayerContractStatus.PracticeSquad)&#xD;&#xA;        {&#xD;&#xA;            int playerSalary = SalaryMetrics.GetMarketContractAnnualSalary(player.Position, PlayerGradeEval.CalculateGrades(player, player.PlayerType), player.Age);&#xD;&#xA;            PlayerContract contract = PlayerContractManager.CreatePlayerContract(playerSalary, 0, 2);&#xD;&#xA;            TeamManager.SignPlayer(Team, player, contract);&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Evaluate the player's retirement status.&#xD;&#xA;    if ((player.ContractStatus == PlayerContractStatus.Signed) ||&#xD;&#xA;        (player.ContractStatus == PlayerContractStatus.FreeAgent) ||&#xD;&#xA;        (player.ContractStatus == PlayerContractStatus.Expiring))&#xD;&#xA;    {&#xD;&#xA;        PlayerRetirementEval.EvaluatePlayerForRetirement(Subject, Team);&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Clear the player's seasonal goal.&#xD;&#xA;    player.SeasonalGoal = null;&#xD;&#xA;}" />
