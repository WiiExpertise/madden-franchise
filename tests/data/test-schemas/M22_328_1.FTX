            <attribute name="BeginDraftPickTradeNegotiation" idx="0" type="TradeNegotiationManager_BeginDraftPickTradeNegotiation" guid="6058-40902" final="true" default="// Create the negotiation.&#xD;&#xA;TradeNegotiation negotiation = TradeEvalRef.CreateDraftPickNegotiation(teamA, draftPickA);&#xD;&#xA;&#xD;&#xA;// If the negotiation was successfully created, fill out the required members.&#xD;&#xA;if (negotiation != null)&#xD;&#xA;{&#xD;&#xA;    // Add the negotiation to the list.&#xD;&#xA;    TradeNegotiationList.Append(negotiation);&#xD;&#xA;&#xD;&#xA;    // Set the draft active flag. This prevents the negotiation from being processed until&#xD;&#xA;    // the team is ready to make a draft selection.&#xD;&#xA;    negotiation.IsDraftActive = true;&#xD;&#xA;&#xD;&#xA;    // Send requests to all other teams.&#xD;&#xA;    TradeMakeOfferRequestTeams view = new TradeMakeOfferRequestTeams;&#xD;&#xA;    {&#xD;&#xA;        view.Subjects = Franchise.Teams;&#xD;&#xA;        view.Negotiation = negotiation;&#xD;&#xA;        view.TeamRequestManager = TeamRequestManagerRef;&#xD;&#xA;        view.TradeEval = TradeEvalRef;&#xD;&#xA;        view.Update();&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Return the negotiation object.&#xD;&#xA;negotiation;" />
            <attribute name="BeginPlayerTradeNegotiation" idx="1" type="TradeNegotiationManager_BeginPlayerTradeNegotiation" guid="6058-24631" final="true" default="// Create the negotiation.&#xD;&#xA;TradeNegotiation negotiation = TradeEvalRef.CreatePlayerNegotiation(teamA, playerA);&#xD;&#xA;&#xD;&#xA;// If the negotiation was successfully created, fill out the required members.&#xD;&#xA;if (negotiation != null)&#xD;&#xA;{&#xD;&#xA;    // Add the negotiation to the list.&#xD;&#xA;    TradeNegotiationList.Append(negotiation);&#xD;&#xA;&#xD;&#xA;    // Send requests to all other teams.&#xD;&#xA;    TradeMakeOfferRequestTeams view = new TradeMakeOfferRequestTeams;&#xD;&#xA;    {&#xD;&#xA;        view.Subjects = Franchise.Teams;&#xD;&#xA;        view.Negotiation = negotiation;&#xD;&#xA;        view.TeamRequestManager = TeamRequestManagerRef;&#xD;&#xA;        view.TradeEval = TradeEvalRef;&#xD;&#xA;        view.Update();&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Send the trade proposal event.&#xD;&#xA;    Trade_TradeBlockEvent event = new Trade_TradeBlockEvent;&#xD;&#xA;    {&#xD;&#xA;        event.PlayerA = playerA;&#xD;&#xA;        event.TeamA = teamA;&#xD;&#xA;        event.Send();&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Return the negotiation object.&#xD;&#xA;negotiation;" />
            <attribute name="BeginTeamTradeNegotiation" idx="2" type="TradeNegotiationManager_BeginTeamTradeNegotiation" guid="6058-40904" final="true" default="// Create the negotiation.&#xD;&#xA;TradeNegotiation negotiation = TradeEvalRef.CreateTeamNegotiation(packageA, packageB);&#xD;&#xA;&#xD;&#xA;// If the negotiation is valid, proceed.&#xD;&#xA;if (negotiation != null)&#xD;&#xA;{&#xD;&#xA;    // Issue requests to both teams.&#xD;&#xA;    negotiation.RequestA = IssueNegotiationUpdateRequest(packageA.Team, negotiation);&#xD;&#xA;    negotiation.RequestB = IssueNegotiationAcceptRequest(packageB.Team, negotiation);&#xD;&#xA;&#xD;&#xA;    // Add the negotiation to the list.&#xD;&#xA;    TradeNegotiationList.Append(negotiation);&#xD;&#xA;&#xD;&#xA;    // send a trade proposal event&#xD;&#xA;    Trade_TradeProposalEvent event = new Trade_TradeProposalEvent;&#xD;&#xA;    {&#xD;&#xA;        event.TeamA = negotiation.PackageA.Team;&#xD;&#xA;        event.PackageA = negotiation.PackageA;&#xD;&#xA;        event.RequestA = negotiation.RequestA;&#xD;&#xA;        event.TeamB = negotiation.PackageB.Team;&#xD;&#xA;        event.PackageB = negotiation.PackageB;&#xD;&#xA;        event.RequestB = negotiation.RequestB;&#xD;&#xA;        event.isModifiedProposal = false;&#xD;&#xA;        event.isCounterProposal = false;&#xD;&#xA;        event.isWithdrawnProposal = false;&#xD;&#xA;        event.isTradeAccepted = false;&#xD;&#xA;        event.isTradeFinished = false;&#xD;&#xA;        event.TradeNegotiation = negotiation;&#xD;&#xA;&#xD;&#xA;        event.Send();&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Return the negotiation object.&#xD;&#xA;negotiation;" />
            <attribute name="CanExecuteTrade" idx="3" type="TradeNegotiationManager_CanExecuteTrade" guid="6058-40933" final="true" default="bool canExecute = false;&#xD;&#xA;&#xD;&#xA;// During the season, we need to make sure that both teams have either completed their game for this&#xD;&#xA;// week, or that neither team has completed their game for this week. Otherwise, a player could play&#xD;&#xA;// two games in one week.&#xD;&#xA;if (Franchise.SeasonInfo.CurrentStage != SeasonStage.OffSeason)&#xD;&#xA;{&#xD;&#xA;    // Retrieve the teams.&#xD;&#xA;    Team teamA = packageA.Team;&#xD;&#xA;    Team teamB = packageB.Team;&#xD;&#xA;&#xD;&#xA;    // Retrieve the current week number.&#xD;&#xA;    int currentWeek = Franchise.SeasonInfo.CurrentWeek;&#xD;&#xA;&#xD;&#xA;    // Prevent the CPU from messing with rosters until after the users have had a chance to go in and make their own trade offers&#xD;&#xA;    if (Franchise.SeasonInfo.CurrentStage == SeasonStage.PreSeason &amp;&amp; currentWeek == 0 &amp;&amp; Franchise.SeasonInfo.CurrentYear == 0 &amp;&amp; !teamA.IsUserManaged() &amp;&amp; !teamB.IsUserManaged())&#xD;&#xA;    {&#xD;&#xA;        canExecute = false;&#xD;&#xA;    }&#xD;&#xA;    else&#xD;&#xA;    {&#xD;&#xA;        TeamStats[] statsA = teamA.GetTeamStatsForStage(Franchise.SeasonInfo.CurrentStage);&#xD;&#xA;        TeamStats[] statsB = teamB.GetTeamStatsForStage(Franchise.SeasonInfo.CurrentStage);&#xD;&#xA;&#xD;&#xA;        // If both teams have game stat arrays, proceed.&#xD;&#xA;        if ((statsA != null) &amp;&amp; (statsB != null))&#xD;&#xA;        {&#xD;&#xA;            // If neither team has played their game this week, the trade can be executed.&#xD;&#xA;            if ((statsA[currentWeek] == null) &amp;&amp; (statsB[currentWeek] == null))&#xD;&#xA;            {&#xD;&#xA;                canExecute = true;&#xD;&#xA;            }&#xD;&#xA;            // If both teams have played their game this week, the trade can be executed.&#xD;&#xA;            else if ((statsA[currentWeek] != null) &amp;&amp; (statsB[currentWeek] != null))&#xD;&#xA;            {&#xD;&#xA;                canExecute = true;&#xD;&#xA;            }&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;// During the offseason, all trades can execute immediately.&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;    canExecute = true;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Return the flag.&#xD;&#xA;canExecute;" />
            <attribute name="EndTradeNegotiation" idx="4" type="TradeNegotiationManager_EndTradeNegotiation" guid="6058-40924" final="true" default="bool tradeExecuted = false;&#xD;&#xA;&#xD;&#xA;// If the negotiation has not yet been processed, do so.&#xD;&#xA;if (!negotiation.IsProcessed)&#xD;&#xA;{&#xD;&#xA;    TeamTradePackage packageA = negotiation.PackageA;&#xD;&#xA;    TeamTradePackage packageB = negotiation.PackageB;&#xD;&#xA;&#xD;&#xA;    // Remove the negotiation from the list to prevent re-entrancy issues.&#xD;&#xA;    TradeNegotiationList.FindAndErase(negotiation);&#xD;&#xA;    negotiation.IsProcessed = true;&#xD;&#xA;&#xD;&#xA;    // If the trade was accepted, execute it.&#xD;&#xA;    if (negotiation.Response == TradeResponse.Accept)&#xD;&#xA;    {&#xD;&#xA;        Team teamA = packageA.Team;&#xD;&#xA;        Team teamB = packageB.Team;&#xD;&#xA;&#xD;&#xA;        // Set both packages as accepted.&#xD;&#xA;        packageA.Response = TradeResponse.Accept;&#xD;&#xA;        packageB.Response = TradeResponse.Accept;&#xD;&#xA;&#xD;&#xA;        // Execute the trade.&#xD;&#xA;        tradeExecuted = TradeManagerRef.ExecuteTrade(packageA, packageB);&#xD;&#xA;&#xD;&#xA;        // If the trade was executed, notify the teams of success.&#xD;&#xA;        if (!tradeExecuted)&#xD;&#xA;        {&#xD;&#xA;            negotiation.Response = TradeResponse.Decline;&#xD;&#xA;        }&#xD;&#xA;&#xD;&#xA;        // Issue notifications to both teams.&#xD;&#xA;        IssueTradeNegotiationNotification(teamA, negotiation);&#xD;&#xA;        IssueTradeNegotiationNotification(teamB, negotiation);&#xD;&#xA;    }&#xD;&#xA;    // If the negotiation failed and was team-to-team, notify both teams of failure.&#xD;&#xA;    else if ((negotiation.PackageA != null) &amp;&amp; (negotiation.PackageB != null))&#xD;&#xA;    {&#xD;&#xA;        Team teamA = negotiation.PackageA.Team;&#xD;&#xA;        Team teamB = negotiation.PackageB.Team;&#xD;&#xA;    &#xD;&#xA;    // Trade was declined by team A&#xD;&#xA;    if(packageA.Response == TradeResponse.Decline)&#xD;&#xA;    {&#xD;&#xA;      //Issue notification to teamB&#xD;&#xA;      IssueTradeNegotiationNotification(teamB, negotiation);&#xD;&#xA;    }&#xD;&#xA;    else if(packageB.Response == TradeResponse.Decline)&#xD;&#xA;    {&#xD;&#xA;      //Issue notification to teamB&#xD;&#xA;      IssueTradeNegotiationNotification(teamA, negotiation);&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;        // Set package A response.&#xD;&#xA;        if (packageA.Response == TradeResponse.None)&#xD;&#xA;        {&#xD;&#xA;            packageA.Response = TradeResponse.Decline;&#xD;&#xA;        }&#xD;&#xA;&#xD;&#xA;        // Set package B response.&#xD;&#xA;        if (packageB.Response == TradeResponse.None)&#xD;&#xA;        {&#xD;&#xA;            packageB.Response = TradeResponse.Decline;&#xD;&#xA;        }  &#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Dismiss all pending team requests.&#xD;&#xA;    negotiation.DismissPendingTeamRequests();&#xD;&#xA;    negotiation.PendingTeamRequestListB = null;&#xD;&#xA;&#xD;&#xA;    // If there were multiple teams involved in the trade, notify them of failure.&#xD;&#xA;    if (negotiation.TeamTradePackageListB != null)&#xD;&#xA;    {&#xD;&#xA;        IssueTradeNegotiationNotificationsView view = new IssueTradeNegotiationNotificationsView;&#xD;&#xA;        {&#xD;&#xA;            view.PackageA = packageA;&#xD;&#xA;            view.PackageB = packageB;&#xD;&#xA;            view.Negotiation = negotiation;&#xD;&#xA;            view.TradeNegotiationManager = this;&#xD;&#xA;            view.Subjects = negotiation.TeamTradePackageListB;&#xD;&#xA;            view.Update();&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Clear any outstanding requests on this negotiation.&#xD;&#xA;negotiation.RequestA = null;&#xD;&#xA;negotiation.RequestB = null;&#xD;&#xA;&#xD;&#xA;// Return true if the trade was executed.&#xD;&#xA;tradeExecuted;&#xD;&#xA;" />
            <attribute name="Franchise" idx="5" type="Franchise" />
            <attribute name="HandleTradeDraftPick" idx="6" type="TradeNegotiationManager_HandleTradeDraftPick" guid="6058-41592" final="true" default="// If the pick moved from one team to another, invalidate any trade negotiations involving the pick.&#xD;&#xA;if ((event.OldTeam != null) &amp;&amp; (event.OldTeam != event.NewTeam))&#xD;&#xA;{&#xD;&#xA;    TradeEvalRef.HandleDraftPickMove(event.Pick, event.OldTeam, TradeNegotiationList);&#xD;&#xA;}" />
            <attribute name="HandleUpdatePlayerContractStatus" idx="7" type="TradeNegotiationManager_HandleUpdatePlayerContractStatus" guid="6058-41588" final="true" default="// If the player moved from a valid team to a new team, make sure any trade negotiations&#xD;&#xA;// involving that player are invalidated.&#xD;&#xA;if ((event.OldTeam != null) &amp;&amp; (event.OldTeam != event.NewTeam))&#xD;&#xA;{&#xD;&#xA;    TradeEvalRef.HandlePlayerMove(event.Player, event.OldTeam, TradeNegotiationList);&#xD;&#xA;}&#xD;&#xA;" />
            <attribute name="IsDelegateResponseAvailable" idx="8" type="bool" default="False" />
            <attribute name="IsNotificationEnabled" idx="9" type="bool" default="True" />
            <attribute name="IssueNegotiationAcceptRequest" idx="10" type="TradeNegotiationManager_IssueNegotiationAcceptRequest" guid="6058-40961" final="true" default="TradeNegotiationRequest request = TradeEvalRef.CreateTradeNegotiationRequest();&#xD;&#xA;&#xD;&#xA;// Set the request settings.&#xD;&#xA;request.IsResponseRequired = true;&#xD;&#xA;request.IsConnectionRequired = false;&#xD;&#xA;request.IsBlocking = false;&#xD;&#xA;&#xD;&#xA;// Set the request data.&#xD;&#xA;request.Team = team;&#xD;&#xA;request.RequestData = negotiation;&#xD;&#xA;request.Negotiation = negotiation;&#xD;&#xA;request.NegotiationManager = this;&#xD;&#xA;&#xD;&#xA;// Set the response forms.&#xD;&#xA;request.ResponseForms = new ResponseForm[4];&#xD;&#xA;request.ResponseForms.Append(TradeNegotiation_AcceptResponse);&#xD;&#xA;request.ResponseForms.Append(TradeNegotiation_DeclineResponse);&#xD;&#xA;request.ResponseForms.Append(TradeNegotiation_CounterResponse);&#xD;&#xA;if (IsDelegateResponseAvailable)&#xD;&#xA;{&#xD;&#xA;    request.ResponseForms.Append(TradeNegotiation_DelegateResponse);&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Set the default response.&#xD;&#xA;request.DefaultResponse = TradeNegotiation_DelegateResponse;&#xD;&#xA;&#xD;&#xA;// Issue the request.&#xD;&#xA;TeamRequestManagerRef.Issue(team, request);&#xD;&#xA;&#xD;&#xA;request;&#xD;&#xA;" />
            <attribute name="IssueNegotiationUpdateRequest" idx="11" type="TradeNegotiationManager_IssueNegotiationUpdateRequest" guid="6058-40959" final="true" default="TradeNegotiationRequest request = TradeEvalRef.CreateTradeNegotiationRequest();&#xD;&#xA;&#xD;&#xA;// Set the request settings.&#xD;&#xA;request.IsResponseRequired = true;&#xD;&#xA;request.IsConnectionRequired = false;&#xD;&#xA;request.IsBlocking = false;&#xD;&#xA;&#xD;&#xA;// Set the request data.&#xD;&#xA;request.Team = team;&#xD;&#xA;request.RequestData = negotiation;&#xD;&#xA;request.Negotiation = negotiation;&#xD;&#xA;request.NegotiationManager = this;&#xD;&#xA;&#xD;&#xA;// Set the response forms.&#xD;&#xA;request.ResponseForms = new ResponseForm[2];&#xD;&#xA;&#xD;&#xA;// If there is no pending team list, this is a team-to-team negotiation. Allow the team to&#xD;&#xA;// modify their offer.&#xD;&#xA;if (negotiation.PendingTeamListB == null)&#xD;&#xA;{&#xD;&#xA;    request.ResponseForms.Append(TradeNegotiation_ModifyResponse);&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Always allow the team to withdraw their offer.&#xD;&#xA;request.ResponseForms.Append(TradeNegotiation_WithdrawResponse);&#xD;&#xA;&#xD;&#xA;// Issue the request.&#xD;&#xA;TeamRequestManagerRef.Issue(team, request);&#xD;&#xA;&#xD;&#xA;request;" />
            <attribute name="IssueTradeNegotiationNotification" idx="12" type="TradeNegotiationManager_IssueTradeNegotiationNotification" guid="6058-41557" final="true" default="// If notifications are enabled, issue the notification.&#xD;&#xA;if (IsNotificationEnabled)&#xD;&#xA;{&#xD;&#xA;    TradeNegotiationRequest request = TradeEvalRef.CreateTradeNegotiationRequest();&#xD;&#xA;&#xD;&#xA;    // Set the request settings.&#xD;&#xA;    request.IsResponseRequired = false;&#xD;&#xA;    request.IsConnectionRequired = false;&#xD;&#xA;    request.IsBlocking = false;&#xD;&#xA;    request.Priority = 25;&#xD;&#xA;&#xD;&#xA;    // Set the request data.&#xD;&#xA;    request.Team = team;&#xD;&#xA;    request.RequestData = negotiation;&#xD;&#xA;    request.Negotiation = negotiation;&#xD;&#xA;    &#xD;&#xA;    if(!IsDelegateResponseAvailable)&#xD;&#xA;    {&#xD;&#xA;    request.ResponseForms = new ResponseForm[1];&#xD;&#xA;    request.ResponseForms.Append( TradeNegotiation_AcknowledgeResponse);&#xD;&#xA;&#xD;&#xA;    request.DefaultResponse = TradeNegotiation_AcknowledgeResponse;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Issue the request.&#xD;&#xA;    TeamRequestManagerRef.Issue(team, request);&#xD;&#xA;}" />
            <attribute name="ProcessAcceptOfferRequest" idx="13" type="TradeNegotiationManager_ProcessAcceptOfferRequest" guid="6058-24930" final="true" default="if (negotiation != null)&#xD;&#xA;{&#xD;&#xA;    // If the select response was selected, set the response to accept.&#xD;&#xA;    if (request.Response == request.AcceptResponse)&#xD;&#xA;    {&#xD;&#xA;        negotiation.Response = TradeResponse.Accept;&#xD;&#xA;    }&#xD;&#xA;    // If the decline response was selected, set the response to decline.&#xD;&#xA;    else if (request.Response == request.DeclineResponse)&#xD;&#xA;    {&#xD;&#xA;        negotiation.Response = TradeResponse.Decline;&#xD;&#xA;    }&#xD;&#xA;    // If the delegate response was selected, set the negotiation to be delegated&#xD;&#xA;    // at the end of the week. Note that we don't process it immediately because&#xD;&#xA;    // all offers might not yet be in.&#xD;&#xA;    else if (request.Response == request.DelegateResponse)&#xD;&#xA;    {&#xD;&#xA;        negotiation.IsDelegated = true;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // If a response was selected, end negotiations.&#xD;&#xA;    if (negotiation.Response != TradeResponse.None)&#xD;&#xA;    {&#xD;&#xA;        bool tradeExecuted = EndTradeNegotiation(negotiation);&#xD;&#xA;    }&#xD;&#xA;    // If the negotiation is to be delegated and there are no remaining teams, process the negotiation.&#xD;&#xA;    else if (negotiation.IsDelegated &amp;&amp; !negotiation.IsDraftActive &amp;&amp; (negotiation.PendingTeamListB.Population() == 0))&#xD;&#xA;    {&#xD;&#xA;        ProcessNegotiation(negotiation);&#xD;&#xA;    }&#xD;&#xA;}" />
            <attribute name="ProcessMakeOfferRequest" idx="14" type="TradeNegotiationManager_ProcessMakeOfferRequest" guid="6058-24646" final="true" default="bool offerProcessed = false;&#xD;&#xA;&#xD;&#xA;if (negotiation != null)&#xD;&#xA;{&#xD;&#xA;    TeamTradePackage packageB = null;&#xD;&#xA;&#xD;&#xA;    // If delegate was selected, run the CPU logic to create an offer. This will return null if no&#xD;&#xA;    // offer is created.&#xD;&#xA;    if (request.Response == request.DelegateResponse)&#xD;&#xA;    {&#xD;&#xA;        packageB = TradeEvalRef.CreateTradeOffer(negotiation.PackageA, request.TeamB);&#xD;&#xA;    }&#xD;&#xA;    // If offer was selected, use the offered package.&#xD;&#xA;    else if (request.Response == request.OfferResponse)&#xD;&#xA;    {&#xD;&#xA;        packageB = request.PackageB;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // If a package was offered, add it to the negotiation and send an event.&#xD;&#xA;    if (packageB != null)&#xD;&#xA;    {&#xD;&#xA;        // Add the package the negotiation.&#xD;&#xA;        negotiation.TeamTradePackageListB.Append(packageB);&#xD;&#xA;&#xD;&#xA;        // Send a trade proposal event.&#xD;&#xA;        Trade_TradeProposalEvent event = new Trade_TradeProposalEvent;&#xD;&#xA;        {&#xD;&#xA;            if (negotiation.PackageA != null)&#xD;&#xA;            {&#xD;&#xA;                event.TeamA = negotiation.PackageA.Team;&#xD;&#xA;            }&#xD;&#xA;            event.PackageA = negotiation.PackageA;&#xD;&#xA;            event.RequestA = null;&#xD;&#xA;            event.TeamB = request.TeamB;&#xD;&#xA;            event.PackageB = packageB;&#xD;&#xA;            event.RequestB = null;&#xD;&#xA;            event.TradeNegotiation = negotiation;&#xD;&#xA;            event.isCounterProposal = false;&#xD;&#xA;            event.isModifiedProposal = false;&#xD;&#xA;            event.isWithdrawnProposal = false;&#xD;&#xA;            event.isTradeAccepted = false;&#xD;&#xA;            event.isTradeFinished = false;&#xD;&#xA;            event.Send();&#xD;&#xA;        }&#xD;&#xA;&#xD;&#xA;        // If this was the first package offered, send a request to team A.&#xD;&#xA;        if (negotiation.TeamTradePackageListB.Population() == 1)&#xD;&#xA;        {&#xD;&#xA;    bool canTradePlayer = true;&#xD;&#xA;            if (negotiation.PackageA != null &amp;&amp; negotiation.PackageA.Team != null &amp;&amp; negotiation.PackageA.Team.TeamSettingRef != null)&#xD;&#xA;          {&#xD;&#xA;            canTradePlayer = negotiation.PackageA.Team.TeamSettingRef.IsTradeAndFreeAgencyEnabled;&#xD;&#xA;        }&#xD;&#xA;    &#xD;&#xA;        if (negotiation.PackageB != null &amp;&amp; negotiation.PackageB.Team != null &amp;&amp; negotiation.PackageB.Team.TeamSettingRef != null)&#xD;&#xA;        {&#xD;&#xA;            canTradePlayer = canTradePlayer  &amp;&amp; negotiation.PackageB.Team.TeamSettingRef.IsTradeAndFreeAgencyEnabled;&#xD;&#xA;        }&#xD;&#xA;&#xD;&#xA;          if (canTradePlayer)&#xD;&#xA;              {&#xD;&#xA;                TradeAcceptOfferRequest acceptRequest = TradeEvalRef.CreateTradeAcceptOfferRequest(negotiation);&#xD;&#xA;                TeamRequestManagerRef.Issue(negotiation.PackageA.Team, acceptRequest);&#xD;&#xA;         }&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;    // If the team is user-controlled, add the player to the blacklist.&#xD;&#xA;    else if (request.TeamB.HeadCoach == request.TeamB.UserCharacter)&#xD;&#xA;    {&#xD;&#xA;        TradeEvalRef.AddPlayerToTeamTradeBlackList(request.TeamB, negotiation.PackageA);&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Remove team B and the request from the pending lists.&#xD;&#xA;    negotiation.PendingTeamListB.FindAndErase(request.TeamB);&#xD;&#xA;    negotiation.PendingTeamRequestListB.FindAndErase(request);&#xD;&#xA;&#xD;&#xA;    // If the negotiation is to be delegated and all teams have responded, process the negotiation.&#xD;&#xA;    if (negotiation.IsDelegated &amp;&amp; !negotiation.IsDraftActive &amp;&amp; (negotiation.PendingTeamListB.Population() == 0))&#xD;&#xA;    {&#xD;&#xA;        ProcessNegotiation(negotiation);&#xD;&#xA;    }&#xD;&#xA;    // If a human user made an offer, issue an update request so that he can withdraw it later.&#xD;&#xA;    else if ((packageB != null) &amp;&amp; (request.TargetUser != null))&#xD;&#xA;    {&#xD;&#xA;        Request teamRequest = IssueNegotiationUpdateRequest(request.TeamB, negotiation);&#xD;&#xA;&#xD;&#xA;        // If the team request is valid, store it in the pending request list.&#xD;&#xA;        if ((negotiation.PendingTeamRequestListB != null) &amp;&amp; (teamRequest != null))&#xD;&#xA;        {&#xD;&#xA;            negotiation.PendingTeamRequestListB.Append(teamRequest);&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Mark the offer as processed.&#xD;&#xA;    offerProcessed = true;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;offerProcessed;&#xD;&#xA;" />
            <attribute name="ProcessNegotiation" idx="15" type="TradeNegotiationManager_ProcessNegotiation" guid="6058-41239" final="true" default="// If the negotiation has not yet been processed, do so now.&#xD;&#xA;if (!negotiation.IsProcessed)&#xD;&#xA;{&#xD;&#xA;    // Select the best offer.&#xD;&#xA;    negotiation.PackageB = TradeEvalRef.EvaluateTradeOffers(negotiation.PackageA, negotiation.TeamTradePackageListB);&#xD;&#xA;&#xD;&#xA;    // If an offer was selected, execute the trade.&#xD;&#xA;    if (negotiation.PackageB != null)&#xD;&#xA;    {&#xD;&#xA;        // Set the response to accepted.&#xD;&#xA;        negotiation.Response = TradeResponse.Accept;&#xD;&#xA;&#xD;&#xA;        // If the trade can be executed, do so. Otherwise, wait until the end of the week after&#xD;&#xA;        // both teams have played their game.&#xD;&#xA;        if (CanExecuteTrade(negotiation.PackageA, negotiation.PackageB))&#xD;&#xA;        {&#xD;&#xA;            bool tradeExecuted = EndTradeNegotiation(negotiation);&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;    // Otherwise, end negotiations immediately.&#xD;&#xA;    else&#xD;&#xA;    {&#xD;&#xA;        negotiation.Response = TradeResponse.Decline;&#xD;&#xA;        bool tradeExecuted = EndTradeNegotiation(negotiation);&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;" />
            <attribute name="ProcessNegotiationRequest" idx="16" type="TradeNegotiationManager_ProcessNegotiationRequest" guid="6058-40976" final="true" default="TradeNegotiation negotiation = request.Negotiation;&#xD;&#xA;&#xD;&#xA;if ( negotiation != null )&#xD;&#xA;{&#xD;&#xA; // If this is a league-wide negotiation, remove the request from the pending request list.&#xD;&#xA; if (negotiation.PendingTeamRequestListB != null)&#xD;&#xA; {&#xD;&#xA;  negotiation.PendingTeamRequestListB.FindAndErase(request);&#xD;&#xA; }&#xD;&#xA;&#xD;&#xA; // If the negotiation has not yet been resolved, proceed. Otherwise, this is simply the&#xD;&#xA; // last team's request being dismissed.&#xD;&#xA; if (negotiation.Response == TradeResponse.None)&#xD;&#xA; {&#xD;&#xA;  // If the trade was modified, re-issue trade requests.&#xD;&#xA;  if ((request.Response == TradeNegotiation_ModifyResponse) ||&#xD;&#xA;  (request.Response == TradeNegotiation_CounterResponse))&#xD;&#xA;  {&#xD;&#xA;   Team teamA = negotiation.PackageA.Team;&#xD;&#xA;   Team teamB = negotiation.PackageB.Team;&#xD;&#xA;&#xD;&#xA;   // If team A responded, dismiss request B and re-issue requests.&#xD;&#xA;   if (request.Team == teamA)&#xD;&#xA;   {&#xD;&#xA;    if (negotiation.RequestB != null)&#xD;&#xA;    {&#xD;&#xA;     negotiation.RequestB.Dismiss();&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    negotiation.RequestA = IssueNegotiationUpdateRequest(teamA, negotiation);&#xD;&#xA;    negotiation.RequestB = IssueNegotiationAcceptRequest(teamB, negotiation);&#xD;&#xA;   }&#xD;&#xA;   // If team B responded, dismiss request A and re-issue requests.&#xD;&#xA;   else if (request.Team == teamB)&#xD;&#xA;   {&#xD;&#xA;    if (negotiation.RequestA != null)&#xD;&#xA;    {&#xD;&#xA;     negotiation.RequestA.Dismiss();&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    negotiation.RequestA = IssueNegotiationAcceptRequest(teamA, negotiation);&#xD;&#xA;    negotiation.RequestB = IssueNegotiationUpdateRequest(teamB, negotiation);&#xD;&#xA;   }&#xD;&#xA;&#xD;&#xA;   // Send an event for the updated trade proposal.&#xD;&#xA;   Trade_TradeProposalEvent event = new Trade_TradeProposalEvent;&#xD;&#xA;   if (event != null)&#xD;&#xA;   {&#xD;&#xA;    if (negotiation.PackageA != null)&#xD;&#xA;    {&#xD;&#xA;     event.TeamA = negotiation.PackageA.Team;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    event.PackageA = negotiation.PackageA;&#xD;&#xA;    event.RequestA = negotiation.RequestA;&#xD;&#xA;    &#xD;&#xA;    if (negotiation.PackageB != null)&#xD;&#xA;    {&#xD;&#xA;     event.TeamB = negotiation.PackageB.Team;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    event.PackageB = negotiation.PackageB;&#xD;&#xA;    event.RequestB = negotiation.RequestB;&#xD;&#xA;    event.isCounterProposal = (request.Response == TradeNegotiation_CounterResponse);&#xD;&#xA;    event.isModifiedProposal = (request.Response == TradeNegotiation_ModifyResponse);&#xD;&#xA;    event.isWithdrawnProposal = false;&#xD;&#xA;    event.isTradeAccepted = false;&#xD;&#xA;    event.isTradeFinished = false;&#xD;&#xA;    event.TradeNegotiation = negotiation;&#xD;&#xA;&#xD;&#xA;    event.Send();&#xD;&#xA;   }&#xD;&#xA;  }  &#xD;&#xA;  else if (request.Response == TradeNegotiation_DeclineResponse)// If the trade was declined, end the negotiation.&#xD;&#xA;  {&#xD;&#xA;   negotiation.Response = TradeResponse.Decline;&#xD;&#xA;&#xD;&#xA;   // Send an event for the trade execution. Note that the trade can fail even if&#xD;&#xA;   // it was accepted.&#xD;&#xA;   Trade_TradeProposalEvent event = new Trade_TradeProposalEvent;&#xD;&#xA;   if (event != null)&#xD;&#xA;   {&#xD;&#xA;    if (negotiation.PackageA != null)&#xD;&#xA;    {&#xD;&#xA;     event.TeamA = negotiation.PackageA.Team;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    event.PackageA = negotiation.PackageA;&#xD;&#xA;    event.RequestA = negotiation.RequestA;&#xD;&#xA;    &#xD;&#xA;    if (negotiation.PackageB != null)&#xD;&#xA;    {&#xD;&#xA;     event.TeamB = negotiation.PackageB.Team;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    event.PackageB = negotiation.PackageB;&#xD;&#xA;    event.RequestB = negotiation.RequestB;&#xD;&#xA;    event.isCounterProposal = false;&#xD;&#xA;    event.isModifiedProposal = false;&#xD;&#xA;    event.isWithdrawnProposal = false;&#xD;&#xA;    event.isTradeAccepted = false;&#xD;&#xA;    event.isTradeFinished = true;&#xD;&#xA;    event.TradeNegotiation = negotiation;&#xD;&#xA;&#xD;&#xA;    event.Send();&#xD;&#xA;   }&#xD;&#xA;   &#xD;&#xA;   Team teamA = negotiation.PackageA.Team;&#xD;&#xA;   Team teamB = negotiation.PackageB.Team;&#xD;&#xA;   &#xD;&#xA;   // If the trade was declined by team A, notify team B of their decision.&#xD;&#xA;   if (request.Team == teamA)&#xD;&#xA;   {&#xD;&#xA;    negotiation.PackageA.Response = TradeResponse.Decline;&#xD;&#xA;   }&#xD;&#xA;   else if (request.Team == teamB)&#xD;&#xA;   {&#xD;&#xA;    negotiation.PackageB.Response = TradeResponse.Decline;&#xD;&#xA;   }&#xD;&#xA;  }&#xD;&#xA;  else if (request.Response == TradeNegotiation_WithdrawResponse)// If the trade was withdrawn, update the negotiation accordingly.&#xD;&#xA;  {&#xD;&#xA;   // If this is a team-to-team negotiation, end the negotiation.&#xD;&#xA;   if (negotiation.TeamTradePackageListB == null)&#xD;&#xA;   {&#xD;&#xA;    negotiation.Response = TradeResponse.Decline;&#xD;&#xA;   }   &#xD;&#xA;   else if (negotiation.PendingTeamRequestListB != null)// If this is a league-wide negotiation, remove the team's offer.&#xD;&#xA;   {&#xD;&#xA;    negotiation.RemoveTeamTradePackage(request.Team);&#xD;&#xA;   }&#xD;&#xA;&#xD;&#xA;   // Send an event for the trade execution. Note that the trade can fail even if&#xD;&#xA;   // it was accepated.&#xD;&#xA;   Trade_TradeProposalEvent event = new Trade_TradeProposalEvent;&#xD;&#xA;   if (event != null)&#xD;&#xA;   {&#xD;&#xA;    if (negotiation.PackageA != null)&#xD;&#xA;    {&#xD;&#xA;     event.TeamA = negotiation.PackageA.Team;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    event.PackageA = negotiation.PackageA;&#xD;&#xA;    event.RequestA = negotiation.RequestA;&#xD;&#xA;    &#xD;&#xA;    if (negotiation.PackageB != null)&#xD;&#xA;    {&#xD;&#xA;     event.TeamB = negotiation.PackageB.Team;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    event.PackageB = negotiation.PackageB;&#xD;&#xA;    event.RequestB = negotiation.RequestB;&#xD;&#xA;    event.isCounterProposal = false;&#xD;&#xA;    event.isModifiedProposal = false;&#xD;&#xA;    event.isWithdrawnProposal = true;&#xD;&#xA;    event.isTradeAccepted = false;&#xD;&#xA;    event.isTradeFinished = true;&#xD;&#xA;    event.TradeNegotiation = negotiation;&#xD;&#xA;&#xD;&#xA;    event.Send();&#xD;&#xA;   }&#xD;&#xA;  }&#xD;&#xA;  else if (request.Response == TradeNegotiation_AcceptResponse)// If the trade was accepted, end the negotiation with acceptance.&#xD;&#xA;  {&#xD;&#xA;   negotiation.Response = TradeResponse.Accept;&#xD;&#xA;&#xD;&#xA;   // Send an event for the trade execution. Note that the trade can fail even if&#xD;&#xA;   // it was accepated.&#xD;&#xA;   Trade_TradeProposalEvent event = new Trade_TradeProposalEvent;&#xD;&#xA;   if (event != null)&#xD;&#xA;   {&#xD;&#xA;    if (negotiation.PackageA != null)&#xD;&#xA;    {&#xD;&#xA;     event.TeamA = negotiation.PackageA.Team;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    event.PackageA = negotiation.PackageA;&#xD;&#xA;    event.RequestA = negotiation.RequestA;&#xD;&#xA;    &#xD;&#xA;    if (negotiation.PackageB != null)&#xD;&#xA;    {&#xD;&#xA;     event.TeamB = negotiation.PackageB.Team;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    event.PackageB = negotiation.PackageB;&#xD;&#xA;    event.RequestB = negotiation.RequestB;&#xD;&#xA;    event.isCounterProposal = false;&#xD;&#xA;    event.isModifiedProposal = false;&#xD;&#xA;    event.isWithdrawnProposal = false;&#xD;&#xA;    event.isTradeAccepted = true;&#xD;&#xA;    event.isTradeFinished = true;&#xD;&#xA;    event.TradeNegotiation = negotiation;&#xD;&#xA;&#xD;&#xA;    event.Send();&#xD;&#xA;   }&#xD;&#xA;  }&#xD;&#xA;  // If the trade was delegated, decline the offer when both teams are User-Managed or use CPU logic to determine whether to accept in other case.&#xD;&#xA;  else if (request.Response == TradeNegotiation_DelegateResponse)&#xD;&#xA;  {&#xD;&#xA;   Team teamA = negotiation.PackageA.Team;&#xD;&#xA;   Team teamB = negotiation.PackageB.Team;&#xD;&#xA;&#xD;&#xA;   int response = TradeResponse.Decline;&#xD;&#xA;   if (!teamA.IsUserManaged() || !teamB.IsUserManaged())&#xD;&#xA;   {&#xD;&#xA;    // Evaluate the trade offer using CPU logic.&#xD;&#xA;    response = TradeEvalRef.EvaluateTradeOffer(request.Team, negotiation.PackageA, negotiation.PackageB);&#xD;&#xA;   }&#xD;&#xA;&#xD;&#xA;   // If the trade was accepted, end the negotiation.&#xD;&#xA;   if (response == TradeResponse.Accept)&#xD;&#xA;   {&#xD;&#xA;    negotiation.Response = TradeResponse.Accept;&#xD;&#xA;   }&#xD;&#xA;   // If the trade was declined by team B, notify team A of their decision.&#xD;&#xA;   else if (request.Team == teamB)&#xD;&#xA;   {&#xD;&#xA;    bool issueNotification = true;&#xD;&#xA;    if (negotiation.RequestA != null)&#xD;&#xA;    {&#xD;&#xA;     // If RequestA is ModifyResponse or CounterResponse, we do not want to issue an &quot;acknowledge&quot; into TeamA corresponding to the old negotiation request.&#xD;&#xA;     issueNotification = ((negotiation.RequestA.Response != TradeNegotiation_ModifyResponse) &amp;&amp; &#xD;&#xA;           (negotiation.RequestA.Response != TradeNegotiation_CounterResponse));&#xD;&#xA;     negotiation.RequestA.Dismiss();&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    if (issueNotification)&#xD;&#xA;    {&#xD;&#xA;     IssueTradeNegotiationNotification(teamA, negotiation);&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    negotiation.RequestA = null;&#xD;&#xA;    negotiation.RequestB = null;&#xD;&#xA;   }&#xD;&#xA;   // Otherwise, end the negotiation.&#xD;&#xA;   else&#xD;&#xA;   {&#xD;&#xA;    negotiation.Response = response;&#xD;&#xA;   }&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  // If the trade has no response, wait for further action.&#xD;&#xA;  if (negotiation.Response == TradeResponse.None)&#xD;&#xA;  {&#xD;&#xA;   // Do nothing. Wait for further action.&#xD;&#xA;  }&#xD;&#xA;  // If the trade was accepted, attempt to execute the trade.&#xD;&#xA;  else if (negotiation.Response == TradeResponse.Accept)&#xD;&#xA;  {&#xD;&#xA;   if (CanExecuteTrade(negotiation.PackageA, negotiation.PackageB))&#xD;&#xA;   {&#xD;&#xA;    EndTradeNegotiation(negotiation);&#xD;&#xA;&#xD;&#xA;    // Send an event for the trade execution. Note that the trade can fail even if&#xD;&#xA;    // it was accepated.&#xD;&#xA;    Trade_TradeProposalEvent event = new Trade_TradeProposalEvent;&#xD;&#xA;    if (event != null)&#xD;&#xA;    {&#xD;&#xA;     if (negotiation.PackageA != null)&#xD;&#xA;     {&#xD;&#xA;      event.TeamA = negotiation.PackageA.Team;&#xD;&#xA;     }&#xD;&#xA;     &#xD;&#xA;     event.PackageA = negotiation.PackageA;&#xD;&#xA;     event.RequestA = negotiation.RequestA;&#xD;&#xA;     &#xD;&#xA;     if (negotiation.PackageB != null)&#xD;&#xA;     {&#xD;&#xA;      event.TeamB = negotiation.PackageB.Team;&#xD;&#xA;     }&#xD;&#xA;     &#xD;&#xA;     event.PackageB = negotiation.PackageB;&#xD;&#xA;     event.RequestB = negotiation.RequestB;&#xD;&#xA;     event.isCounterProposal = false;&#xD;&#xA;     event.isModifiedProposal = false;&#xD;&#xA;     event.isWithdrawnProposal = false;&#xD;&#xA;     event.isTradeAccepted = (negotiation.Response == TradeResponse.Accept);&#xD;&#xA;     event.isTradeFinished = true;&#xD;&#xA;     event.TradeNegotiation = negotiation;&#xD;&#xA;&#xD;&#xA;     event.Send();&#xD;&#xA;    }&#xD;&#xA;   }&#xD;&#xA;  }&#xD;&#xA;  // Otherwise, the trade was declined. End negotiations.&#xD;&#xA;  else&#xD;&#xA;  {&#xD;&#xA;   EndTradeNegotiation(negotiation);&#xD;&#xA;&#xD;&#xA;   // Send and event for the declined negotiation.&#xD;&#xA;   Trade_TradeProposalEvent event = new Trade_TradeProposalEvent;&#xD;&#xA;   if (event != null)&#xD;&#xA;   {&#xD;&#xA;    if (negotiation.PackageA != null)&#xD;&#xA;    {&#xD;&#xA;     event.TeamA = negotiation.PackageA.Team;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    event.PackageA = negotiation.PackageA;&#xD;&#xA;    event.RequestA = negotiation.RequestA;&#xD;&#xA;    &#xD;&#xA;    if (negotiation.PackageB != null)&#xD;&#xA;    {&#xD;&#xA;     event.TeamB = negotiation.PackageB.Team;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    event.PackageB = negotiation.PackageB;&#xD;&#xA;    event.RequestB = negotiation.RequestB;&#xD;&#xA;    event.isCounterProposal = false;&#xD;&#xA;    event.isModifiedProposal = false;&#xD;&#xA;    event.isWithdrawnProposal = (request.Response == TradeNegotiation_WithdrawResponse);&#xD;&#xA;    event.isTradeAccepted = false;&#xD;&#xA;    event.isTradeFinished = true;&#xD;&#xA;    event.TradeNegotiation = negotiation;&#xD;&#xA;&#xD;&#xA;    event.Send();&#xD;&#xA;   }&#xD;&#xA;  }&#xD;&#xA; }&#xD;&#xA;}" />
            <attribute name="ProcessNegotiations" idx="17" type="none()" guid="6058-24859" final="true" default="// If there are outstanding negotiations, process them all.&#xD;&#xA;if ((TradeNegotiationList != null) &amp;&amp; (TradeNegotiationList.Population() != 0))&#xD;&#xA;{&#xD;&#xA;    // Process all outstanding trade negotiations.&#xD;&#xA;    UpdateTradeNegotiations view = new UpdateTradeNegotiations;&#xD;&#xA;    {&#xD;&#xA;        view.Subjects = TradeNegotiationList;&#xD;&#xA;        view.TradeEvalRef = TradeEvalRef;&#xD;&#xA;        view.TradeNegotiationManagerRef = this;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    // Clear the negotiation list.&#xD;&#xA;    TradeNegotiationList.EraseAll();&#xD;&#xA;}" />
