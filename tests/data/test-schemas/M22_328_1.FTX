            <attribute name="Formula" idx="0" type="UpdatePostSeasonStartStats_Formula" guid="6728-11144" default="SeasonGame game = Subject;&#xD;&#xA;&#xD;&#xA;// If this is a playoff or Pro Bowl bowl week, update both team coach's stats.&#xD;&#xA;if ( ( (game.SeasonWeekType &gt;= SeasonWeekType.FirstPlayoff_) &amp;&amp; (game.SeasonWeekType &lt;= SeasonWeekType.LastPlayoff_) ) ||&#xD;&#xA;     ( game.SeasonWeekType == SeasonWeekType.ProBowl ) )&#xD;&#xA;{&#xD;&#xA;    Team homeTeam = game.HomeTeam;&#xD;&#xA;    Team awayTeam = game.AwayTeam;&#xD;&#xA;&#xD;&#xA;    if (homeTeam != null)&#xD;&#xA;    {&#xD;&#xA;        Coach homeCoach = homeTeam.HeadCoach;&#xD;&#xA;        if (homeCoach != null)&#xD;&#xA;        {&#xD;&#xA;            homeCoach.CareerPlayoffsMade = homeCoach.CareerPlayoffsMade + 1;&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    if (awayTeam != null)&#xD;&#xA;    {&#xD;&#xA;        Coach awayCoach = awayTeam.HeadCoach;&#xD;&#xA;        if (awayCoach != null)&#xD;&#xA;        {&#xD;&#xA;            awayCoach.CareerPlayoffsMade = awayCoach.CareerPlayoffsMade + 1;&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;" />
