            <attribute name="HandleUpdatePlayerContractStatus" idx="8" type="TransactionHistoryManager_HandleUpdatePlayerContractStatus" guid="30126-132029" final="true" default="PlayerContractStatus newContractStatus = event.NewContractStatus;&#xD;&#xA;PlayerContractStatus oldContractStatus = event.OldContractStatus;&#xD;&#xA;&#xD;&#xA;bool createEntry = (!event.IsUndo) &amp;&amp; Configuration != null;&#xD;&#xA; &#xD;&#xA;if (createEntry)&#xD;&#xA;{&#xD;&#xA; // Special configuration for draft class players&#xD;&#xA; if (oldContractStatus == PlayerContractStatus.Draft)&#xD;&#xA; {&#xD;&#xA;  if (newContractStatus == PlayerContractStatus.Drafted)&#xD;&#xA;  {&#xD;&#xA;   createEntry = Configuration.TrackDraftPlayerDrafted;&#xD;&#xA;  }&#xD;&#xA;  else if (newContractStatus == PlayerContractStatus.FreeAgent)&#xD;&#xA;  {&#xD;&#xA;   createEntry = Configuration.TrackDraftPlayerNotDrafted;&#xD;&#xA;  }&#xD;&#xA; }&#xD;&#xA; else if (oldContractStatus == PlayerContractStatus.Drafted)&#xD;&#xA; {&#xD;&#xA;  if (newContractStatus == PlayerContractStatus.Signed)&#xD;&#xA;  {&#xD;&#xA;   createEntry = Configuration.TrackDraftPlayerSignedToTeam;&#xD;&#xA;  }&#xD;&#xA; }&#xD;&#xA; else&#xD;&#xA; {&#xD;&#xA;  createEntry = (newContractStatus == PlayerContractStatus.Signed) ||&#xD;&#xA;   (newContractStatus == PlayerContractStatus.FreeAgent) ||&#xD;&#xA;   (newContractStatus == PlayerContractStatus.Retired) ||&#xD;&#xA;   (newContractStatus == PlayerContractStatus.PracticeSquad);&#xD;&#xA; }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if (createEntry)&#xD;&#xA;{&#xD;&#xA; PlayerTransactionHistoryEntry entry = new PlayerTransactionHistoryEntry;&#xD;&#xA; if (entry != null)&#xD;&#xA; {&#xD;&#xA;  if (!((oldContractStatus == PlayerContractStatus.Expiring) &amp;&amp;&#xD;&#xA;     (newContractStatus == PlayerContractStatus.FreeAgent)))&#xD;&#xA;  {&#xD;&#xA;   // Initialize the date information.&#xD;&#xA;   entry.SeasonYear = SeasonInfo.CurrentYear;&#xD;&#xA;   entry.SeasonStage = SeasonInfo.CurrentStage;&#xD;&#xA;   &#xD;&#xA;   bool excludeContractYearZero = false;&#xD;&#xA;&#xD;&#xA;   // EA_TODO(&quot;cwood&quot;,&quot;4/24/15&quot;,&quot;Fix season manager end week to handle pushing week past valid number in post release clean up&quot;);&#xD;&#xA;   // If this is the offseason, use the stage (convert to zero-based).&#xD;&#xA;   if (entry.SeasonStage == SeasonStage.OffSeason)&#xD;&#xA;   {&#xD;&#xA;    entry.SeasonWeek = SeasonInfo.CurrentOffseasonStage - 1;&#xD;&#xA;   }&#xD;&#xA;   else&#xD;&#xA;   {&#xD;&#xA;    if (entry.SeasonStage == SeasonStage.PreSeason)&#xD;&#xA;    {&#xD;&#xA;     if (SeasonInfo.CurrentWeek &gt;= SeasonInfo.PreseasonWeekCount)&#xD;&#xA;     {&#xD;&#xA;      entry.SeasonWeek = (SeasonInfo.PreseasonWeekCount - 1);&#xD;&#xA;     }&#xD;&#xA;     else&#xD;&#xA;     {&#xD;&#xA;      entry.SeasonWeek = SeasonInfo.CurrentWeek;&#xD;&#xA;     }&#xD;&#xA;    }&#xD;&#xA;    else if (entry.SeasonStage == SeasonStage.NFLSeason)&#xD;&#xA;    {&#xD;&#xA;     if (SeasonInfo.CurrentWeek &gt;= SeasonInfo.NflseasonWeekCount)&#xD;&#xA;     {&#xD;&#xA;      entry.SeasonWeek = (SeasonInfo.NflseasonWeekCount - 1);&#xD;&#xA;     }&#xD;&#xA;     else&#xD;&#xA;     {&#xD;&#xA;      entry.SeasonWeek = SeasonInfo.CurrentWeek;&#xD;&#xA;     }&#xD;&#xA;    }&#xD;&#xA;    if ((newContractStatus == PlayerContractStatus.Signed) &amp;&amp; (event.OldTeam == event.NewTeam) &amp;&amp; (event.OldContractStatus != PlayerContractStatus.PracticeSquad))&#xD;&#xA;    {&#xD;&#xA;     // This is a re-sign transaction.  Do NOT include the player's contract year zero info.&#xD;&#xA;     // Year 0 contains the last year of the player's expiring contract.&#xD;&#xA;     excludeContractYearZero = true;&#xD;&#xA;    }&#xD;&#xA;   }&#xD;&#xA;&#xD;&#xA;   // Initialize the team and player information.&#xD;&#xA;   entry.OldTeam = event.OldTeam;&#xD;&#xA;   entry.NewTeam = event.NewTeam;&#xD;&#xA;   entry.Player = event.Player;&#xD;&#xA;&#xD;&#xA;   Player player = event.Player;&#xD;&#xA;&#xD;&#xA;   // Initialize the contract information.&#xD;&#xA;   entry.ContractStatus = event.NewContractStatus;&#xD;&#xA;   entry.OldContractStatus = event.OldContractStatus;&#xD;&#xA;   &#xD;&#xA;   entry.ContractLength = event.Player.ContractLength;&#xD;&#xA;   entry.ContractTotalSalary = player.ContractSalary0 + player.ContractBonus0 +&#xD;&#xA;          player.ContractSalary1 + player.ContractBonus1 +&#xD;&#xA;          player.ContractSalary2 + player.ContractBonus2 +&#xD;&#xA;          player.ContractSalary3 + player.ContractBonus3 +&#xD;&#xA;          player.ContractSalary4 + player.ContractBonus4 +&#xD;&#xA;          player.ContractSalary5 + player.ContractBonus5 +&#xD;&#xA;          player.ContractSalary6 + player.ContractBonus6 +&#xD;&#xA;          player.ContractSalary7 + player.ContractBonus7;&#xD;&#xA;   entry.ContractSalary = player.ContractSalary0 + player.ContractSalary1 +&#xD;&#xA;           player.ContractSalary2 + player.ContractSalary3 +&#xD;&#xA;           player.ContractSalary4 + player.ContractSalary5 +&#xD;&#xA;           player.ContractSalary6 + player.ContractSalary7;&#xD;&#xA;   entry.ContractBonus = player.ContractBonus0 + player.ContractBonus1 + player.ContractBonus2 +&#xD;&#xA;          player.ContractBonus3 + player.ContractBonus4 + player.ContractBonus5 +&#xD;&#xA;          player.ContractBonus6 + player.ContractBonus7;&#xD;&#xA;   &#xD;&#xA;   if (excludeContractYearZero)&#xD;&#xA;   {&#xD;&#xA;    // Remove all values from contract year zero since it's part of a different (expiring) contract.&#xD;&#xA;    entry.ContractLength = entry.ContractLength - 1;&#xD;&#xA;    entry.ContractTotalSalary = entry.ContractTotalSalary - (player.ContractSalary0 + player.ContractBonus0);&#xD;&#xA;    entry.ContractSalary = entry.ContractSalary - player.ContractSalary0;&#xD;&#xA;    entry.ContractBonus = entry.ContractBonus - player.ContractBonus0;&#xD;&#xA;   }&#xD;&#xA;&#xD;&#xA;   // Add the entry to the list.&#xD;&#xA;   AppendTransaction