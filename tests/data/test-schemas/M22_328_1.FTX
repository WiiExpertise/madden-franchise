            <attribute name="StartDraftStage_ProcessV2" idx="51" type="none()" guid="5295443-1824105" final="true" default="// Clear the trade status.&#xD;&#xA;IsDraftPickTraded = false;&#xD;&#xA;TradePackageA = null;&#xD;&#xA;TradePackageB = null;&#xD;&#xA;&#xD;&#xA;// If the draft is live, set the time scale to real-time.&#xD;&#xA;if (SimState == DraftSimState.Live)&#xD;&#xA;{&#xD;&#xA;    SeasonInfoUpdateEvent.SeasonInfo.IsDraftActive = true;&#xD;&#xA;    SeasonInfoUpdateEvent.Send();&#xD;&#xA;    TimeScale = 1.0;&#xD;&#xA;}&#xD;&#xA;// Otherwise, set the time scale to simulate.&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;    TimeScale = 0.0;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Enable trade notifications if the draft is live.&#xD;&#xA;TradeNegotiationManager.IsNotificationEnabled = (SimState == DraftSimState.Live);&#xD;&#xA;&#xD;&#xA;// Initialize the advance request.&#xD;&#xA;{&#xD;&#xA;    AdvanceRequest.ResponseForms.EraseAll();&#xD;&#xA;&#xD;&#xA;    // If this is the normal draft, include all advance options.&#xD;&#xA;    if (!DraftInfo.IsFantasyDraft)&#xD;&#xA;    {&#xD;&#xA;        AdvanceRequest.ResponseForms.Append(AdvanceRequest.NextPickResponse);&#xD;&#xA;        AdvanceRequest.ResponseForms.Append(AdvanceRequest.NextUserPickResponse);&#xD;&#xA;        AdvanceRequest.ResponseForms.Append(AdvanceRequest.NextRoundResponse);&#xD;&#xA;        AdvanceRequest.ResponseForms.Append(AdvanceRequest.EndDraftResponse);&#xD;&#xA;    }&#xD;&#xA;    // Otherwise, include only the next user pick option.&#xD;&#xA;    else&#xD;&#xA;    {&#xD;&#xA;        AdvanceRequest.ResponseForms.Append(AdvanceRequest.NextUserPickResponse);&#xD;&#xA;        AdvanceRequest.ResponseForms.Append(AdvanceRequest.EndDraftResponse);&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Begin the draft.&#xD;&#xA;DraftManager.BeginDraft(DraftInfo.IsFantasyDraft);&#xD;&#xA;&#xD;&#xA;// If first pick is not a user pick and Manual Advancement is OFF, set to auto advance to the next user pick.&#xD;&#xA;if ((DraftInfo.CurrentTeam != null) &amp;&amp; (DraftInfo.CurrentTeam.UserCharacter == null))&#xD;&#xA;{&#xD;&#xA;    // If Advancement setting is set to AUTO and Draft is not going to be full simulated, sim to next user pick.&#xD;&#xA;    if (!DraftEval.IsManualAdvancementEnabled() &amp;&amp; (SimState == DraftSimState.Live))&#xD;&#xA;    {&#xD;&#xA;        SetSimState(DraftSimState.SimToNextUserPick);&#xD;&#xA;    }&#xD;&#xA;}" />
