            <attribute name="PlayerSelectedTransitionV2" idx="24" type="bool()" guid="5295443-1824156" final="true" default="bool transition = false;&#xD;&#xA;&#xD;&#xA;// If a player was selected, transition.&#xD;&#xA;if (SelectedPlayer != null)&#xD;&#xA;{&#xD;&#xA;    transition = true;&#xD;&#xA;}&#xD;&#xA;// If the select player response was chosen, transition.&#xD;&#xA;else if (Request.Response == Request.SelectPlayerResponse)&#xD;&#xA;{&#xD;&#xA;    SelectedPlayer = Request.SelectedPlayer;&#xD;&#xA;&#xD;&#xA;    // If Manual Advancement is OFF, auto advance to the next user pick.&#xD;&#xA;    if (!DraftEval.IsManualAdvancementEnabled())&#xD;&#xA;    {&#xD;&#xA;        SetSimState(DraftSimState.SimToNextUserPick);&#xD;&#xA;    }&#xD;&#xA;    transition = true;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Return the transition value.&#xD;&#xA;transition;&#xD;&#xA;" />
