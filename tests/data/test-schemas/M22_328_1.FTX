            <attribute name="HandleTradeDraftPick" idx="6" type="TransactionHistoryManager_HandleTradeDraftPick" guid="30126-114935" final="true" default="DraftPickTransactionHistoryEntry entry = new DraftPickTransactionHistoryEntry ;&#xD;&#xA;if (entry != null)&#xD;&#xA;{&#xD;&#xA;    DraftPick pick = event.Pick;&#xD;&#xA;    DraftPick nullPick = null;&#xD;&#xA;    if (pick != nullPick)&#xD;&#xA;    {&#xD;&#xA;        // Initialize the date information.&#xD;&#xA;        entry.SeasonYear = SeasonInfo.CurrentYear;&#xD;&#xA;        entry.SeasonStage = SeasonInfo.CurrentStage;&#xD;&#xA;&#xD;&#xA;        // If this is the offseason, use the stage (convert to zero-based).&#xD;&#xA;        if (entry.SeasonStage == SeasonStage.OffSeason)&#xD;&#xA;        {&#xD;&#xA;            entry.SeasonWeek = SeasonInfo.CurrentOffseasonStage - 1;&#xD;&#xA;        }&#xD;&#xA;        // Otherwise, use the week number.&#xD;&#xA;        else&#xD;&#xA;        {&#xD;&#xA;            entry.SeasonWeek = SeasonInfo.CurrentWeek;&#xD;&#xA;        }&#xD;&#xA;&#xD;&#xA;        // Initialize the team and player information.&#xD;&#xA;        entry.OldTeam = event.OldTeam;&#xD;&#xA;        entry.NewTeam = event.NewTeam;&#xD;&#xA;&#xD;&#xA;        // Set Draft Pick information&#xD;&#xA;        entry.Round = pick.Round;&#xD;&#xA;        entry.Pick = pick.PickNumber;&#xD;&#xA;        entry.Year = pick.YearOffset + SeasonInfo.CurrentSeasonYear;&#xD;&#xA;&#xD;&#xA;        // Add the entry to the list.&#xD;&#xA;        AppendTransaction(entry);&#xD;&#xA;    }&#xD;&#xA;}" />
            <attribute name="HandleUpdateCoachContractStatus" idx="7" type="TransactionHistoryManager_HandleUpdateCoachContractStatus" guid="30126-132035" final="true" default="StaffPersonContractStatus newContractStatus = event.NewContractStatus;&#xD;&#xA;&#xD;&#xA;if ((newContractStatus == StaffPersonContractStatus.Signed) ||&#xD;&#xA;    (newContractStatus == StaffPersonContractStatus.FreeAgent) ||&#xD;&#xA;    (newContractStatus == StaffPersonContractStatus.Retired))&#xD;&#xA;{&#xD;&#xA;    CoachTransactionHistoryEntry entry = new CoachTransactionHistoryEntry;&#xD;&#xA;    if (entry != null)&#xD;&#xA;    {&#xD;&#xA;        // Initialize the date information.&#xD;&#xA;        entry.SeasonYear = SeasonInfo.CurrentYear;&#xD;&#xA;        entry.SeasonStage = SeasonInfo.CurrentStage;&#xD;&#xA;&#xD;&#xA;            // EA_TODO(&quot;cwood&quot;,&quot;4/24/15&quot;,&quot;Fix season manager end week to handle pushing week past valid number in post release clean up&quot;);&#xD;&#xA;            // If this is the offseason, use the stage (convert to zero-based).&#xD;&#xA;            if (entry.SeasonStage == SeasonStage.OffSeason)&#xD;&#xA;            {&#xD;&#xA;                entry.SeasonWeek = SeasonInfo.CurrentOffseasonStage - 1;&#xD;&#xA;            }&#xD;&#xA;            else if (entry.SeasonStage == SeasonStage.PreSeason)&#xD;&#xA;            {&#xD;&#xA;                if (SeasonInfo.CurrentWeek &gt;= SeasonInfo.PreseasonWeekCount)&#xD;&#xA;                {&#xD;&#xA;                    entry.SeasonWeek = (SeasonInfo.PreseasonWeekCount - 1);&#xD;&#xA;                }&#xD;&#xA;                else&#xD;&#xA;                {&#xD;&#xA;                    entry.SeasonWeek = SeasonInfo.CurrentWeek;&#xD;&#xA;                }&#xD;&#xA;            }&#xD;&#xA;            else if (entry.SeasonStage == SeasonStage.NFLSeason)&#xD;&#xA;            {&#xD;&#xA;                if (SeasonInfo.CurrentWeek &gt;= SeasonInfo.NflseasonWeekCount)&#xD;&#xA;                {&#xD;&#xA;       