            <attribute name="CalculateCombineBenchPress" idx="0" type="DraftEval_CalculateCombineBenchPress" guid="5157-1321037" final="true" default="int strength = draftPlayer.Player.StrengthRating;&#xD;&#xA;&#xD;&#xA;//There's no way currently to cast between int's and floats&#xD;&#xA;//So we have to multiply the initial value of each part by 10,000, then divide by that at the end&#xD;&#xA;//0.0079 * 10,000; 0.4028 * 10,000; 8.7735 * 10,000&#xD;&#xA;//int reps = (0.0079 * (strength * strength)) - (0.4028 * strength) + 8.7735;&#xD;&#xA;int reps = ((79 * (strength * strength)) - (4028 * strength) + 87735) / 10000;&#xD;&#xA;&#xD;&#xA;int finalReps = reps + RandomRef.GetRandomIntRange(-2,2);&#xD;&#xA;&#xD;&#xA;//Clamp the value&#xD;&#xA;reps = ClampValue(finalReps, 4, 46);&#xD;&#xA;&#xD;&#xA;reps;&#xD;&#xA;" />
            <attribute name="CalculateCombineBroadJump" idx="1" type="DraftEval_CalculateCombineBroadJump" guid="5157-1321039" final="true" default="int jump = draftPlayer.Player.JumpingRating;&#xD;&#xA;int strength = draftPlayer.Player.StrengthRating;&#xD;&#xA;&#xD;&#xA;//There's no way currently to cast between int's and floats&#xD;&#xA;//So we have to multiply the initial value of each part by 1,000, then divide by that at the end&#xD;&#xA;//DistanceInInches = (.554)(.7*Jumping + .5*Strength) + 68.156&#xD;&#xA;int inches = (((554 * ((700 * jump) + (500 * strength)))/1000) + 68156) / 1000;&#xD;&#xA;&#xD;&#xA;int inchesFinal = inches + RandomRef.GetRandomIntRange(-5,5);&#xD;&#xA;&#xD;&#xA;//Clamp the value&#xD;&#xA;inches = ClampValue(inchesFinal, 87, 134);&#xD;&#xA;&#xD;&#xA;//returns inches&#xD;&#xA;inches;&#xD;&#xA;" />
            <attribute name="CalculateCombineFortyYardDash" idx="2" type="DraftEval_CalculateCombineFortyYardDash" guid="5157-1321036" final="true" default="int speed = draftPlayer.Player.SpeedRating;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;//There's no way currently to cast between int's and floats&#xD;&#xA;//this function would always equal 6 because 27/980 = 0, effectively ignoring speed &#xD;&#xA;//So we have to multiply the initial value of each part by 10,000, then divide by that at the end&#xD;&#xA;//int time = (34141 / 4900) - (27/980 * speed);&#xD;&#xA;int time = ((((34141 *10000) / 4900) - ((27 * 10000)/980) * (speed + ((speed * RandomRef.GetRandomIntRange(-3,3))/100) )  ) / 100) + RandomRef.GetRandomIntRange(-1,1);&#xD;&#xA;&#xD;&#xA;//Clamp the value&#xD;&#xA;time = ClampValue(time, 424, 559);&#xD;&#xA;&#xD;&#xA;//returns time in seconds x 100 (10000 / 100 = 100)&#xD;&#xA;//otherwise we would be truncating the fractional part&#xD;&#xA;//ex: 4.24 seconds = 424&#xD;&#xA;&#xD;&#xA;time;&#xD;&#xA;" />
            <attribute name="CalculateCombineOverallGrade" idx="3" type="DraftEval_CalculateCombineOverallGrade" guid="5157-1321066" final="true" default="int speed = draftPlayer.Player.SpeedRating;&#xD;&#xA;int acceleration = draftPlayer.Player.AccelerationRating;&#xD;&#xA;int agility = draftPlayer.Player.AgilityRating;&#xD;&#xA;int strength = draftPlayer.Player.StrengthRating;&#xD;&#xA;int jump = draftPlayer.Player.JumpingRating;&#xD;&#xA;&#xD;&#xA;//COMBINE_TOTAL = SPEED + ACCELERATION + AGILITY + 2*STRENGTH + JUMPING&#xD;&#xA;int combineTotal = speed + acceleration + agility + (2 * strength) + jump;&#xD;&#xA;&#xD;&#xA;//There's no way currently to cast between int's and floats&#xD;&#xA;//So we have to multiply the initial value of each part by 10,000, then divide by that at the end&#xD;&#xA;//COMBINE GRADE = 0.0304*COMBINE_TOTAL - 8.0547&#xD;&#xA;int grade = ((304 * combineTotal) - 80547) / 100;&#xD;&#xA;&#xD;&#xA;//Clamp the value&#xD;&#xA;grade = ClampValue(grade, 100, 1000);&#xD;&#xA;&#xD;&#xA;//returns grade x 100 (10000 / 100 = 100)&#xD;&#xA;//otherwise we would be truncating the fractional part&#xD;&#xA;//ex: 7.30 = 730&#xD;&#xA;grade;&#xD;&#xA;" />
            <attribute name="CalculateCombineReport" idx="4" type="DraftEval_CalculateCombineReport" guid="5157-1321031" final="true" default="draftPlayer.CombineFortyYardDash = CalculateCombineFortyYardDash(draftPlayer);&#xD;&#xA;&#xD;&#xA;draftPlayer.CombineBenchPress = CalculateCombineBenchPress(draftPlayer);&#xD;&#xA;&#xD;&#xA;draftPlayer.CombineVerticalJump = CalculateCombineVerticalJump(draftPlayer);&#xD;&#xA;&#xD;&#xA;draftPlayer.CombineBroadJump = CalculateCombineBroadJump(draftPlayer);&#xD;&#xA;&#xD;&#xA;draftPlayer.CombineThreeConeDrill = CalculateCombineThreeConeDrill(draftPlayer);&#xD;&#xA;&#xD;&#xA;draftPlayer.CombineTwentyYardShuttle = CalculateCombineTwentyYardShuttle(draftPlayer);&#xD;&#xA;&#xD;&#xA;draftPlayer.CombineOverallGrade = CalculateCombineOverallGrade(draftPlayer);&#xD;&#xA;" />
            <attribute name="CalculateCombineThreeConeDrill" idx="5" type="DraftEval_CalculateCombineThreeConeDrill" guid="5157-1321040" final="true" default="int agility = draftPlayer.Player.AgilityRating;&#xD;&#xA;int acceleration = draftPlayer.Player.AccelerationRating;&#xD;&#xA;&#xD;&#xA;//There's no way currently to cast between int's and floats&#xD;&#xA;//So we have to multiply the initial value of each part by 10,000, then divide by that at the end&#xD;&#xA;//TimeInSec= (-0.0464)(.4*Agility + .6*Acceleration) + 11.066 &#xD;&#xA;int seconds = (((-464* ((4000 * agility) + (6000 * acceleration)))/10000) + 110660) / 100;&#xD;&#xA;&#xD;&#xA;//add some randomness&#xD;&#xA;int secondsFinal = seconds + RandomRef.GetRandomIntRange(-1,1);&#xD;&#xA;&#xD;&#xA;//Clamp the value&#xD;&#xA;seconds = ClampValue(secondsFinal, 647, 829);&#xD;&#xA;&#xD;&#xA;//returns time in seconds x 100 (10000 / 100 = 100)&#xD;&#xA;//otherwise we would be truncating the fractional part&#xD;&#xA;//ex: 6.42 seconds = 642&#xD;&#xA;seconds;&#xD;&#xA;" />
            <attribute name="CalculateCombineTwentyYardShuttle" idx="6" type="DraftEval_CalculateCombineTwentyYardShuttle" guid="5157-1321041" final="true" default="int agility = draftPlayer.Player.AgilityRating;&#xD;&#xA;int cod = draftPlayer.Player.ChangeOfDirectionRating;&#xD;&#xA;&#xD;&#xA;//There's no way currently to cast between int's and floats&#xD;&#xA;//So we have to multiply the initial value of each part by 10,000, then divide by that at the end&#xD;&#xA;//TimeInSec= (-0.0349)(.6*Agility + .4*ChangeOfDirection) + 7.2679  &#xD;&#xA;int seconds = (((-349* ((6000 * agility) + (4000 * cod)))/10000) + 72679) / 100;&#xD;&#xA;&#xD;&#xA;//randomness&#xD;&#xA;int secondsFinal = seconds + RandomRef.GetRandomIntRange(-1,1);&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;//Clamp the value&#xD;&#xA;seconds = ClampValue(secondsFinal, 381, 527);&#xD;&#xA;&#xD;&#xA;//returns time in seconds x 100 (10000 / 100 = 100)&#xD;&#xA;//otherwise we would be truncating the fractional part&#xD;&#xA;//ex: 3.81 seconds = 381&#xD;&#xA;seconds;&#xD;&#xA;" />
            <attribute name="CalculateCombineVerticalJump" idx="7" type="DraftEval_CalculateCombineVerticalJump" guid="5157-1321038" final="true" default="int jump = draftPlayer.Player.JumpingRating;&#xD;&#xA;&#xD;&#xA;//There's no way currently to cast between int's and floats&#xD;&#xA;//So we have to multiply the initial value of each part by 10,000, then divide by that at the end&#xD;&#xA;//DistanceInInches = (.0045* JUMPING^2) - (0.2436*JUMPING) + 23.655&#xD;&#xA;int inches = ((45 * (jump * jump)) - (2436 * jump) + 236550) / 1000;&#xD;&#xA;&#xD;&#xA;int inchesFinal = inches + RandomRef.GetRandomIntRange(-5,5);&#xD;&#xA;&#xD;&#xA;//Clamp the value&#xD;&#xA;inches = ClampValue(inchesFinal, 205, 436);&#xD;&#xA;&#xD;&#xA;//returns inches x 10 (10000 / 1000 = 10)&#xD;&#xA;//otherwise we would be truncating the fractional part&#xD;&#xA;//ex: 20.5 inches = 205&#xD;&#xA;inches;&#xD;&#xA;" />
            <attribute name="CalculateProDayReport" idx="8" type="DraftEval_CalculateProDayReport" guid="5157-1839013" final="true" default="//Results stats use combine calculations&#xD;&#xA;draftPlayer.ProDayBenchPress = CalculateCombineBenchPress(draftPlayer);&#xD;&#xA;draftPlayer.ProDayVerticalJump = CalculateCombineVerticalJump(draftPlayer);&#xD;&#xA;draftPlayer.ProDayBroadJump = CalculateCombineBroadJump(draftPlayer);&#xD;&#xA;&#xD;&#xA;//Time Stats will be capped around the results from the combine&#xD;&#xA;draftPlayer.ProDayFortyYardDash = (draftPlayer.CombineFortyYardDash) != 0 ? (CalculateProDayVariance(draftPlayer.CombineFortyYardDash, 424, 559)) :  (CalculateCombineFortyYardDash(draftPlayer));&#xD;&#xA;draftPlayer.ProDayThreeConeDrill = (draftPlayer.CombineThreeConeDrill != 0) ? (CalculateProDayVariance(draftPlayer.CombineThreeConeDrill, 647, 829)) : (CalculateCombineThreeConeDrill(draftPlayer));&#xD;&#xA;draftPlayer.ProDayTwentyYardShuttle = (draftPlayer.CombineTwentyYardShuttle != 0) ? (CalculateProDayVariance(draftPlayer.CombineTwentyYardShuttle, 381, 527)) : (CalculateCombineTwentyYardShuttle(draftPlayer));" />
            <attribute name="CalculateProDayVariance" idx="9" type="DraftEval_CalculateProDayVariance" guid="5157-1839024" final="true" default="//There's no way currently to cast between int's and floats&#xD;&#xA;//So we have to multiply the initial value of each part by 10,000, then divide by that at the end&#xD;&#xA;int floatOriginalValue = originalValue * 10000;&#xD;&#xA;&#xD;&#xA;// 1.5% Difference max&#xD;&#xA;int minValue = floatOriginalValue - (originalValue * 150);&#xD;&#xA;// 0.3% worse minimum&#xD;&#xA;int maxValue = floatOriginalValue + (originalValue * 30);&#xD;&#xA;&#xD;&#xA;int time = ClampValue((RandomRef.GetRandomIntRange(minValue ,maxValue) / 10000), clampMin, clampMax);&#xD;&#xA;time;" />
            <attribute name="ClampValue" idx="10" type="DraftEval_ClampValue" guid="5157-1321089" final="true" default="int newValue = valueIn;&#xD;&#xA;&#xD;&#xA;if (newValue &lt; valueMin)&#xD;&#xA;{&#xD;&#xA; newValue = valueMin;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if (newValue &gt; valueMax)&#xD;&#xA;{&#xD;&#xA; newValue = valueMax;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;newValue;&#xD;&#xA;" />
