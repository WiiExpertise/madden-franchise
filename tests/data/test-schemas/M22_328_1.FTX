OffYardsPerGame0 * 100) / 17;&#xD;&#xA;  teamTotalOffYardsPerGame1 = (teamTotalOffYardsPerGame1 * 100) / 16;&#xD;&#xA;  teamTotalOffYardsPerGame2 = (teamTotalOffYardsPerGame2 * 100) / 16;&#xD;&#xA;  teamTotalDefYardsPerGame0 = (teamTotalDefYardsPerGame0 * 100) / 17;&#xD;&#xA;  teamTotalDefYardsPerGame1 = (teamTotalDefYardsPerGame1 * 100) / 16;&#xD;&#xA;  teamTotalDefYardsPerGame2 = (teamTotalDefYardsPerGame2 * 100) / 16;&#xD;&#xA;}&#xD;&#xA;else if (currentYear == 2022)&#xD;&#xA;{&#xD;&#xA;  teamTotalOffYardsPerGame0 = (teamTotalOffYardsPerGame0 * 100) / 17;&#xD;&#xA;  teamTotalOffYardsPerGame1 = (teamTotalOffYardsPerGame1 * 100) / 17;&#xD;&#xA;  teamTotalOffYardsPerGame2 = (teamTotalOffYardsPerGame2 * 100) / 16;&#xD;&#xA;  teamTotalDefYardsPerGame0 = (teamTotalDefYardsPerGame0 * 100) / 17;&#xD;&#xA;  teamTotalDefYardsPerGame1 = (teamTotalDefYardsPerGame1 * 100) / 17;&#xD;&#xA;  teamTotalDefYardsPerGame2 = (teamTotalDefYardsPerGame2 * 100) / 16;&#xD;&#xA;}&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;  teamTotalOffYardsPerGame0 = (teamTotalOffYardsPerGame0 * 100) / 17;&#xD;&#xA;  teamTotalOffYardsPerGame1 = (teamTotalOffYardsPerGame1 * 100) / 17;&#xD;&#xA;  teamTotalOffYardsPerGame2 = (teamTotalOffYardsPerGame2 * 100) / 17;&#xD;&#xA;  teamTotalDefYardsPerGame0 = (teamTotalDefYardsPerGame0 * 100) / 17;&#xD;&#xA;  teamTotalDefYardsPerGame1 = (teamTotalDefYardsPerGame1 * 100) / 17;&#xD;&#xA;  teamTotalDefYardsPerGame2 = (teamTotalDefYardsPerGame2 * 100) / 17;  &#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if(teamWins0 == 0)&#xD;&#xA;{&#xD;&#xA;  shouldFire = true;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if (contentionPhase == TeamContentionPhase.Losing)&#xD;&#xA;{&#xD;&#xA;  if(teamWins0 &lt; teamWins1 &amp;&amp; teamWins0 &lt; teamWins2 &amp;&amp; teamWins0 &lt; 5 &amp;&amp; seasonsWithTeam &gt;= 3)&#xD;&#xA;  {&#xD;&#xA;    shouldFire = true;&#xD;&#xA;  }&#xD;&#xA;  else if(teamWins0 &lt; 7 &amp;&amp; teamWins1 &lt; 7 &amp;&amp; teamWins2 &lt; 7 &amp;&amp; seasonsWithTeam &gt;= 3)&#xD;&#xA;  {&#xD;&#xA;    shouldFire = true;&#xD;&#xA;  }&#xD;&#xA;  else if(teamWins0  &lt; teamWins1 &amp;&amp; teamWins0 &lt; 5 &amp;&amp; seasonsWithTeam &gt;= 2)&#xD;&#xA;  {&#xD;&#xA;    shouldFire = true;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;  if (position == CoachPosition.HeadCoach)&#xD;&#xA;  {&#xD;&#xA;    //TODO: Add any special logic for Head Coach Firing&#xD;&#xA;  }&#xD;&#xA;  else if(position == CoachPosition.OffensiveCoordinator)&#xD;&#xA;  {&#xD;&#xA;    if(teamStats0.PASSINTS &gt; teamStats0.PASSTDS)&#xD;&#xA;    {&#xD;&#xA;      shouldFire = true;&#xD;&#xA;    }&#xD;&#xA;  else if(teamTotalOffYardsPerGame0 &lt; teamTotalOffYardsPerGame1 &amp;&amp; teamTotalOffYardsPerGame1 &lt; teamTotalOffYardsPerGame2 &amp;&amp; seasonsWithTeam &gt;= 3)&#xD;&#xA;  {&#xD;&#xA;    shouldFire = true;&#xD;&#xA;  }&#xD;&#xA;    else if(teamStats0.PASSTDS + teamStats0.RUSHTDS &lt; teamStats1.PASSTDS + teamStats1.RUSHTDS &amp;&amp; teamStats1.PASSTDS + teamStats1.RUSHTDS &lt; teamStats2.PASSTDS + teamStats2.RUSHTDS &amp;&amp; seasonsWithTeam &gt;= 3)&#xD;&#xA;    {&#xD;&#xA;      shouldFire = true;&#xD;&#xA;    }&#xD;&#xA;  }&#xD;&#xA;  else if(position == CoachPosition.DefensiveCoordinator)&#xD;&#xA;  {&#xD;&#xA;    if(teamTotalDefYardsPerGame0 &gt; teamTotalDefYardsPerGame1 &amp;&amp; teamTotalDefYardsPerGame1 &gt; teamTotalDefYardsPerGame2 &amp;&amp; seasonsWithTeam &gt;= 3)&#xD;&#xA;    {&#xD;&#xA;      shouldFire = true;&#xD;&#xA;    }&#xD;&#xA;  }&#xD;&#xA;}&#xD;&#xA;else if (contentionPhase == TeamContentionPhase.Playoff)&#xD;&#xA;{&#xD;&#xA;  if(teamWins0 &lt; teamWins1 &amp;&amp; teamWins0 &lt; teamWins2 &amp;&amp; teamWins0 &lt; 6 &amp;&amp; seasonsWithTeam &gt;= 2)&#xD;&#xA;  {&#xD;&#xA;    shouldFire = true;&#xD;&#xA;  }&#xD;&#xA;  else if(teamWins0 &lt; 9 &amp;&amp; teamWins1 &lt; 9 &amp;&amp; teamWins2 &lt; 9 &amp;&amp; seasonsWithTeam &gt;= 2)&#xD;&#xA;  {&#xD;&#xA;    shouldFire = true;&#xD;&#xA;  }&#xD;&#xA;  else if(teamWins0  &lt; teamWins1 &amp;&amp; teamStats0.WIN